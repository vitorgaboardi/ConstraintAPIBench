                {
                    "name": "country",
                    "type": "STRING",
                    "description": "The marketplace country",
                    "default": "de",
                    "required": true,
                    "constraints": {
                        "format": "country code"
                    }
                },
                {
                    "name": "source",
                    "type": "STRING",
                    "description": "Marketplace name (amazon, google, idealo, ebay)",
                    "default": "amazon",
                    "required": true,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "amazon",
                                "google",
                                "idealo",
                                "ebay"
