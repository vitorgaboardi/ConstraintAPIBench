{
    "name": "Microsoft Authenticator",
    "description": "Generate and validate time-based one-time passwords (TOTP) for your second factor authentication (2FA) scenario using Microsoft Authenticator app",
    "url": "https://rapidapi.com/chdan/api/microsoft-authenticator/",
    "api_methods": [
        {
            "name": "enroll",
            "description": "Step 2. Generate QR code and enroll using Google Authenticator app",
            "url": "https://microsoft-authenticator.p.rapidapi.com/enroll/",
            "parameters": [
                {
                    "name": "secret",
                    "type": "STRING",
                    "description": "Secret code that /new/ or /new_v2/ return",
                    "default": "IH225HMVWDS3XJVY",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "account",
                    "type": "STRING",
                    "description": "Account within the company",
                    "default": "User1",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "issuer",
                    "type": "STRING",
                    "description": "A company issuing temporary codes",
                    "default": "HomeCorp",
                    "required": false,
                    "constraints": {}
                }
            ],
            "utterances": [
                {
                    "utterance": "I need to set up my account with the Microsoft Authenticator app.",
                    "parameters": {
                        "account": "User1"
                    }
                },
                {
