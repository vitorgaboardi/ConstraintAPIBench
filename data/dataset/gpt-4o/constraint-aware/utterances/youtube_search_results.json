{
    "name": "YouTube Search Results",
    "description": "Fetch the YouTube Search Results and the information for each item for a specific search term, without any limits!",
    "url": "https://rapidapi.com/marindelija/api/youtube-search-results/",
    "api_methods": [
        {
            "name": "YouTubeSearchResults",
            "description": "Get all the YouTube Search Results Items and their Information! No limits and no Google API Key needed!",
            "url": "https://youtube-search-results.p.rapidapi.com/youtube-search/",
            "parameters": [
                {
                    "name": "q",
                    "type": "STRING",
                    "description": "The URL Encoded search Query",
                    "default": "justin+bieber",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "next",
                    "type": "STRING",
                    "description": "Leave empty if you want the results from the first page.\nUse the value from [nextpageRef] from the previous request (for example first page) to get the next page (for example 2nd page)\nMUST BE URL ENCODED",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me all the YouTube videos about space exploration.",
                    "parameters": {
                        "q": "space+exploration"
                    }
                },
                {
                    "utterance": "Find YouTube content related to vegan recipes.",
                    "parameters": {
                        "q": "vegan+recipes"
                    }
                },
                {
                    "utterance": "Can you get me YouTube search results for the latest tech gadgets?",
                    "parameters": {
                        "q": "latest+tech+gadgets"
                    }
                },
                {
                    "utterance": "I want to see YouTube videos on mindfulness meditation.",
                    "parameters": {
                        "q": "mindfulness+meditation"
                    }
                },
                {
                    "utterance": "Fetch YouTube search results for travel vlogs in Japan.",
                    "parameters": {
                        "q": "travel+vlogs+Japan"
                    }
                },
                {
                    "utterance": "Look up YouTube videos about climate change solutions.",
                    "parameters": {
                        "q": "climate+change+solutions"
                    }
                },
                {
                    "utterance": "Get YouTube search results for the best workout routines.",
                    "parameters": {
                        "q": "best+workout+routines"
                    }
                },
                {
                    "utterance": "Find YouTube videos on how to play the guitar.",
                    "parameters": {
                        "q": "how+to+play+guitar"
                    }
                },
                {
                    "utterance": "Show me YouTube content about the history of art.",
                    "parameters": {
                        "q": "history+of+art"
                    }
                },
                {
                    "utterance": "Can you find YouTube videos on digital marketing strategies?",
                    "parameters": {
                        "q": "digital+marketing+strategies"
                    }
                }
            ]
        }
    ]
}