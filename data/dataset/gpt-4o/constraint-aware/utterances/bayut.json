                    "constraints": {
                        "values": {
                            "enumerated": [
                                "commerical-properties",
                                "mixed-use-land",
                                "factories",
                                "commercial-floors",
                                "bulk-units",
                                "labour-camps",
                                "shops",
                                "showrooms",
                                "industrial-land",
                                "commercial-buildings",
                                "commercial-plots",
                                "commercial-villas",
                                "warehouses",
                                "offices",
