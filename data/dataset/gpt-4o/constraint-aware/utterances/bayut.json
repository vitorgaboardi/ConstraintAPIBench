                    "default": "price-asc",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "price-desc",
                                "price-asc",
                                "city-level-score",
                                "date-desc",
                                "date-asc",
                                "verified-score"
                            ]
