                        "values": {
                            "enumerated": [
                                "monthly",
                                "yearly",
                                "weekly",
                                "daily"
                            ]
                        },
                        "inter-dependency": "rentFrequency, purpose: rentFrequency only takes effect if purpose is for-rent."
                    }
                },
                {
                    "name": "purpose",
                    "type": "STRING",
                    "description": "One of the following : for-rent|for-sale",
                    "default": "",
