{
    "name": "Flowcode",
    "description": "Check out our new Developer Portal and docs: https://www.flowcode.com/developer-portal",
    "url": "https://rapidapi.com/flowcode/api/flowcode1/",
    "api_methods": [
        {
            "name": "Create QR Code",
            "description": "This API takes an endpoint and return and image for a QR Code in either svg, png, jpg, or pdf format",
            "url": "https://flowcode1.p.rapidapi.com/flowcode",
            "parameters": [
                {
                    "name": "url",
                    "type": "STRING",
                    "description": "This is the URL the QR code will scan to",
                    "default": "https://www.flowcode.com",
                    "required": true
                },
                {
                    "name": "imageType",
                    "type": "STRING",
                    "description": "Response image type:\npng, svg, pdf, or jpg",
                    "default": "svg",
                    "required": true,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "png",
                                "svg",
                                "pdf",
                                "jpg"
                            ]
                        }
                    }
                },
                {
                    "name": "bgColor",
                    "type": "STRING",
                    "description": "Hex background color for flowcode.\nFormat: #rrggbb or transparent for transparent background.",
                    "default": "#cbcbcb",
                    "required": false,
                    "constraints": {
                        "format": "Hex color code or 'transparent'"
                    }
                },
                {
                    "name": "fgColor",
                    "type": "STRING",
                    "description": "Hex foreground color for flowcode\nFormat: #rrggbb",
                    "default": "#ff0000",
                    "required": false,
                    "constraints": {
                        "format": "Hex color code"
                    }
                },
                {
                    "name": "rimTextTop",
                    "type": "STRING",
                    "description": "For style=rim, the call to action text to put at top of the rim",
                    "default": "Scan Me",
                    "required": false
                },
                {
                    "name": "style",
                    "type": "STRING",
                    "description": "Style of flowcode:\ndefault, inset, or rim",
                    "default": "default",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "default",
                                "inset",
                                "rim"
                            ]
                        }
                    }
                },
                {
                    "name": "moduleShape",
                    "type": "STRING",
                    "description": "Shape to use for code pattern:\ndefault, circle, square, or diamond",
                    "default": "default",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "default",
                                "circle",
                                "square",
                                "diamond"
                            ]
                        }
                    }
                },
                {
                    "name": "rimTextBottom",
                    "type": "STRING",
                    "description": "For style=rim, the call to action text to put at bottom of the rim",
                    "default": "Scan Me",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Generate a QR code for https://www.example.com in PNG format.",
                    "parameters": {
                        "url": "https://www.example.com",
                        "imageType": "png"
                    }
                },
                {
                    "utterance": "I need a QR code for https://www.shopnow.com with a transparent background in SVG format.",
                    "parameters": {
                        "url": "https://www.shopnow.com",
                        "imageType": "svg",
                        "bgColor": "transparent"
                    }
                },
                {
                    "utterance": "Create a QR code for https://www.learnmore.com in PDF format with a blue foreground.",
                    "parameters": {
                        "url": "https://www.learnmore.com",
                        "imageType": "pdf",
                        "fgColor": "#0000ff"
                    }
                },
                {
                    "utterance": "Make a QR code for https://www.travelnow.com in JPG format with a rim style and 'Book Now' at the top.",
                    "parameters": {
                        "url": "https://www.travelnow.com",
                        "imageType": "jpg",
                        "style": "rim",
                        "rimTextTop": "Book Now"
                    }
                },
                {
                    "utterance": "Can you create a QR code for https://www.foodie.com in SVG format with a circle pattern?",
                    "parameters": {
                        "url": "https://www.foodie.com",
                        "imageType": "svg",
                        "moduleShape": "circle"
                    }
                },
                {
                    "utterance": "I'd like a QR code for https://www.musicstream.com in PNG format with a red background and square pattern.",
                    "parameters": {
                        "url": "https://www.musicstream.com",
                        "imageType": "png",
                        "bgColor": "#ff0000",
                        "moduleShape": "square"
                    }
                },
                {
                    "utterance": "Create a QR code for https://www.fitnesshub.com in PDF format with a diamond pattern and 'Join Us' at the bottom.",
                    "parameters": {
                        "url": "https://www.fitnesshub.com",
                        "imageType": "pdf",
                        "moduleShape": "diamond",
                        "rimTextBottom": "Join Us"
                    }
                },
                {
                    "utterance": "Generate a QR code for https://www.bookstore.com in JPG format with an inset style.",
                    "parameters": {
                        "url": "https://www.bookstore.com",
                        "imageType": "jpg",
                        "style": "inset"
                    }
                },
                {
                    "utterance": "I need a QR code for https://www.technews.com in SVG format with a green foreground and default style.",
                    "parameters": {
                        "url": "https://www.technews.com",
                        "imageType": "svg",
                        "fgColor": "#00ff00",
                        "style": "default"
                    }
                },
                {
                    "utterance": "Make a QR code for https://www.artgallery.com in PNG format with a yellow background and 'Explore' at the top.",
                    "parameters": {
                        "url": "https://www.artgallery.com",
                        "imageType": "png",
                        "bgColor": "#ffff00",
                        "rimTextTop": "Explore"
                    }
                }
            ]
        }
    ]
}