{
    "name": "Google Web Search",
    "description": "Google Web Search API. Search the world\u2019s information, including webpages, related keywords and more.",
    "url": "https://rapidapi.com/UnlimitedAPI/api/google-web-search1/",
    "api_methods": [
        {
            "name": "Search",
            "description": "Search",
            "url": "https://google-web-search1.p.rapidapi.com/",
            "parameters": [
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "Search query. You can use Google Search parameters.\n\ne.g. adele site:youtube.com\ne.g. harry potter filetype:pdf\ne.g. inurl:store\ne.g. ronaldo -cristiano",
                    "default": "World Cup",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "related_keywords",
                    "type": "STRING",
                    "description": "Shows related keywords. Default: false\n\nEntering true, increases API latency",
                    "default": "true",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "limit",
                    "type": "NUMBER",
                    "description": "Max results number. Max recommended value is 300.",
                    "default": "20",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 300
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you find information about the latest iPhone models?",
                    "parameters": {
                        "query": "latest iPhone models"
                    }
                },
                {
                    "utterance": "Search for recipes with chocolate and show related keywords.",
                    "parameters": {
                        "query": "recipes with chocolate",
                        "related_keywords": "true"
                    }
                },
                {
                    "utterance": "Look up the best hiking trails in California and limit the results to 50.",
                    "parameters": {
                        "query": "best hiking trails in California",
                        "limit": 50
                    }
                },
                {
                    "utterance": "Find articles about climate change.",
                    "parameters": {
                        "query": "articles about climate change"
                    }
                },
                {
                    "utterance": "Search for the top 10 tourist attractions in Paris.",
                    "parameters": {
                        "query": "top tourist attractions in Paris",
                        "limit": 10
                    }
                },
                {
                    "utterance": "Can you show me the latest news on electric cars and include related keywords?",
                    "parameters": {
                        "query": "latest news on electric cars",
                        "related_keywords": "true"
                    }
                },
                {
                    "utterance": "Find PDF files about machine learning.",
                    "parameters": {
                        "query": "machine learning filetype:pdf"
                    }
                },
                {
                    "utterance": "Search for Adele's music videos on YouTube.",
                    "parameters": {
                        "query": "adele site:youtube.com"
                    }
                },
                {
                    "utterance": "Look up information on the Mars rover and show related keywords.",
                    "parameters": {
                        "query": "Mars rover",
                        "related_keywords": "true"
                    }
                },
                {
                    "utterance": "Find the top 5 restaurants in New York City.",
                    "parameters": {
                        "query": "top restaurants in New York City",
                        "limit": 5
                    }
                }
            ]
        }
    ]
}