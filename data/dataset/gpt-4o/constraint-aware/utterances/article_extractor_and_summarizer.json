                {
                    "name": "url",
                    "type": "STRING",
                    "description": "",
                    "default": "https://time.com/6266679/musk-ai-open-letter/",
                    "required": true
                },
                {
                    "name": "lang",
                    "type": "ENUM",
                    "description": "Target language to translate summary into",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "en",
                                "es",
                                "fr",
                                "de",
                                "it",
                                "pt",
                                "ru",
                                "zh",
                                "ja",
                                "ko",
                                "ar",
                                "hi",
                                "bn",
