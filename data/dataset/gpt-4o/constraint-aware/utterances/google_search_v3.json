{
    "name": "Google Search_v3",
    "description": "API used to retrieve data from Google search results in real time. Support web search and image search",
    "url": "https://rapidapi.com/neoscrap-net/api/google-search72/",
    "api_methods": [
        {
            "name": "Image Search",
            "description": "This endpoint is used for image searches",
            "url": "https://google-search72.p.rapidapi.com/imagesearch",
            "parameters": [
                {
                    "name": "q",
                    "type": "STRING",
                    "description": "",
                    "default": "Word Cup",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "start",
                    "type": "STRING",
                    "description": "The index of the first result to return.",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "num",
                    "type": "NUMBER",
                    "description": "Number of search results to return.\nValid values are integers between 1 and 20, inclusive",
                    "default": "10",
                    "required": false,
                    "constraints": {
                        "values": {
                            "min": 1,
                            "max": 20
                        }
                    }
                },
                {
                    "name": "gl",
                    "type": "STRING",
                    "description": "The gl parameter value is a two-letter country code. The gl parameter boosts search results whose country of origin matches the parameter value.",
                    "default": "us",
                    "required": false,
                    "constraints": {
                        "format": "two-letter country code"
                    }
                },
                {
                    "name": "lr",
                    "type": "STRING",
                    "description": "Restricts the search to documents written in a particular language",
                    "default": "lang_en",
                    "required": false,
                    "constraints": {}
