                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "lat",
                    "type": "NUMBER",
                    "description": "Latitude of a geo-location in degrees. ",
                    "default": "49.3427818",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "from",
                    "type": "STRING",
                    "description": "Start date in YYYY-MM-DD format.",
                    "default": "2021-12-31",
                    "required": true,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                },
                {
                    "name": "to",
                    "type": "STRING",
                    "description": "End date in YYYY-MM-DD format. \n\nNote: if time period relative to `from` is more than 366 days it will automatically be replaced with this date.",
                    "default": "2022-09-31",
                    "required": false,
                    "constraints": {
                        "format": "YYYY-MM-DD",
                        "inter-dependency": "from, to: The time period between 'from' and 'to' must not exceed 366 days."
                    }
                }
            ],
