{
    "name": "Weather by API-Ninjas",
    "description": "Get the latest weather data for any region in the world. See more info at https://api-ninjas.com/api/weather.",
    "url": "https://rapidapi.com/apininjas/api/weather-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/weather",
            "description": "API Ninjas Weather API endpoint.",
            "url": "https://weather-by-api-ninjas.p.rapidapi.com/v1/weather",
            "parameters": [
                {
                    "name": "country",
                    "type": "STRING",
                    "description": "Country name.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lon",
                    "type": "STRING",
                    "description": "Longitude of desired location. If used, **lat** parameter must also be supplied.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "lon, lat: If lon is used, lat parameter must also be supplied."
                    }
                },
                {
                    "name": "zip",
                    "type": "NUMBER",
                    "description": "5-digit Zip code (United States only).",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "min": 10000,
                            "max": 99999
                        }
                    }
                },
                {
                    "name": "state",
                    "type": "STRING",
                    "description": "US state (United States only).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "city",
                    "type": "STRING",
                    "description": "City name.",
                    "default": "Seattle",
                    "required": false
                },
                {
                    "name": "lat",
                    "type": "NUMBER",
                    "description": "Latitude of desired location. If used, **lon** parameter must also be supplied.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "lat, lon: If lat is used, lon parameter must also be supplied."
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the weather like in New York City?",
                    "parameters": {
                        "city": "New York"
                    }
                },
                {
                    "utterance": "Can you give me the weather update for Los Angeles, California?",
                    "parameters": {
                        "city": "Los Angeles",
                        "state": "California"
                    }
                },
                {
                    "utterance": "I'd like to know the current weather in Paris, France.",
                    "parameters": {
                        "city": "Paris",
                        "country": "France"
                    }
                },
                {
                    "utterance": "What's the weather forecast for the area with zip code 90210?",
                    "parameters": {
                        "zip": 90210
                    }
                },
                {
                    "utterance": "Tell me the weather conditions at latitude 34.05 and longitude -118.25.",
                    "parameters": {
                        "lat": 34.05,
                        "lon": -118.25
                    }
                },
                {
                    "utterance": "How's the weather in Tokyo?",
                    "parameters": {
                        "city": "Tokyo"
                    }
                },
                {
                    "utterance": "Could you provide the weather details for Miami, Florida?",
                    "parameters": {
                        "city": "Miami",
                        "state": "Florida"
                    }
                },
                {
                    "utterance": "What's the weather like in Berlin, Germany?",
                    "parameters": {
                        "city": "Berlin",
                        "country": "Germany"
                    }
                },
                {
                    "utterance": "Give me the weather report for the location with zip code 30301.",
                    "parameters": {
                        "zip": 30301
                    }
                },
                {
                    "utterance": "I'd like to know the weather at coordinates 51.51, -0.13.",
                    "parameters": {
                        "lat": 51.51,
                        "lon": -0.13
                    }
                }
            ]
        }
    ]
}