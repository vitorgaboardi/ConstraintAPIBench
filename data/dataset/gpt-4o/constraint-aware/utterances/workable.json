{
    "name": "Workable",
    "description": "Workable, recruit people by creating beautiful job posts through an API",
    "url": "https://rapidapi.com/orliesaurus/api/workable/",
    "api_methods": [
        {
            "name": "/candidates",
            "description": "Creating a candidate",
            "url": "https://workable.p.rapidapi.com/{API-KEY}/jobs/{SHORTCODE}/candidates",
            "parameters": [
                {
                    "name": "candidate",
                    "type": "STRING",
                    "description": "Look at the example on the right hand side for all parameters",
                    "default": "JSON Object",
                    "required": true
                },
                {
                    "name": "api-key",
                    "type": "STRING",
                    "description": "Your APIKey provided by Workable",
                    "default": "YOUR-API-KEY",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "shortcode",
                    "type": "STRING",
                    "description": "The shortcode is a unique identifier for each jobs and can be seen by invoking  \"/jobs\"",
                    "default": "1234",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Add John Doe as a candidate for the software engineer position with the job code SE123.",
                    "parameters": {
                        "candidate": "John Doe",
                        "shortcode": "SE123"
                    }
                },
                {
                    "utterance": "Can you create a candidate profile for Jane Smith for the marketing manager role with the code MM456?",
                    "parameters": {
                        "candidate": "Jane Smith",
                        "shortcode": "MM456"
                    }
                },
                {
                    "utterance": "Please register Emily Johnson as a candidate for the data analyst job, using the code DA789.",
                    "parameters": {
                        "candidate": "Emily Johnson",
                        "shortcode": "DA789"
                    }
                },
                {
                    "utterance": "I need to add Michael Brown to the candidate list for the project manager position, job code PM321.",
