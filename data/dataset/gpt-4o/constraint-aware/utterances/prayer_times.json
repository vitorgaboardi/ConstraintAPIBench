{
    "name": "Prayer Times",
    "description": "A flexible RESTful prayer times API that returns JSON times for a given day or a calendar for a whole month.\r\n\r\nFor more detailed documentation, various methods and additional endpoints, please see the complete documentation on http://aladhan.com/prayer-times-api.",
    "url": "https://rapidapi.com/meezaan/api/prayer-times/",
    "api_methods": [
        {
            "name": "Calendar by Address",
            "description": "Get a prayer times calendar for a month by address",
            "url": "https://aladhan.p.rapidapi.com/calendarByAddress",
            "parameters": [
                {
                    "name": "address",
                    "type": "STRING",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "year",
                    "type": "NUMBER",
                    "description": "4 digit year - example 2017",
                    "default": "",
                    "required": true
                },
                {
                    "name": "month",
                    "type": "NUMBER",
                    "description": "2 digit month, example 03 for March",
                    "default": "",
                    "required": true
                },
                {
                    "name": "method",
                    "type": "NUMBER",
                    "description": "Any of the prayer time calculation methods specified on https://aladhan.com/calculation-methods",
                    "default": "",
                    "required": false
                },
                {
                    "name": "school",
                    "type": "NUMBER",
                    "description": "1 for Hanfi. 0 for all others, including, Shafi, Hanbali, etc.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "latitudeAdjustmentMethod",
                    "type": "NUMBER",
                    "description": "Method for adjusting times higher latitudes - for instance, if you are checking timings in the UK or Sweden. 1 - Middle of the Night 2 - One Seventh 3 - Angle Based",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you provide the prayer times for March 2023 in New York City?",
                    "parameters": {
                        "address": "New York City",
                        "year": 2023,
                        "month": 3
                    }
                },
                {
                    "utterance": "I need the prayer calendar for London in April 2024 using the Hanafi school.",
                    "parameters": {
                        "address": "London",
                        "year": 2024,
                        "month": 4,
                        "school": 1
                    }
                },
                {
                    "utterance": "Get me the prayer times for May 2022 in Paris with the calculation method 2.",
                    "parameters": {
                        "address": "Paris",
                        "year": 2022,
                        "month": 5,
                        "method": 2
                    }
                },
                {
                    "utterance": "What's the prayer schedule for June 2025 in Tokyo using the angle-based latitude adjustment?",
                    "parameters": {
                        "address": "Tokyo",
                        "year": 2025,
                        "month": 6,
                        "latitudeAdjustmentMethod": 3
                    }
                },
                {
                    "utterance": "Show me the prayer times for July 2021 in Sydney with the middle of the night latitude adjustment.",
                    "parameters": {
                        "address": "Sydney",
                        "year": 2021,
                        "month": 7,
                        "latitudeAdjustmentMethod": 1
                    }
                },
                {
                    "utterance": "I'd like the prayer calendar for August 2023 in Cairo using method 5.",
                    "parameters": {
                        "address": "Cairo",
                        "year": 2023,
                        "month": 8,
                        "method": 5
                    }
                },
                {
                    "utterance": "Can you give me the prayer times for September 2024 in Dubai with the one-seventh latitude adjustment?",
                    "parameters": {
                        "address": "Dubai",
                        "year": 2024,
                        "month": 9,
                        "latitudeAdjustmentMethod": 2
                    }
                },
                {
                    "utterance": "I need the prayer schedule for October 2022 in Istanbul using the Shafi school.",
                    "parameters": {
                        "address": "Istanbul",
                        "year": 2022,
                        "month": 10,
                        "school": 0
                    }
                },
                {
                    "utterance": "What's the prayer calendar for November 2025 in Kuala Lumpur with calculation method 3?",
                    "parameters": {
                        "address": "Kuala Lumpur",
                        "year": 2025,
                        "month": 11,
                        "method": 3
                    }
                },
                {
                    "utterance": "Provide the prayer times for December 2021 in Los Angeles using method 7 and the Hanafi school.",
                    "parameters": {
                        "address": "Los Angeles",
                        "year": 2021,
                        "month": 12,
                        "method": 7,
                        "school": 1
                    }
                }
            ]
        }
    ]
}