{
    "name": "Currency Converter by API-Ninjas",
    "description": "Convert currencies using the latest exchange rates. See more info at https://api-ninjas.com/api/convertcurrency.",
    "url": "https://rapidapi.com/apininjas/api/currency-converter-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/convertcurrency",
            "description": "API Ninjas Convert Currency API endpoint.",
            "url": "https://currency-converter-by-api-ninjas.p.rapidapi.com/v1/convertcurrency",
            "parameters": [
                {
                    "name": "amount",
                    "type": "NUMBER",
                    "description": "Amount of currency to convert.",
                    "default": "5000",
                    "required": true,
                    "constraints": {
                        "values": {
                            "min": 0
                        }
                    }
                },
                {
                    "name": "have",
                    "type": "STRING",
                    "description": "Currency you currently hold. Must be 3-character currency code (e.g. **USD**).",
                    "default": "USD",
                    "required": true,
                    "constraints": {
                        "format": "3-character currency code"
                    }
                },
                {
                    "name": "want",
                    "type": "STRING",
                    "description": "Currency you want to convert to. Must be 3-character currency code (e.g. **EUR**)",
                    "default": "EUR",
                    "required": true,
                    "constraints": {
                        "format": "3-character currency code"
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "How much is 1000 US dollars in euros?",
                    "parameters": {
                        "amount": 1000,
                        "have": "USD",
                        "want": "EUR"
                    }
                },
                {
                    "utterance": "Convert 2500 British pounds to Japanese yen.",
                    "parameters": {
                        "amount": 2500,
                        "have": "GBP",
                        "want": "JPY"
                    }
                },
                {
                    "utterance": "I need to know the value of 3000 Canadian dollars in Swiss francs.",
                    "parameters": {
                        "amount": 3000,
                        "have": "CAD",
                        "want": "CHF"
                    }
                },
                {
                    "utterance": "What's the equivalent of 1500 euros in Australian dollars?",
                    "parameters": {
                        "amount": 1500,
                        "have": "EUR",
                        "want": "AUD"
                    }
                },
                {
                    "utterance": "Can you convert 5000 Indian rupees to US dollars?",
                    "parameters": {
                        "amount": 5000,
                        "have": "INR",
                        "want": "USD"
                    }
                },
                {
                    "utterance": "How many New Zealand dollars will I get for 2000 euros?",
                    "parameters": {
                        "amount": 2000,
                        "have": "EUR",
                        "want": "NZD"
                    }
                },
                {
                    "utterance": "Please convert 3500 Japanese yen to Canadian dollars.",
                    "parameters": {
                        "amount": 3500,
                        "have": "JPY",
                        "want": "CAD"
                    }
                },
                {
                    "utterance": "I want to exchange 4000 Swiss francs to British pounds.",
                    "parameters": {
                        "amount": 4000,
                        "have": "CHF",
                        "want": "GBP"
                    }
                },
                {
                    "utterance": "How much is 6000 Australian dollars in Indian rupees?",
                    "parameters": {
                        "amount": 6000,
                        "have": "AUD",
                        "want": "INR"
                    }
                },
                {
                    "utterance": "Convert 4500 New Zealand dollars to Canadian dollars.",
                    "parameters": {
                        "amount": 4500,
                        "have": "NZD",
                        "want": "CAD"
                    }
                }
            ]
        }
    ]
}