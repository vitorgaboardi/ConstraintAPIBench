{
    "name": "Recipe by API-Ninjas",
    "description": "Search over 200,000 creative recipes. See more info at https://api-ninjas.com/api/recipe.",
    "url": "https://rapidapi.com/apininjas/api/recipe-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/recipe",
            "description": "Get a list of recipes for a given search query. Returns at most 10 results. To access more than the first 10 results, use the offset parameter to offset results in multiple API calls.",
            "url": "https://recipe-by-api-ninjas.p.rapidapi.com/v1/recipe",
            "parameters": [
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "query text to search.",
                    "default": "italian wedding soup",
                    "required": true
                },
                {
                    "name": "offset",
                    "type": "STRING",
                    "description": "number of results to offset for pagination.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you find me some recipes for chocolate cake?",
                    "parameters": {
                        "query": "chocolate cake"
                    }
                },
