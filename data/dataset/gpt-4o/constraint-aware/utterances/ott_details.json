        {
            "name": "Advanced Search",
            "description": "This endpoint allows you to search for a movie or tvshow based on various parameters such as release year , imdb rating , genre , language etc.",
            "url": "https://ott-details.p.rapidapi.com/advancedsearch",
            "parameters": [
                {
                    "name": "start_year",
                    "type": "NUMBER",
                    "description": "Enter any year between 1970 to 2020 to get results.",
                    "default": "1970",
                    "required": false,
                    "constraints": {
                        "values": {
                            "min": 1970,
                            "max": 2020
                        }
                    }
                },
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "",
                    "default": "1",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "max_imdb",
                    "type": "NUMBER",
                    "description": "",
                    "default": "7.8",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 10,
                            "min": 0
                        }
                    }
                },
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "Enter type 'movie' or 'show'",
                    "default": "movie",
                    "required": false,
                    "constraints": {
                        "values": {
