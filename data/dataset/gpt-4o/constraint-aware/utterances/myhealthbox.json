        {
            "name": "Updated Documents",
            "description": "Search documents that have been updated after a certain date or within a date interval",
            "url": "https://myhealthbox.p.rapidapi.com/search/updatedDocuments",
            "parameters": [
                {
                    "name": "sd",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "",
                    "default": "2020-06-01",
                    "required": true,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                },
                {
                    "name": "t",
                    "type": "ENUM",
                    "description": "Document type, one of:  \npil (Patient Information Leaflet), spc (Summary of Product Characteristics), par (Public Assessment Report), sds (Safety Data Sheet), man (User Manual), inf (Product Information), mmr (Additional Risk Minimisation Measures), hta (Health Technology Assessment Report)",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "pil",
                                "spc",
                                "par",
                                "sds",
                                "man",
                                "inf",
                                "mmr",
                                "hta"
                            ]
                        }
                    }
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "ed",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                },
                {
                    "name": "limit",
                    "type": "NUMBER",
                    "description": "Default value: 32\nMax value: 100",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 100
                        }
                    }
                },
                {
                    "name": "c",
                    "type": "STRING",
                    "description": "See the Supported countries section in the Developers Manual",
                    "default": "us",
                    "required": false
                },
                {
                    "name": "f",
                    "type": "ENUM",
                    "description": "Field optional parameter, if present it can only have 1 value. Perform date comparison on this attribute (date when record was updated in the myHealthbox database) instead of the default one (date when the actual document was updated).",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "date"
                            ]
                        }
                    }
                },
                {
                    "name": "l",
                    "type": "STRING",
                    "description": "See the Supported countries section in the Developers Manual.",
                    "default": "en",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me the updated documents from June 1, 2021, related to patient information leaflets.",
                    "parameters": {
                        "sd": "2021-06-01",
                        "t": "pil"
                    }
                },
                {
                    "utterance": "I need the documents updated after March 15, 2022, with a limit of 50 results.",
                    "parameters": {
                        "sd": "2022-03-15",
                        "limit": 50
                    }
                },
                {
                    "utterance": "Find the safety data sheets updated since January 1, 2023, in the UK.",
                    "parameters": {
                        "sd": "2023-01-01",
                        "t": "sds",
                        "c": "uk"
                    }
                },
                {
                    "utterance": "Can you get me the documents updated between February 1, 2022, and February 28, 2022?",
                    "parameters": {
                        "sd": "2022-02-01",
                        "ed": "2022-02-28"
                    }
                },
                {
                    "utterance": "Retrieve the user manuals updated after July 10, 2021, in French.",
                    "parameters": {
                        "sd": "2021-07-10",
                        "t": "man",
                        "l": "fr"
                    }
                },
                {
                    "utterance": "Give me the health technology assessment reports updated from April 5, 2020, with a maximum of 20 entries.",
                    "parameters": {
                        "sd": "2020-04-05",
                        "t": "hta",
                        "limit": 20
                    }
                },
                {
                    "utterance": "List the documents updated after May 1, 2021, starting from the 10th entry.",
                    "parameters": {
                        "sd": "2021-05-01",
                        "from": 10
                    }
                },
                {
                    "utterance": "I want the public assessment reports updated since December 1, 2022, using the database update date.",
                    "parameters": {
                        "sd": "2022-12-01",
                        "t": "par",
                        "f": "date"
                    }
                },
                {
                    "utterance": "Fetch the product information documents updated after August 15, 2021, in Spanish.",
                    "parameters": {
                        "sd": "2021-08-15",
                        "t": "inf",
                        "l": "es"
                    }
                },
                {
                    "utterance": "Can you provide the additional risk minimisation measures updated from November 20, 2020, in Canada?",
                    "parameters": {
                        "sd": "2020-11-20",
                        "t": "mmr",
                        "c": "ca"
                    }
                }
            ]
