{
    "name": "Veriphone",
    "description": "Global phone number verification in a free, fast, reliable JSON API",
    "url": "https://rapidapi.com/Veriphone/api/veriphone/",
    "api_methods": [
        {
            "name": "verify",
            "description": "Global phone number verification",
            "url": "https://veriphone.p.rapidapi.com/verify",
            "parameters": [
                {
                    "name": "phone",
                    "type": "STRING",
                    "description": "The phone number to verify",
                    "default": "+4915123577723",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "default_country",
                    "type": "STRING",
                    "description": "The default country in a 2 letters ISO format. Example: US, RU.  Optional: the country will be infered from the prefix, from this parameter or from the IP address (in that order).",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "format": "2 letters ISO format"
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you verify the phone number +4915123577723?",
                    "parameters": {
                        "phone": "+4915123577723"
                    }
                },
                {
