{
    "name": "Text Translator",
    "description": "Translate text to 100+ languages \u200b. Fast processing, cost saving. Free up to 100,000 characters per month",
    "url": "https://rapidapi.com/dickyagustin/api/text-translator2/",
    "api_methods": [
        {
            "name": "translate",
            "description": "This endpoint is used to translate text from one language to another using the post  method",
            "url": "https://text-translator2.p.rapidapi.com/translate",
            "parameters": [
                {
                    "name": "target_language",
                    "type": "STRING",
                    "description": "Target language, filled with supported language codes such as: **en**, **eu**, **id**, and many others.",
                    "default": "id",
                    "required": true,
                    "constraints": {
                        "format": "language code"
                    }
                },
                {
                    "name": "source_language",
                    "type": "STRING",
                    "description": "Source language, filled with supported language codes such as: **en**, **eu**, **id**, and many others.\nYou can also set the value '**auto**' if you want an API that detects the language automatically",
                    "default": "en",
                    "required": true,
                    "constraints": {
                        "format": "language code",
                        "values": {
                            "enumerated": [
                                "auto"
                            ]
                        }
                    }
                },
                {
                    "name": "text",
                    "type": "STRING",
                    "description": "The field must be filled with text content to be translated, maximum 15,000 characters",
                    "default": "What is your name?",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 15000
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you translate 'Hello, how are you?' from English to Spanish?",
                    "parameters": {
                        "target_language": "es",
                        "source_language": "en",
                        "text": "Hello, how are you?"
                    }
                },
                {
                    "utterance": "I need the French translation for 'Good morning'.",
                    "parameters": {
                        "target_language": "fr",
                        "source_language": "en",
                        "text": "Good morning"
                    }
                },
                {
                    "utterance": "Translate 'Je t'aime' to English, please.",
                    "parameters": {
                        "target_language": "en",
                        "source_language": "fr",
                        "text": "Je t'aime"
                    }
                },
                {
                    "utterance": "Could you convert 'Hola, \u00bfqu\u00e9 tal?' from Spanish to German?",
                    "parameters": {
                        "target_language": "de",
                        "source_language": "es",
                        "text": "Hola, \u00bfqu\u00e9 tal?"
                    }
                },
                {
                    "utterance": "What's the Italian version of 'I love pizza'?",
                    "parameters": {
                        "target_language": "it",
                        "source_language": "en",
                        "text": "I love pizza"
                    }
                },
                {
                    "utterance": "Please translate 'Guten Tag' to Japanese.",
                    "parameters": {
                        "target_language": "ja",
                        "source_language": "de",
                        "text": "Guten Tag"
                    }
                },
                {
                    "utterance": "How do you say 'Thank you' in Chinese?",
                    "parameters": {
                        "target_language": "zh",
                        "source_language": "en",
                        "text": "Thank you"
                    }
                },
                {
                    "utterance": "Translate 'Ciao, come stai?' from Italian to Portuguese.",
                    "parameters": {
                        "target_language": "pt",
                        "source_language": "it",
                        "text": "Ciao, come stai?"
                    }
                },
                {
                    "utterance": "Can you translate 'Bonjour tout le monde' to Russian?",
                    "parameters": {
                        "target_language": "ru",
                        "source_language": "fr",
                        "text": "Bonjour tout le monde"
                    }
                },
                {
                    "utterance": "I need 'Good night' translated to Korean.",
                    "parameters": {
                        "target_language": "ko",
                        "source_language": "en",
                        "text": "Good night"
                    }
                }
            ]
        }
    ]
}