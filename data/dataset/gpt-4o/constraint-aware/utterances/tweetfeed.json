{
    "name": "TweetFeed",
    "description": "Free feed with IOCs -  malicious URLs, domains, IPs, and hashes. [Website: https://tweetfeed.live]",
    "url": "https://rapidapi.com/danlopgom/api/tweetfeed/",
    "api_methods": [
        {
            "name": "v1",
            "description": "Version 1 Endpoint.",
            "url": "https://tweetfeed.p.rapidapi.com/v1/{time}/{filter1}/{filter2}",
            "parameters": [
                {
                    "name": "time",
                    "type": "string",
                    "description": "Select 1 of these timeframes:\n\n**today** - Today starting 00:00 UTC)\n**week** - Last 7 days\n**month** - Last 30 days\n**year** - Last 365 days",
                    "default": "today",
                    "required": true,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "today",
                                "week",
                                "month",
                                "year"
                            ]
                        }
                    }
                },
                {
                    "name": "filter1",
                    "type": "string",
                    "description": "Can be an specific user, type or tag:\n\nType \u2192 **url** / **domain** / **ip** / **sha256** / **md5**\nTag \u2192 **phishing** / **ransomware** / **CobaltStrike** ...\nUser \u2192 **@malwrhunterteam** / **@1ZRR4H** / **@MBThreatIntel** / ... (don't forget the @)",
                    "default": "phishing",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "url",
                                "domain",
                                "ip",
                                "sha256",
                                "md5",
                                "phishing",
                                "ransomware",
                                "CobaltStrike",
                                "@malwrhunterteam",
                                "@1ZRR4H",
                                "@MBThreatIntel"
                            ]
                        }
                    }
                },
                {
                    "name": "filter2",
                    "type": "string",
                    "description": "Can be an specific user, type or tag:\n\nType \u2192 **url** / **domain** / **ip** / **sha256** / **md5**\nTag \u2192 **phishing** / **ransomware** / **CobaltStrike** ...\nUser \u2192 **@malwrhunterteam** / **@1ZRR4H** / **@MBThreatIntel** / ... (don't forget the @)",
                    "default": "url",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "url",
                                "domain",
                                "ip",
                                "sha256",
                                "md5",
                                "phishing",
                                "ransomware",
                                "CobaltStrike",
                                "@malwrhunterteam",
                                "@1ZRR4H",
                                "@MBThreatIntel"
                            ]
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me the malicious URLs detected today.",
                    "parameters": {
                        "time": "today",
                        "filter1": "url"
                    }
                },
                {
                    "utterance": "What are the phishing threats reported this week?",
                    "parameters": {
                        "time": "week",
                        "filter1": "phishing"
                    }
                },
                {
                    "utterance": "Give me the list of domains flagged for ransomware in the past month.",
                    "parameters": {
                        "time": "month",
                        "filter1": "ransomware",
                        "filter2": "domain"
                    }
                },
                {
                    "utterance": "Can you provide the IPs associated with CobaltStrike for the last year?",
                    "parameters": {
                        "time": "year",
                        "filter1": "CobaltStrike",
                        "filter2": "ip"
                    }
                },
                {
                    "utterance": "Fetch the SHA256 hashes identified today.",
                    "parameters": {
                        "time": "today",
                        "filter1": "sha256"
                    }
                },
                {
                    "utterance": "I need the MD5 hashes from the past week.",
                    "parameters": {
                        "time": "week",
                        "filter1": "md5"
                    }
                },
                {
                    "utterance": "List the malicious domains reported by @malwrhunterteam this month.",
                    "parameters": {
                        "time": "month",
                        "filter1": "@malwrhunterteam",
                        "filter2": "domain"
                    }
                },
                {
                    "utterance": "What URLs did @1ZRR4H flag as malicious today?",
                    "parameters": {
                        "time": "today",
                        "filter1": "@1ZRR4H",
                        "filter2": "url"
                    }
                },
                {
                    "utterance": "Show the IP addresses tagged by @MBThreatIntel over the last 30 days.",
                    "parameters": {
                        "time": "month",
                        "filter1": "@MBThreatIntel",
                        "filter2": "ip"
                    }
                },
                {
                    "utterance": "Provide the list of phishing URLs from the past year.",
                    "parameters": {
                        "time": "year",
                        "filter1": "phishing",
                        "filter2": "url"
                    }
                }
            ]
        }
    ]
}