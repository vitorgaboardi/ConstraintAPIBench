                {
                    "name": "county",
                    "type": "STRING",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "state",
                    "type": "STRING",
                    "description": "",
                    "default": "NY",
                    "required": false
                },
                {
                    "name": "street",
                    "type": "STRING",
                    "description": "",
                    "default": "34 West 13th Street",
                    "required": false
                },
                {
                    "name": "viewbox",
                    "type": "STRING",
                    "description": "Format: x1,y1,x2,y2 - restrict search area to a bounded box defined by two coordinates (x is longitude, y is latitude). The two coordinates have to span a box to work.",
                    "default": "",
