{
    "name": "Tasty",
    "description": "API to query data about recipe, plan, ingredients, etc... as on official site",
    "url": "https://rapidapi.com/apidojo/api/tasty/",
    "api_methods": [
        {
            "name": "recipes/list",
            "description": "List recipes by option filters or name",
            "url": "https://tasty.p.rapidapi.com/recipes/list",
            "parameters": [
                {
                    "name": "size",
                    "type": "NUMBER",
                    "description": "Number of items returned per response",
                    "default": "20",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "The offset of items to be ignored in response for paging",
                    "default": "0",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "Leave empty to sort by popular as default OR one of the following : approved_at:desc|approved_at:asc",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "approved_at:desc",
                                "approved_at:asc"
                            ]
                        }
                    }
                },
                {
                    "name": "q",
                    "type": "STRING",
                    "description": "Name of food or, ingredients to search by",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "tags",
                    "type": "STRING",
                    "description": "Get suitable values from /tags/list API",
                    "default": "under_30_minutes",
                    "required": false,
                    "constraints": {}
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me some popular recipes?",
                    "parameters": {
                        "sort": ""
                    }
                },
                {
                    "utterance": "Find recipes for chocolate cake.",
                    "parameters": {
                        "q": "chocolate cake"
                    }
                },
                {
                    "utterance": "What are some quick recipes under 30 minutes?",
                    "parameters": {
                        "tags": "under_30_minutes"
                    }
                },
                {
                    "utterance": "List the latest approved recipes.",
                    "parameters": {
                        "sort": "approved_at:desc"
                    }
                },
                {
                    "utterance": "Do you have any vegan recipes?",
                    "parameters": {
                        "tags": "vegan"
                    }
                },
                {
                    "utterance": "Show me recipes for pasta sorted by the oldest approval date.",
                    "parameters": {
                        "q": "pasta",
                        "sort": "approved_at:asc"
                    }
                },
                {
                    "utterance": "Find some dessert recipes.",
                    "parameters": {
                        "tags": "dessert"
                    }
                },
                {
                    "utterance": "Can you list recipes for chicken curry?",
                    "parameters": {
                        "q": "chicken curry"
                    }
                },
                {
                    "utterance": "What are some recipes for a quick breakfast?",
                    "parameters": {
                        "tags": "breakfast",
                        "sort": ""
                    }
                },
                {
                    "utterance": "Show me the most recently approved vegetarian recipes.",
                    "parameters": {
                        "tags": "vegetarian",
                        "sort": "approved_at:desc"
                    }
                }
            ]
        },
