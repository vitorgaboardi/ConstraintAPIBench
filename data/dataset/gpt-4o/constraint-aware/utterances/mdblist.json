{
    "name": "MDBList",
    "description": "Get Movies and TV Shows data with ratings from multiple providers in JSON format",
    "url": "https://rapidapi.com/linaspurinis/api/mdblist/",
    "api_methods": [
        {
            "name": "Search by Title",
            "description": "Search for Movies or TV Shows by title, include option year or mediatype filter to narrow your results",
            "url": "https://mdblist.p.rapidapi.com/",
            "parameters": [
                {
                    "name": "s",
                    "type": "STRING",
                    "description": "Movie or TV Show Title to search",
                    "default": "jaws",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "l",
                    "type": "NUMBER",
                    "description": "Limit returned items.\nYou can use numbes from 1 to 100 (default is 50)",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 100,
                            "min": 1
                        }
                    }
                },
                {
                    "name": "y",
                    "type": "NUMBER",
                    "description": "Year (format YYYY, for example 2020) to limit title search (would also include previous and next year movies)",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "format": "YYYY"
                    }
                },
                {
                    "name": "m",
                    "type": "ENUM",
                    "description": "Search for only movie or tv show, leave empty for both",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "movie",
                                "tv show"
                            ]
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you find information about the movie 'Inception'?",
                    "parameters": {
                        "s": "Inception",
                        "m": "movie"
                    }
                },
                {
                    "utterance": "Show me TV shows from 2015 with the title 'Breaking Bad'.",
                    "parameters": {
                        "s": "Breaking Bad",
                        "y": 2015,
                        "m": "tv show"
                    }
                },
                {
                    "utterance": "I'm looking for movies titled 'The Matrix' released around 1999.",
                    "parameters": {
                        "s": "The Matrix",
                        "y": 1999,
                        "m": "movie"
                    }
                },
                {
                    "utterance": "Find the first 10 results for 'The Office'.",
                    "parameters": {
                        "s": "The Office",
                        "l": 10
                    }
                },
                {
                    "utterance": "Search for TV shows called 'Friends' and limit the results to 20.",
                    "parameters": {
                        "s": "Friends",
                        "l": 20,
                        "m": "tv show"
                    }
                },
                {
                    "utterance": "Get me movies with the title 'Titanic' from 1997.",
                    "parameters": {
