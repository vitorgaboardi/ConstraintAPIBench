{
    "name": "Domainr",
    "description": "Instant domain search.\r\n\r\nDocumentation available at https://domainr.com/docs/api",
    "url": "https://rapidapi.com/domainr/api/domainr/",
    "api_methods": [
        {
            "name": "/v2/register",
            "description": "Responds with an HTTP redirect to a supporting registrar.",
            "url": "https://domainr.p.rapidapi.com/v2/register",
            "parameters": [
                {
                    "name": "domain",
                    "type": "STRING",
                    "description": "Domain to register.",
                    "default": "example.com",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "registrar",
                    "type": "STRING",
                    "description": "The registrar's root domain.",
                    "default": "namecheap.com",
                    "required": false,
                    "constraints": {}
                }
            ],
            "utterances": [
                {
                    "utterance": "I want to register the domain techstartup.io.",
                    "parameters": {
                        "domain": "techstartup.io"
                    }
                },
                {
                    "utterance": "Can you help me register the domain greenenergy.org with GoDaddy?",
                    "parameters": {
                        "domain": "greenenergy.org",
                        "registrar": "godaddy.com"
                    }
                },
                {
                    "utterance": "Please register the domain mynewblog.net.",
                    "parameters": {
                        "domain": "mynewblog.net"
                    }
                },
                {
                    "utterance": "I need to register the domain artgallery.co with Namecheap.",
                    "parameters": {
                        "domain": "artgallery.co",
                        "registrar": "namecheap.com"
                    }
                },
                {
                    "utterance": "Register the domain traveladventures.us.",
                    "parameters": {
                        "domain": "traveladventures.us"
                    }
                },
                {
                    "utterance": "Can you register the domain healthyliving.info using Bluehost?",
                    "parameters": {
                        "domain": "healthyliving.info",
                        "registrar": "bluehost.com"
                    }
                },
                {
                    "utterance": "I'd like to register the domain codingacademy.dev.",
                    "parameters": {
                        "domain": "codingacademy.dev"
                    }
                },
                {
                    "utterance": "Register the domain petloversclub.org with Google Domains.",
                    "parameters": {
                        "domain": "petloversclub.org",
                        "registrar": "domains.google"
                    }
                },
                {
                    "utterance": "Can you assist me in registering the domain fashionhub.shop?",
                    "parameters": {
                        "domain": "fashionhub.shop"
                    }
                },
                {
                    "utterance": "I want to register the domain gourmetkitchen.store with Hover.",
                    "parameters": {
                        "domain": "gourmetkitchen.store",
                        "registrar": "hover.com"
                    }
                }
            ]
        }
    ]
}