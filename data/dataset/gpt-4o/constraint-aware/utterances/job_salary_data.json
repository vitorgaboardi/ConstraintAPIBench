                    "required": true
                },
                {
                    "name": "radius",
                    "type": "STRING",
                    "description": "Search radius in km (measured from *location*).\nDefault: `200`.",
                    "default": "200",
                    "required": false,
                    "constraints": {
                        "values": {
                            "min": 0
                        }
                    }
                }
