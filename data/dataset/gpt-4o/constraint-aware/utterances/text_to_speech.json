{
    "name": "Text-to-Speech",
    "description": "The Voice RSS Text-to-Speech (TTS) API allows conversion of textual content to speech easier than ever. Just connect to our Text-to-Speech (TTS) API with a few lines of code and get verbal representation of a textual content.",
    "url": "https://rapidapi.com/voicerss/api/text-to-speech-1/",
    "api_methods": [
        {
            "name": "Convert text to speech via HTTP POST",
            "description": "Converts textual content to audio content",
            "url": "https://voicerss-text-to-speech.p.rapidapi.com/",
            "parameters": [
                {
                    "name": "src",
                    "type": "STRING",
                    "description": "The textual content for converting to speech",
                    "default": "Hello, world!",
                    "required": true
                },
                {
                    "name": "hl",
                    "type": "STRING",
                    "description": "The textual content language",
                    "default": "en-us",
                    "required": true
                },
                {
                    "name": "r",
                    "type": "NUMBER",
                    "description": "The speech rate (speed). Allows values: from -10 (slowest speed) up to 10 (fastest speed). Default value: 0 (normal speed)",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 10,
                            "min": -10
                        }
                    }
                },
                {
                    "name": "c",
                    "type": "STRING",
                    "description": "The speech audio codec",
                    "default": "mp3",
                    "required": false
                },
                {
                    "name": "f",
                    "type": "STRING",
                    "description": "The speech audio formats",
                    "default": "8khz_8bit_mono",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you convert 'Good morning, everyone!' to speech in English?",
                    "parameters": {
                        "src": "Good morning, everyone!",
                        "hl": "en-us"
                    }
                },
                {
                    "utterance": "I need the text 'Bonjour tout le monde' converted to French speech at a slower rate.",
                    "parameters": {
                        "src": "Bonjour tout le monde",
                        "hl": "fr-fr",
                        "r": -5
                    }
                },
                {
                    "utterance": "Please convert 'Hola, \u00bfc\u00f3mo est\u00e1s?' to Spanish audio with the fastest speed.",
                    "parameters": {
                        "src": "Hola, \u00bfc\u00f3mo est\u00e1s?",
                        "hl": "es-es",
                        "r": 10
                    }
                },
                {
                    "utterance": "Turn 'Guten Tag' into German speech using the OGG codec.",
                    "parameters": {
                        "src": "Guten Tag",
                        "hl": "de-de",
                        "c": "ogg"
                    }
                },
                {
                    "utterance": "Can you convert 'Ciao a tutti' to Italian speech with a 16khz_16bit_stereo format?",
                    "parameters": {
                        "src": "Ciao a tutti",
                        "hl": "it-it",
                        "f": "16khz_16bit_stereo"
                    }
                },
                {
                    "utterance": "I want '\u3053\u3093\u306b\u3061\u306f' converted to Japanese speech at a normal speed.",
                    "parameters": {
                        "src": "\u3053\u3093\u306b\u3061\u306f",
                        "hl": "ja-jp",
                        "r": 0
                    }
                },
                {
                    "utterance": "Convert '\u041f\u0440\u0438\u0432\u0435\u0442, \u043a\u0430\u043a \u0434\u0435\u043b\u0430?' to Russian speech with a WAV codec.",
                    "parameters": {
                        "src": "\u041f\u0440\u0438\u0432\u0435\u0442, \u043a\u0430\u043a \u0434\u0435\u043b\u0430?",
                        "hl": "ru-ru",
                        "c": "wav"
                    }
                },
                {
                    "utterance": "Please convert '\uc548\ub155\ud558\uc138\uc694' to Korean speech with a slightly faster rate.",
                    "parameters": {
                        "src": "\uc548\ub155\ud558\uc138\uc694",
                        "hl": "ko-kr",
                        "r": 3
                    }
                },
                {
                    "utterance": "Can you convert 'Ol\u00e1, tudo bem?' to Portuguese speech with a 44khz_16bit_stereo format?",
                    "parameters": {
                        "src": "Ol\u00e1, tudo bem?",
                        "hl": "pt-pt",
                        "f": "44khz_16bit_stereo"
                    }
                },
                {
                    "utterance": "I need 'Merhaba' converted to Turkish speech with a normal speed and MP3 codec.",
                    "parameters": {
                        "src": "Merhaba",
                        "hl": "tr-tr",
                        "r": 0,
                        "c": "mp3"
                    }
                }
            ]
        }
    ]
}