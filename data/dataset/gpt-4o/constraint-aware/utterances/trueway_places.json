                    "name": "radius",
                    "type": "NUMBER",
                    "description": "The distance (in meters) within which to return results. Max = 10000 m",
                    "default": "180",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 10000
                        }
                    }
                },
                {
                    "name": "type",
