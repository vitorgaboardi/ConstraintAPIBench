{
    "name": "Holidays by API-Ninjas",
    "description": "Get past, present and future holiday data for any country. See more info at https://api-ninjas.com/api/holidays.",
    "url": "https://rapidapi.com/apininjas/api/holidays-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/holidays",
            "description": "API Ninjas Holidays endpoint",
            "url": "https://holidays-by-api-ninjas.p.rapidapi.com/v1/holidays",
            "parameters": [
                {
                    "name": "year",
                    "type": "NUMBER",
                    "description": "Calendar year between 2010 and 2030 (inclusive). Note: not all countries are guaranteed contain data going back to 2010.",
                    "default": "2021",
                    "required": true,
                    "constraints": {
                        "values": {
                            "min": 2010,
                            "max": 2030
                        }
                    }
                },
                {
                    "name": "country",
                    "type": "STRING",
                    "description": "Country name or ISO 3166-2 country code (preferred).",
                    "default": "us",
                    "required": true,
                    "constraints": {
                        "format": "ISO 3166-2 country code"
                    }
                },
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "Holiday type filter. Possible values are:\n\n- public_holiday\n- observance\n- national_holiday\n- season\n- state_holiday\n- optional_holiday\n- clock_change_daylight_saving_time\n- local_holiday\n- united_nations_observance\n- observance_christian\n- bank_holiday\n- common_local_holiday\n- national_holiday_christian\n- christian\n- observance_hebrew\n- jewish_holiday\n- muslim\n- hindu_holiday\n- restricted_holiday\n- official_holiday\n- national_holiday_orthodox\n- local_observance\n- ",
                    "default": "federal_holiday",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "public_holiday",
                                "observance",
                                "national_holiday",
                                "season",
                                "state_holiday",
                                "optional_holiday",
                                "clock_change_daylight_saving_time",
                                "local_holiday",
                                "united_nations_observance",
                                "observance_christian",
                                "bank_holiday",
                                "common_local_holiday",
                                "national_holiday_christian",
                                "christian",
                                "observance_hebrew",
                                "jewish_holiday",
                                "muslim",
                                "hindu_holiday",
                                "restricted_holiday",
                                "official_holiday",
                                "national_holiday_orthodox",
                                "local_observance"
                            ]
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "What are the public holidays in Germany for 2025?",
                    "parameters": {
                        "year": 2025,
                        "country": "DE",
                        "type": "public_holiday"
                    }
                },
                {
                    "utterance": "Can you list the national holidays in Japan for the year 2023?",
                    "parameters": {
                        "year": 2023,
                        "country": "JP",
                        "type": "national_holiday"
                    }
                },
                {
                    "utterance": "Tell me about the observances in India for 2022.",
                    "parameters": {
                        "year": 2022,
                        "country": "IN",
                        "type": "observance"
                    }
                },
                {
                    "utterance": "What are the holidays in the United States for 2021?",
                    "parameters": {
                        "year": 2021,
                        "country": "US"
                    }
                },
                {
                    "utterance": "List the Jewish holidays in Israel for 2024.",
                    "parameters": {
                        "year": 2024,
                        "country": "IL",
                        "type": "jewish_holiday"
                    }
                },
                {
                    "utterance": "Are there any bank holidays in the United Kingdom in 2026?",
                    "parameters": {
                        "year": 2026,
                        "country": "GB",
                        "type": "bank_holiday"
                    }
                },
                {
                    "utterance": "What are the local holidays in Brazil for 2027?",
                    "parameters": {
                        "year": 2027,
                        "country": "BR",
                        "type": "local_holiday"
                    }
                },
                {
                    "utterance": "Can you provide the list of Christian holidays in Italy for 2028?",
                    "parameters": {
                        "year": 2028,
                        "country": "IT",
                        "type": "christian"
                    }
                },
                {
                    "utterance": "What are the official holidays in Canada for 2029?",
                    "parameters": {
                        "year": 2029,
                        "country": "CA",
                        "type": "official_holiday"
                    }
                },
                {
                    "utterance": "Tell me about the Muslim holidays in Saudi Arabia for 2030.",
                    "parameters": {
                        "year": 2030,
                        "country": "SA",
                        "type": "muslim"
                    }
                }
            ]
        }
    ]
}