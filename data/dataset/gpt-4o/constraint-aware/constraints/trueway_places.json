{
    "name": "TrueWay Places",
    "description": "Search for places. Provides detailed information for over 100 million places and points of interest.",
    "url": "https://rapidapi.com/trueway/api/trueway-places/",
    "api_methods": [
        {
            "name": "FindPlacesNearby",
            "description": "Search for nearby places",
            "url": "https://trueway-places.p.rapidapi.com/FindPlacesNearby",
            "parameters": [
                {
                    "name": "location",
                    "type": "STRING",
                    "description": "The latitude/longitude around which to retrieve places",
                    "default": "37.783366,-122.402325",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "language",
                    "type": "STRING",
                    "description": "The language in which to return results",
                    "default": "en",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "radius",
                    "type": "NUMBER",
                    "description": "The distance (in meters) within which to return results. Max = 10000 m",
                    "default": "180",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 10000
                        }
                    }
                },
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "The type of places that are returned",
                    "default": "cafe",
                    "required": false,
                    "constraints": {}
                }
            ]
        },
        {
            "name": "FindPlaceByText",
            "description": "Search for places by text string",
            "url": "https://trueway-places.p.rapidapi.com/FindPlaceByText",
            "parameters": [
                {
                    "name": "text",
                    "type": "STRING",
                    "description": "A text string on which to search",
                    "default": "Children's Creativity Museum",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "language",
                    "type": "STRING",
                    "description": "The language in which to return results",
                    "default": "en",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "bounds",
                    "type": "STRING",
                    "description": "Rectangular bounds (south,west,north,east)",
                    "default": "",
                    "required": false,
                    "constraints": {}
                }
            ]
        }
    ]
}