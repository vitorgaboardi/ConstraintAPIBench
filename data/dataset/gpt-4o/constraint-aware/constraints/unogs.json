                    "constraints": {}
                },
                {
                    "name": "offset",
                    "type": "NUMBER",
                    "description": "where you want the response to start e.g. 10 starts with the 10th element",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "country_list",
                    "type": "STRING",
                    "description": "CSV list of country IDs e.g. 46,78",
