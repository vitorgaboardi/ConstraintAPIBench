            "name": "Nowcast",
            "description": "3-hour forecast in 15-minute time steps.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/15minutely/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "tz",
                    "type": "STRING",
                    "description": "Time zone in response (IANA time zone database names)",
                    "default": "Europe/London",
                    "required": false,
                    "constraints": {
                        "format": "IANA time zone database names"
                    }
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 12
                        }
                    }
                }
            ]
        },
