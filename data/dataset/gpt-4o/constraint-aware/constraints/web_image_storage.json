{
    "name": "Web Image Storage",
    "description": "Store images in web and get URL back. Deprecated, please use \"Image CDN\" API now.",
    "url": "https://rapidapi.com/thoenehannes/api/web-image-storage/",
    "api_methods": [
        {
            "name": "Get History",
            "description": "Returns the history as pages",
            "url": "https://web-image-storage.p.rapidapi.com/list",
            "parameters": [
                {
                    "name": "pageSize",
                    "type": "NUMBER",
                    "description": "The size of one page",
                    "default": "20",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "The page you want to return, starting at 0",
                    "default": "0",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                }
            ]
        }
    ]
}