    "url": "https://rapidapi.com/rapidspotify-rapidspotify-default/api/latest-spotify-downloader/",
    "api_methods": [
        {
            "name": "Download Track",
            "description": "Download a track by passing the details from the fetch track API",
            "url": "https://latest-spotify-downloader.p.rapidapi.com/download",
            "parameters": [
                {
                    "name": "Content-Type",
                    "type": "STRING",
                    "description": "",
                    "default": "audio/mpeg",
                    "required": true
                },
                {
                    "name": "track_name",
                    "type": "STRING",
                    "description": "",
                    "default": "TWFyYSBEaWwgQiBDaGFta2EgRHkubXAz ",
                    "required": true
                },
                {
                    "name": "track_id",
                    "type": "STRING",
                    "description": "",
                    "default": "2gAKyzcfFIlMYdJ2b836At",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ]
        },
        {
            "name": "Fetch Track Details",
            "description": "Download a track mp3 file returned by the download endpoint",
            "url": "https://latest-spotify-downloader.p.rapidapi.com/track/{track_id}",
            "parameters": [
                {
                    "name": "track_id",
                    "type": "string",
                    "description": "",
                    "default": "2gAKyzcfFIlMYdJ2b836At",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ]
        }
    ]
}