{
    "name": "MuscleWiki",
    "description": "The API provides information about exercises from MuscleWiki, including the name, category, target muscles,instructions for performing the exercise and a short video demonstration.",
    "url": "https://rapidapi.com/rahulbanerjee26/api/musclewiki/",
    "api_methods": [
        {
            "name": "GET Exercise by ID",
            "description": "Get an exercise by it's ID. Valid IDs [0,953]",
            "url": "https://musclewiki.p.rapidapi.com/exercises/20",
            "parameters": [
                {
                    "name": "id",
                    "type": "string",
                    "description": "Exercise's ID",
                    "default": "20",
                    "required": true,
                    "constraints": {
                        "values": {
                            "min": 0,
                            "max": 953
                        },
                        "id": true
                    }
                }
            ]
        },
        {
            "name": "GET Exercises",
            "description": "Get all Exercises",
            "url": "https://musclewiki.p.rapidapi.com/exercises",
            "parameters": [
                {
                    "name": "force",
                    "type": "STRING",
                    "description": "Filter by force",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "name",
                    "type": "STRING",
                    "description": "Filter by exercises name",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "muscle",
                    "type": "STRING",
                    "description": "Filter by targeted muscle",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "category",
                    "type": "STRING",
                    "description": "Filter by category",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "difficulty",
                    "type": "STRING",
                    "description": "Filter by difficulty",
                    "default": "",
                    "required": false,
                    "constraints": {}
                }
            ]
        },
        {
            "name": "GET Attributes",
            "description": "Get Attributes you can use to filter exercises",
            "url": "https://musclewiki.p.rapidapi.com/exercises/attributes",
            "parameters": []
        }
    ]
}