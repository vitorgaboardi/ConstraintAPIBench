{
    "name": "SynWave",
    "description": "A data storage management API offering the following features:\n\n",
    "url": "https://rapidapi.com/rpi4gx/api/synwave/",
    "api_methods": [
        {
            "name": "Upload a new file",
            "description": "Upload a new file.",
            "url": "https://synwave.p.rapidapi.com/v1/upload",
            "parameters": [
                {
                    "name": "file",
                    "type": "BINARY",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "auth_password",
                    "type": "STRING",
                    "description": "Password to protect this file. auth_username parameter is required if auth_password is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "expiration_seconds",
                    "type": "NUMBER",
                    "description": "Number of seconds before the file becomes unavailable.\n",
                    "default": 3600,
                    "required": false
                },
                {
                    "name": "auth_username",
                    "type": "STRING",
                    "description": "Username to protect this file. auth_password parameter is required if auth_username is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "download_limit",
                    "type": "NUMBER",
                    "description": "Number of downloads before the file becomes unavailable.\n",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Upload this document and set it to expire in 2 hours.",
                    "parameters": {
                        "file": "document",
                        "expiration_seconds": 7200
                    }
                },
                {
                    "utterance": "Please upload the image with a download limit of 5 times.",
                    "parameters": {
                        "file": "image",
                        "download_limit": 5
                    }
                },
                {
                    "utterance": "Upload the video file and protect it with username 'user123' and password 'securepass'.",
                    "parameters": {
                        "file": "video file",
                        "auth_username": "user123",
                        "auth_password": "securepass"
                    }
                },
                {
                    "utterance": "Can you upload this audio file and make it available for only 30 minutes?",
                    "parameters": {
                        "file": "audio file",
                        "expiration_seconds": 1800
                    }
                },
                {
                    "utterance": "Upload the PDF and set it to expire after 24 hours.",
                    "parameters": {
                        "file": "PDF",
                        "expiration_seconds": 86400
                    }
                },
                {
                    "utterance": "Upload this spreadsheet and limit the downloads to 10.",
                    "parameters": {
                        "file": "spreadsheet",
                        "download_limit": 10
                    }
                },
                {
                    "utterance": "Upload the presentation file with a password 'mypassword'.",
                    "parameters": {
                        "file": "presentation file",
                        "auth_password": "mypassword"
                    }
                },
                {
                    "utterance": "Upload this archive and protect it with username 'admin' and password 'adminpass'.",
                    "parameters": {
                        "file": "archive",
                        "auth_username": "admin",
                        "auth_password": "adminpass"
                    }
                },
                {
                    "utterance": "Upload the text file and make it available for 15 minutes only.",
                    "parameters": {
                        "file": "text file",
                        "expiration_seconds": 900
                    }
                },
                {
                    "utterance": "Upload this graphic file and set a download limit of 3.",
                    "parameters": {
                        "file": "graphic file",
                        "download_limit": 3
                    }
                }
            ]
        }
    ]
}