{
    "name": "Postal Ninja",
    "description": "Official Postal Ninja tracking API for developers",
    "url": "https://rapidapi.com/PostalNinja/api/postal-ninja/",
    "api_methods": [
        {
            "name": "createTrack",
            "description": "Returns ID of newly created package (status=CREATED) or stored ID if this tracking number already exists (status=FOUND).",
            "url": "https://postal-ninja.p.rapidapi.com/v1/track",
            "parameters": [
                {
                    "name": "trackCode",
                    "type": "STRING",
                    "description": "Tracking number",
                    "default": "",
                    "required": true
                },
                {
                    "name": "ds",
                    "type": "STRING",
                    "description": "Carrier ID (optional)",
                    "default": "",
                    "required": false
                },
                {
                    "name": "destCountry",
                    "type": "STRING",
                    "description": "Optional destination country code (ISO 3166-1 alpha-2)",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you track the package with the tracking number 1Z999AA10123456784?",
                    "parameters": {
                        "trackCode": "1Z999AA10123456784"
                    }
                },
                {
                    "utterance": "I need to find the status of my shipment with tracking code 1234567890.",
                    "parameters": {
                        "trackCode": "1234567890"
                    }
                },
                {
                    "utterance": "Please track the parcel with tracking number 9876543210 and carrier ID UPS.",
                    "parameters": {
                        "trackCode": "9876543210",
                        "ds": "UPS"
                    }
                },
                {
                    "utterance": "What is the current status of the package with tracking code AB123456789CD?",
                    "parameters": {
                        "trackCode": "AB123456789CD"
                    }
                },
                {
                    "utterance": "Track my package with tracking number 1Z12345E1512345676 and destination country US.",
                    "parameters": {
                        "trackCode": "1Z12345E1512345676",
                        "destCountry": "US"
                    }
                },
                {
                    "utterance": "Can you provide the tracking details for the package with code 1122334455?",
                    "parameters": {
                        "trackCode": "1122334455"
                    }
                },
                {
                    "utterance": "I need to track a shipment with tracking number 9988776655 and carrier ID FedEx.",
                    "parameters": {
                        "trackCode": "9988776655",
                        "ds": "FedEx"
                    }
                },
                {
                    "utterance": "Please find the status of my parcel with tracking code 5566778899.",
                    "parameters": {
                        "trackCode": "5566778899"
                    }
                },
                {
                    "utterance": "Track the package with tracking number 123ABC456DEF and destination country code CA.",
                    "parameters": {
                        "trackCode": "123ABC456DEF",
                        "destCountry": "CA"
                    }
                },
                {
                    "utterance": "What is the delivery status of the package with tracking code 1Z98765E1512345678?",
                    "parameters": {
