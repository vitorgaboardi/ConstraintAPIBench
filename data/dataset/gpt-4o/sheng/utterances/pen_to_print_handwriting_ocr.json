{
    "name": "Pen to Print - Handwriting OCR",
    "description": "Handwriting Recognition OCR - Convert scanned handwritten notes into editable text.",
    "url": "https://rapidapi.com/serendi/api/pen-to-print-handwriting-ocr/",
    "api_methods": [
        {
            "name": "recognize",
            "description": "extarct text from an image",
            "url": "https://pen-to-print-handwriting-ocr.p.rapidapi.com/recognize/",
            "parameters": [
                {
                    "name": "includeSubScan",
                    "type": "NUMBER",
                    "description": "integer [0/1] indicate whether the return value should include images of the split lines.",
                    "default": 0,
                    "required": false
                },
                {
                    "name": "srcImg",
                    "type": "BINARY",
                    "description": "file object of the image (jpg or png format) to be scanned.",
                    "default": "File",
                    "required": false
                },
                {
                    "name": "Session",
                    "type": "STRING",
                    "description": "GUID string to represent the session. Will appear in billing for tracking purposes.",
                    "default": "string",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you convert this handwritten note into text?",
                    "parameters": {
                        "srcImg": "File"
                    }
                },
                {
                    "utterance": "Please extract the text from this image and include images of the split lines.",
                    "parameters": {
                        "srcImg": "File",
                        "includeSubScan": 1
                    }
                },
                {
                    "utterance": "I need the text from this scanned note for session ID 1234-5678-9101.",
                    "parameters": {
                        "srcImg": "File",
                        "Session": "1234-5678-9101"
                    }
                },
                {
                    "utterance": "Convert this image to text and track it under session ABCD-1234.",
                    "parameters": {
                        "srcImg": "File",
                        "Session": "ABCD-1234"
                    }
                },
                {
                    "utterance": "Extract text from this image without including split line images.",
                    "parameters": {
                        "srcImg": "File",
                        "includeSubScan": 0
                    }
                },
                {
                    "utterance": "Can you scan this handwritten page and provide the text output?",
                    "parameters": {
                        "srcImg": "File"
                    }
                },
                {
                    "utterance": "Please convert this handwritten document to text and include sub-scan images.",
                    "parameters": {
                        "srcImg": "File",
                        "includeSubScan": 1
                    }
                },
                {
                    "utterance": "I need the text from this image for billing session XYZ-9876.",
                    "parameters": {
                        "srcImg": "File",
                        "Session": "XYZ-9876"
                    }
                },
                {
                    "utterance": "Extract the text from this image and track it with session ID 5678-ABCD.",
                    "parameters": {
                        "srcImg": "File",
                        "Session": "5678-ABCD"
                    }
                },
                {
                    "utterance": "Can you convert this handwritten note into editable text and include the split line images?",
                    "parameters": {
                        "srcImg": "File",
                        "includeSubScan": 1
                    }
                }
            ]
        }
    ]
}