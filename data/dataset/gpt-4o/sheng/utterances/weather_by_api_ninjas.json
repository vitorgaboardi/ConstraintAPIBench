{
    "name": "Weather by API-Ninjas",
    "description": "Get the latest weather data for any region in the world. See more info at https://api-ninjas.com/api/weather.",
    "url": "https://rapidapi.com/apininjas/api/weather-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/weather",
            "description": "API Ninjas Weather API endpoint.",
            "url": "https://weather-by-api-ninjas.p.rapidapi.com/v1/weather",
            "parameters": [
                {
                    "name": "country",
                    "type": "STRING",
                    "description": "Country name.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lon",
                    "type": "STRING",
                    "description": "Longitude of desired location. If used, **lat** parameter must also be supplied.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "zip",
                    "type": "NUMBER",
                    "description": "5-digit Zip code (United States only).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "state",
                    "type": "STRING",
                    "description": "US state (United States only).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "city",
                    "type": "STRING",
                    "description": "City name.",
                    "default": "Seattle",
                    "required": false
                },
                {
                    "name": "lat",
                    "type": "NUMBER",
                    "description": "Latitude of desired location. If used, **lon** parameter must also be supplied.",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the weather like in Paris today?",
                    "parameters": {
                        "city": "Paris"
                    }
                },
                {
                    "utterance": "Can you give me the current weather in Tokyo, Japan?",
                    "parameters": {
                        "city": "Tokyo",
                        "country": "Japan"
                    }
                },
                {
                    "utterance": "What's the weather forecast for New York City?",
                    "parameters": {
                        "city": "New York City"
                    }
                },
                {
                    "utterance": "How's the weather in Los Angeles, California?",
                    "parameters": {
                        "city": "Los Angeles",
                        "state": "California"
                    }
                },
                {
                    "utterance": "Tell me the weather conditions in London.",
                    "parameters": {
                        "city": "London"
                    }
                },
                {
                    "utterance": "What's the weather like at latitude 34.05 and longitude -118.25?",
                    "parameters": {
                        "lat": 34.05,
                        "lon": -118.25
                    }
                },
                {
                    "utterance": "Can you provide the weather update for the zip code 90210?",
                    "parameters": {
                        "zip": 90210
                    }
                },
                {
                    "utterance": "What's the current weather in Sydney, Australia?",
                    "parameters": {
                        "city": "Sydney",
                        "country": "Australia"
                    }
                },
                {
                    "utterance": "How's the weather in Chicago, Illinois?",
                    "parameters": {
                        "city": "Chicago",
                        "state": "Illinois"
                    }
                },
                {
                    "utterance": "Give me the weather details for Berlin, Germany.",
                    "parameters": {
                        "city": "Berlin",
                        "country": "Germany"
                    }
                }
            ]
        }
    ]
}