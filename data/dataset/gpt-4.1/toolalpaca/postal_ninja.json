{
    "name": "Postal Ninja",
    "description": "Official Postal Ninja tracking API for developers",
    "url": "https://rapidapi.com/PostalNinja/api/postal-ninja/",
    "api_methods": [
        {
            "name": "createTrack",
            "description": "Returns ID of newly created package (status=CREATED) or stored ID if this tracking number already exists (status=FOUND).",
            "url": "https://postal-ninja.p.rapidapi.com/v1/track",
            "parameters": [
                {
                    "name": "trackCode",
                    "type": "STRING",
                    "description": "Tracking number",
                    "default": "",
                    "required": true
                },
                {
                    "name": "ds",
                    "type": "STRING",
                    "description": "Carrier ID (optional)",
                    "default": "",
                    "required": false
                },
                {
                    "name": "destCountry",
                    "type": "STRING",
                    "description": "Optional destination country code (ISO 3166-1 alpha-2)",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you start tracking my Amazon order with the tracking number 1Z999AA10123456784?",
                    "parameters": {
                        "trackCode": "1Z999AA10123456784"
                    }
                },
                {
                    "utterance": "I need to add my DHL package, tracking code JD0146000065423456, that's coming to Germany.",
                    "parameters": {
                        "trackCode": "JD0146000065423456",
                        "destCountry": "DE"
                    }
                },
                {
                    "utterance": "Please register the parcel with tracking number LX123456789CN from China Post.",
                    "parameters": {
                        "trackCode": "LX123456789CN",
                        "ds": "china-post"
                    }
                },
                {
                    "utterance": "Track my eBay shipment with code 9400111899223856921456, which is headed to the United States.",
                    "parameters": {
                        "trackCode": "9400111899223856921456",
                        "destCountry": "US"
                    }
                },
                {
                    "utterance": "Could you add the FedEx package 612999987654321 to my tracking list?",
                    "parameters": {
                        "trackCode": "612999987654321",
                        "ds": "fedex"
                    }
                },
                {
                    "utterance": "I want to monitor the delivery with tracking number RR123456789RU that's being sent to France.",
                    "parameters": {
                        "trackCode": "RR123456789RU",
                        "destCountry": "FR"
                    }
                },
                {
                    "utterance": "Register my package with tracking code 00340434161094000000 from Deutsche Post.",
                    "parameters": {
                        "trackCode": "00340434161094000000",
                        "ds": "deutsche-post"
                    }
                },
                {
                    "utterance": "Can you help me track the parcel 9274899991234567890123 that's coming to Canada?",
                    "parameters": {
                        "trackCode": "9274899991234567890123",
                        "destCountry": "CA"
                    }
                },
                {
                    "utterance": "Add the Royal Mail shipment with tracking number ZW123456789GB to my account.",
                    "parameters": {
                        "trackCode": "ZW123456789GB",
                        "ds": "royal-mail"
                    }
                },
                {
                    "utterance": "Please start tracking the package with code EA123456785HK destined for Australia.",
                    "parameters": {
                        "trackCode": "EA123456785HK",
                        "destCountry": "AU"
                    }
                }
            ]
        }
    ]
}