{
    "name": "SynWave",
    "description": "A data storage management API offering the following features:\n\n",
    "url": "https://rapidapi.com/rpi4gx/api/synwave/",
    "api_methods": [
        {
            "name": "Upload a new file",
            "description": "Upload a new file.",
            "url": "https://synwave.p.rapidapi.com/v1/upload",
            "parameters": [
                {
                    "name": "file",
                    "type": "BINARY",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "auth_password",
                    "type": "STRING",
                    "description": "Password to protect this file. auth_username parameter is required if auth_password is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "expiration_seconds",
                    "type": "NUMBER",
                    "description": "Number of seconds before the file becomes unavailable.\n",
                    "default": 3600,
                    "required": false
                },
                {
                    "name": "auth_username",
                    "type": "STRING",
                    "description": "Username to protect this file. auth_password parameter is required if auth_username is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "download_limit",
                    "type": "NUMBER",
                    "description": "Number of downloads before the file becomes unavailable.\n",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you upload the scanned copy of my passport and set it to expire in 10 minutes?",
                    "parameters": {
                        "file": "scanned_passport.pdf",
                        "expiration_seconds": 600
                    }
                },
                {
                    "utterance": "Please store the final project report and restrict access to only three downloads.",
                    "parameters": {
                        "file": "final_project_report.docx",
                        "download_limit": 3
                    }
                },
                {
                    "utterance": "I want to upload the wedding video and protect it with the username 'family' and password 'June2024'.",
                    "parameters": {
                        "file": "wedding_video.mp4",
                        "auth_username": "family",
                        "auth_password": "June2024"
                    }
                },
                {
                    "utterance": "Could you save the tax return PDF and make it available for just one hour?",
                    "parameters": {
                        "file": "tax_return_2023.pdf",
                        "expiration_seconds": 3600
                    }
                },
                {
                    "utterance": "Upload the conference presentation slides and set a download limit of 10.",
                    "parameters": {
                        "file": "conference_slides.pptx",
                        "download_limit": 10
                    }
                },
                {
                    "utterance": "I need to upload the family reunion photo album and ensure it expires after 24 hours.",
                    "parameters": {
                        "file": "reunion_album.zip",
                        "expiration_seconds": 86400
                    }
                },
                {
                    "utterance": "Can you upload the signed lease agreement and require the username 'tenant' and password 'Lease2024' for access?",
                    "parameters": {
                        "file": "lease_agreement_signed.pdf",
                        "auth_username": "tenant",
                        "auth_password": "Lease2024"
                    }
                },
                {
                    "utterance": "Please store the backup of my phone contacts and set it to expire in 30 minutes.",
                    "parameters": {
                        "file": "contacts_backup.vcf",
                        "expiration_seconds": 1800
                    }
                },
                {
                    "utterance": "Upload the recipe collection and allow it to be downloaded only twice.",
                    "parameters": {
                        "file": "recipe_collection.docx",
                        "download_limit": 2
                    }
                },
                {
                    "utterance": "I want to save the scanned medical records and protect them with the username 'doctor' and password 'Health2024', and make them expire in 2 hours.",
                    "parameters": {
                        "file": "medical_records.pdf",
                        "auth_username": "doctor",
                        "auth_password": "Health2024",
                        "expiration_seconds": 7200
                    }
                }
            ]
        }
    ]
}