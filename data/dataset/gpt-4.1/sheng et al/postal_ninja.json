{
    "name": "Postal Ninja",
    "description": "Official Postal Ninja tracking API for developers",
    "url": "https://rapidapi.com/PostalNinja/api/postal-ninja/",
    "api_methods": [
        {
            "name": "createTrack",
            "description": "Returns ID of newly created package (status=CREATED) or stored ID if this tracking number already exists (status=FOUND).",
            "url": "https://postal-ninja.p.rapidapi.com/v1/track",
            "parameters": [
                {
                    "name": "trackCode",
                    "type": "STRING",
                    "description": "Tracking number",
                    "default": "",
                    "required": true
                },
                {
                    "name": "ds",
                    "type": "STRING",
                    "description": "Carrier ID (optional)",
                    "default": "",
                    "required": false
                },
                {
                    "name": "destCountry",
                    "type": "STRING",
                    "description": "Optional destination country code (ISO 3166-1 alpha-2)",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Track my package with tracking number LX123456789CN.",
                    "parameters": {
                        "trackCode": "LX123456789CN"
                    }
                },
                {
                    "utterance": "Can you add tracking for parcel number 9400111899223856921234 with USPS as the carrier?",
                    "parameters": {
                        "trackCode": "9400111899223856921234",
                        "ds": "USPS"
                    }
                },
                {
                    "utterance": "Please create a tracking entry for shipment RR987654321RU going to Germany.",
                    "parameters": {
                        "trackCode": "RR987654321RU",
                        "destCountry": "DE"
                    }
                },
                {
                    "utterance": "Register tracking number EA123456785HK with Hong Kong Post for delivery to the UK.",
                    "parameters": {
                        "trackCode": "EA123456785HK",
                        "ds": "HongKongPost",
                        "destCountry": "GB"
                    }
                },
                {
                    "utterance": "Add my package with tracking code CP123456789JP to the system.",
                    "parameters": {
                        "trackCode": "CP123456789JP"
                    }
                },
                {
                    "utterance": "Track shipment 1Z999AA10123456784 with UPS as the carrier and destination Canada.",
                    "parameters": {
                        "trackCode": "1Z999AA10123456784",
                        "ds": "UPS",
                        "destCountry": "CA"
                    }
                },
                {
                    "utterance": "Please create a tracking record for package S1234567890 with destination country Australia.",
                    "parameters": {
                        "trackCode": "S1234567890",
                        "destCountry": "AU"
                    }
                },
                {
                    "utterance": "Track my order with tracking number RB123456789SG and carrier Singapore Post.",
                    "parameters": {
                        "trackCode": "RB123456789SG",
                        "ds": "SingaporePost"
                    }
                },
                {
                    "utterance": "Add tracking for parcel 00340434161094000000 going to France.",
                    "parameters": {
                        "trackCode": "00340434161094000000",
                        "destCountry": "FR"
                    }
                },
                {
                    "utterance": "Register tracking code EE987654321SE with PostNord for delivery to Sweden.",
                    "parameters": {
                        "trackCode": "EE987654321SE",
                        "ds": "PostNord",
                        "destCountry": "SE"
                    }
                }
            ]
        }
    ]
}