{
    "name": "SynWave",
    "description": "A data storage management API offering the following features:\n\n",
    "url": "https://rapidapi.com/rpi4gx/api/synwave/",
    "api_methods": [
        {
            "name": "Upload a new file",
            "description": "Upload a new file.",
            "url": "https://synwave.p.rapidapi.com/v1/upload",
            "parameters": [
                {
                    "name": "file",
                    "type": "BINARY",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "auth_password",
                    "type": "STRING",
                    "description": "Password to protect this file. auth_username parameter is required if auth_password is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "expiration_seconds",
                    "type": "NUMBER",
                    "description": "Number of seconds before the file becomes unavailable.\n",
                    "default": 3600,
                    "required": false
                },
                {
                    "name": "auth_username",
                    "type": "STRING",
                    "description": "Username to protect this file. auth_password parameter is required if auth_username is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "download_limit",
                    "type": "NUMBER",
                    "description": "Number of downloads before the file becomes unavailable.\n",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Upload this document to SynWave.",
                    "parameters": {
                        "file": "this document"
                    }
                },
                {
                    "utterance": "Can you upload my presentation file to SynWave with a password?",
                    "parameters": {
                        "file": "my presentation file",
                        "auth_password": "password"
                    }
                },
                {
                    "utterance": "Upload the report to SynWave and set it to expire in 2 hours.",
                    "parameters": {
                        "file": "the report",
                        "expiration_seconds": 7200
                    }
                },
                {
                    "utterance": "Please upload the image to SynWave with a username and password for protection.",
                    "parameters": {
                        "file": "the image",
                        "auth_username": "username",
                        "auth_password": "password"
                    }
                },
                {
                    "utterance": "Upload the video to SynWave and limit it to 5 downloads.",
                    "parameters": {
                        "file": "the video",
                        "download_limit": 5
                    }
                },
                {
                    "utterance": "Can you upload this file to SynWave with a password and set it to expire in 1 hour?",
                    "parameters": {
                        "file": "this file",
                        "auth_password": "password",
                        "expiration_seconds": 3600
                    }
                },
                {
                    "utterance": "Upload the document to SynWave with a username and password, and limit it to 3 downloads.",
                    "parameters": {
                        "file": "the document",
                        "auth_username": "username",
                        "auth_password": "password",
                        "download_limit": 3
                    }
                },
                {
                    "utterance": "Please upload the file to SynWave and set it to expire in 30 minutes.",
                    "parameters": {
                        "file": "the file",
                        "expiration_seconds": 1800
                    }
                },
                {
                    "utterance": "Upload the image to SynWave with a password and limit it to 10 downloads.",
                    "parameters": {
                        "file": "the image",
                        "auth_password": "password",
                        "download_limit": 10
                    }
                },
                {
                    "utterance": "Can you upload this file to SynWave with a username and password, set it to expire in 1 hour, and limit it to 2 downloads?",
                    "parameters": {
                        "file": "this file",
                        "auth_username": "username",
                        "auth_password": "password",
                        "expiration_seconds": 3600,
                        "download_limit": 2
                    }
                }
            ]
        }
    ]
}