{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "url": "https://rapidapi.com/foreca-ltd-foreca-ltd-default/api/foreca-weather/",
    "api_methods": [
        {
            "name": "Daily",
            "description": "Daily forecast.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/daily/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What is the daily weather forecast for New York?",
                    "parameters": {
                        "location": "New York"
                    }
                },
                {
                    "utterance": "Can you provide the daily weather forecast for London in Fahrenheit?",
                    "parameters": {
                        "location": "London",
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "What is the daily weather forecast for Paris with wind speed in kilometers per hour?",
                    "parameters": {
                        "location": "Paris",
                        "windunit": "KMH"
                    }
                },
                {
                    "utterance": "Get the daily weather forecast for Tokyo for the next 10 days.",
                    "parameters": {
                        "location": "Tokyo",
                        "periods": 10
                    }
                },
                {
                    "utterance": "What is the daily weather forecast for Berlin at an altitude of 50 meters?",
                    "parameters": {
                        "location": "Berlin",
                        "alt": 50
                    }
                },
                {
                    "utterance": "Can you provide the daily weather forecast for Sydney with the full dataset?",
                    "parameters": {
                        "location": "Sydney",
                        "dataset": "full"
                    }
                },
                {
                    "utterance": "What is the daily weather forecast for Los Angeles with wind speed in miles per hour?",
                    "parameters": {
                        "location": "Los Angeles",
                        "windunit": "MPH"
                    }
                },
                {
                    "utterance": "Get the daily weather forecast for Chicago for the next 12 days.",
                    "parameters": {
                        "location": "Chicago",
                        "periods": 12
                    }
                },
                {
                    "utterance": "What is the daily weather forecast for Toronto in Celsius?",
                    "parameters": {
                        "location": "Toronto",
                        "tempunit": "C"
                    }
                },
                {
                    "utterance": "Can you provide the daily weather forecast for Mumbai at an altitude of 100 meters?",
                    "parameters": {
                        "location": "Mumbai",
                        "alt": 100
                    }
                }
            ]
        }
    ]
}