{
    "name": "Weather by API-Ninjas",
    "description": "Get the latest weather data for any region in the world. See more info at https://api-ninjas.com/api/weather.",
    "url": "https://rapidapi.com/apininjas/api/weather-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/weather",
            "description": "API Ninjas Weather API endpoint.",
            "url": "https://weather-by-api-ninjas.p.rapidapi.com/v1/weather",
            "parameters": [
                {
                    "name": "country",
                    "type": "STRING",
                    "description": "Country name.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lon",
                    "type": "STRING",
                    "description": "Longitude of desired location. If used, **lat** parameter must also be supplied.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "zip",
                    "type": "NUMBER",
                    "description": "5-digit Zip code (United States only).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "state",
                    "type": "STRING",
                    "description": "US state (United States only).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "city",
                    "type": "STRING",
                    "description": "City name.",
                    "default": "Seattle",
                    "required": false
                },
                {
                    "name": "lat",
                    "type": "NUMBER",
                    "description": "Latitude of desired location. If used, **lon** parameter must also be supplied.",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the weather like in New York City?",
                    "parameters": {
                        "city": "New York City"
                    }
                },
                {
                    "utterance": "Can you tell me the weather in Los Angeles, California?",
                    "parameters": {
                        "city": "Los Angeles",
                        "state": "California"
                    }
                },
                {
                    "utterance": "Get the weather for the coordinates 40.7128, -74.0060.",
                    "parameters": {
                        "lat": 40.7128,
                        "lon": -74.006
                    }
                },
                {
                    "utterance": "What's the weather in Chicago, Illinois?",
                    "parameters": {
                        "city": "Chicago",
                        "state": "Illinois"
                    }
                },
                {
                    "utterance": "Check the weather for the zip code 10001.",
                    "parameters": {
                        "zip": 10001
                    }
                },
                {
                    "utterance": "What's the weather in Paris, France?",
                    "parameters": {
                        "city": "Paris",
                        "country": "France"
                    }
                },
                {
                    "utterance": "Get the weather for the location at latitude 34.0522 and longitude -118.2437.",
                    "parameters": {
                        "lat": 34.0522,
                        "lon": -118.2437
                    }
                },
                {
                    "utterance": "What's the weather in Austin, Texas?",
                    "parameters": {
                        "city": "Austin",
                        "state": "Texas"
                    }
                },
                {
                    "utterance": "Check the weather for the zip code 90210.",
                    "parameters": {
                        "zip": 90210
                    }
                },
                {
                    "utterance": "What's the weather in Tokyo, Japan?",
                    "parameters": {
                        "city": "Tokyo",
                        "country": "Japan"
                    }
                }
            ]
        }
    ]
}