{
    "name": "CurrencyAPI.net",
    "description": "Real-time currency conversion on 152 currencies and cryptos",
    "url": "https://rapidapi.com/currencyapinet/api/currencyapi-net/",
    "api_methods": [
        {
            "name": "Convert",
            "description": "Convert a custom amount value from one currency to another",
            "url": "https://currencyapi-net.p.rapidapi.com/convert",
            "parameters": [
                {
                    "name": "to",
                    "type": "STRING",
                    "description": "Currency code of the currency you want to convert to. Eg, USD if you want to convert GBP to USD",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "Currency code"
                    }
                },
                {
                    "name": "amount",
                    "type": "STRING",
                    "description": "The value you want to convert. Eg 10.99 or 15",
                    "default": "",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "from",
                    "type": "STRING",
                    "description": "Currency code of the currency you want to convert from. Eg, GBP if you want to convert GBP to USD",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "Currency code"
                    }
                },
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "JSON",
                                "XML"
                            ]
                        },
                        "technical": true
                    }
                }
            ]
        },
        {
            "name": "Currencies",
            "description": "Gets list of currencies we support",
            "url": "https://currencyapi-net.p.rapidapi.com/currencies",
            "parameters": [
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "JSON",
                                "XML"
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "Timeframe",
            "description": "Display a currency conversion's historical rates within a given timeframe",
            "url": "https://currencyapi-net.p.rapidapi.com/timeframe",
            "parameters": [
                {
                    "name": "end_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-29",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "DATE (YYYY-MM-DD)"
                    }
                },
                {
                    "name": "base",
                    "type": "STRING",
                    "description": "The currency code you want to get the historical data of. Eg, USD",
                    "default": "USD",
                    "required": true,
                    "constraints": {
                        "format": "currency code"
                    }
                },
                {
                    "name": "start_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-25",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "DATE (YYYY-MM-DD)"
                    }
                },
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "JSON",
                                "XML"
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "Rates",
            "description": "Get live currency conversion rates of a given currency",
            "url": "https://currencyapi-net.p.rapidapi.com/rates",
            "parameters": [
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "JSON",
                                "XML"
                            ]
                        }
                    }
                },
                {
                    "name": "base",
                    "type": "STRING",
                    "description": "Change the base currency. Eg GBP or BTC",
                    "default": "USD",
                    "required": false,
                    "constraints": {
                        "format": "currency code"
                    }
                }
            ]
        },
        {
            "name": "History",
            "description": "Display the currency conversions historical rates for a particular day",
            "url": "https://currencyapi-net.p.rapidapi.com/history",
            "parameters": [
                {
                    "name": "date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the single date you wish to get the historical data from. Eg 2010-12-25",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "DATE (YYYY-MM-DD)"
                    }
                },
                {
                    "name": "base",
                    "type": "STRING",
                    "description": "The currency code you wish to get the historical rates of. Eg, USD",
                    "default": "USD",
                    "required": true,
                    "constraints": {
                        "format": "currency code"
                    }
                },
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "JSON",
                                "XML"
                            ]
                        }
                    }
                }
            ]
        }
    ]
}