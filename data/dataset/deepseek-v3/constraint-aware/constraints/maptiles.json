{
    "name": "MapTiles",
    "description": "Map Tiles for interactive online maps based on data by OpenStreetMap contributors with labels in English, French or Spanish. ",
    "url": "https://rapidapi.com/MapTilesApi/api/maptiles/",
    "api_methods": [
        {
            "name": "getMapTilewithSpanishLabels",
            "description": "Raster Map Tiles with Spanish Labels. Please see [tutorial](https://rapidapi.com/MapTilesApi/api/maptiles/tutorials/openstreetmap-in-spanish---add-a-spanish-map-to-your-website-with-leaflet-js-and-maptiles-api) on how to use the Spanish world map.",
            "url": "https://maptiles.p.rapidapi.com/es/map/v1/{z}/{x}/{y}.png",
            "parameters": [
                {
                    "name": "z",
                    "type": "NUMBER",
                    "description": "zoom (from 0 up to zoom 19)",
                    "default": "3",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 19,
                            "min": 0
                        }
                    }
                },
                {
                    "name": "x",
                    "type": "NUMBER",
                    "description": "X-number of tile (see documentation)",
                    "default": "4",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "y",
                    "type": "NUMBER",
                    "description": "Y-number of tile (see documentation)",
                    "default": "2",
                    "required": true,
                    "constraints": {}
                }
            ]
        },
        {
            "name": "getMapTilewithEnglishLabels",
            "description": "Raster Map Tiles with English Labels. Please see our [tutorials page](https://rapidapi.com/MapTilesApi/api/maptiles/tutorials) on how to use this.",
            "url": "https://maptiles.p.rapidapi.com/en/map/v1/{z}/{x}/{y}.png",
            "parameters": [
                {
                    "name": "x",
                    "type": "NUMBER",
                    "description": "X-number of tile (see documentation)",
                    "default": "6",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "z",
                    "type": "NUMBER",
                    "description": "zoom (from 0 up to zoom 19)",
                    "default": "3",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 19,
                            "min": 0
                        }
                    }
                },
                {
                    "name": "y",
                    "type": "NUMBER",
                    "description": "Y-number of tile (see documentation)",
                    "default": "3",
                    "required": true,
                    "constraints": {}
                }
            ]
        },
        {
            "name": "getMapTilewithFrenchLabels",
            "description": "Raster Map Tiles with French Labels",
            "url": "https://maptiles.p.rapidapi.com/fr/map/v1/{z}/{x}/{y}.png",
            "parameters": [
                {
                    "name": "x",
                    "type": "NUMBER",
                    "description": "X-number of tile (see documentation)",
                    "default": "8",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "y",
                    "type": "NUMBER",
                    "description": "Y-number of tile (see documentation)",
                    "default": "5",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "z",
                    "type": "NUMBER",
                    "description": "zoom (from 0 up to zoom 19)",
                    "default": "4",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 19,
                            "min": 0
                        }
                    }
                }
            ]
        },
        {
            "name": "getStandardMapTile",
            "description": "Standard tiles with labels in local language for a place (untranslated and untransliterated) as known from OpenStreetMap",
            "url": "https://maptiles.p.rapidapi.com/local/osm/v1/{z}/{x}/{y}.png",
            "parameters": [
                {
                    "name": "y",
                    "type": "NUMBER",
                    "description": "Y-number of tile (see documentation)",
                    "default": "3",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "x",
                    "type": "NUMBER",
                    "description": "X-number of tile (see documentation)",
                    "default": "6",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "z",
                    "type": "NUMBER",
                    "description": "zoom (from 0 up to zoom 19)",
                    "default": "3",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 19,
                            "min": 0
                        },
                        "technical": true
                    }
                }
            ]
        }
    ]
}