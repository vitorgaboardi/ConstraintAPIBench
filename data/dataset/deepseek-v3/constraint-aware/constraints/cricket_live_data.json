{
    "name": "Cricket Live Data",
    "description": "Fixtures, Results, and scorecards for the worlds most popular cricket tournaments",
    "url": "https://rapidapi.com/sportcontentapi/api/cricket-live-data/",
    "api_methods": [
        {
            "name": "Series",
            "description": "Lists available cricket series' to query",
            "url": "https://cricket-live-data.p.rapidapi.com/series",
            "parameters": []
        },
        {
            "name": "Fixtures",
            "description": "Lists next 50 fixtures across all series'",
            "url": "https://cricket-live-data.p.rapidapi.com/fixtures",
            "parameters": []
        },
        {
            "name": "Fixtures By Series",
            "description": "Lists Fixtures and Results for a given tournament.",
            "url": "https://cricket-live-data.p.rapidapi.com/fixtures-by-series/{series_id}",
            "parameters": [
                {
                    "name": "series_id",
                    "type": "NUMBER",
                    "description": "",
                    "default": "606",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ]
        },
        {
            "name": "Fixtures By Date",
            "description": "Lists fixtures for a given date",
            "url": "https://cricket-live-data.p.rapidapi.com/fixtures-by-date/{date}",
            "parameters": [
                {
                    "name": "date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "",
                    "default": "2020-09-21",
                    "required": true,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                }
            ]
        },
        {
            "name": "Results",
            "description": "Lists last 50 Results across all series'",
            "url": "https://cricket-live-data.p.rapidapi.com/results",
            "parameters": []
        },
        {
            "name": "Results By Date",
            "description": "Lists results for given date",
            "url": "https://cricket-live-data.p.rapidapi.com/results-by-date/{date}",
            "parameters": [
                {
                    "name": "date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "",
                    "default": "2020-09-20",
                    "required": true,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                }
            ]
        },
        {
            "name": "Match Scorecard",
            "description": "Lists full scorecard (updated live) for a given match id.",
            "url": "https://cricket-live-data.p.rapidapi.com/match/{match_id}",
            "parameters": [
                {
                    "name": "match_id",
                    "type": "NUMBER",
                    "description": "",
                    "default": "2432999",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ]
        }
    ]
}