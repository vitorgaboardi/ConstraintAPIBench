{
    "name": "Bayut",
    "description": "This API helps to query real estate in UAE to create a real estate site/application",
    "url": "https://rapidapi.com/apidojo/api/bayut/",
    "api_methods": [
        {
            "name": "properties/detail",
            "description": "Get detail information of a property",
            "url": "https://bayut.p.rapidapi.com/properties/detail",
            "parameters": [
                {
                    "name": "externalID",
                    "type": "STRING",
                    "description": "The value of externalID returned in ..../properties/list endpoint",
                    "default": "4937770",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ]
        },
        {
            "name": "agencies/list",
            "description": "List agencies or search for agencies by name",
            "url": "https://bayut.p.rapidapi.com/agencies/list",
            "parameters": [
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "One of the following : en|ar",
                    "default": "en",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "en",
                                "ar"
                            ]
                        },
                        "technical": true
                    }
                },
                {
                    "name": "hitsPerPage",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "25",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "Any term or phrase that you are familiar with",
                    "default": "patriot",
                    "required": false
                }
            ]
        },
        {
            "name": "agencies/get-listings",
            "description": "Get listing belonging to an agency",
            "url": "https://bayut.p.rapidapi.com/agencies/get-listings",
            "parameters": [
                {
                    "name": "agencySlug",
                    "type": "STRING",
                    "description": "The value of hits -> slug field returned in .../agencies/list endpoint",
                    "default": "patriot-real-estate-7737",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "categorySlug",
                    "type": "STRING",
                    "description": "One of the following : commerical-properties|mixed-use-land|factories|commercial-floors|bulk-units|labour-camps|shops|showrooms|industrial-land|commercial-buildings|commercial-plots|commercial-villas|warehouses|offices|residential-floors|hotel-apartments|penthouse|villas|residential-building|residential-plots|villa-compound|townhouses|apartments",
                    "default": "commerical-properties",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "commerical-properties",
                                "mixed-use-land",
                                "factories",
                                "commercial-floors",
                                "bulk-units",
                                "labour-camps",
                                "shops",
                                "showrooms",
                                "industrial-land",
                                "commercial-buildings",
                                "commercial-plots",
                                "commercial-villas",
                                "warehouses",
                                "offices",
                                "residential-floors",
                                "hotel-apartments",
                                "penthouse",
                                "villas",
                                "residential-building",
                                "residential-plots",
                                "villa-compound",
                                "townhouses",
                                "apartments"
                            ]
                        }
                    }
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "One of the following : price-desc|price-asc|city-level-score|date-desc|date-asc|verified-score",
                    "default": "price-asc",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "price-desc",
                                "price-asc",
                                "city-level-score",
                                "date-desc",
                                "date-asc",
                                "verified-score"
                            ]
                        }
                    }
                },
                {
                    "name": "completionStatus",
                    "type": "STRING",
                    "description": "One of the following : completed|under-construction\nThis parameter only takes effect if purpose is for-sale",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "completed",
                                "under-construction"
                            ]
                        },
                        "inter-dependency": "purpose, completionStatus: completionStatus only takes effect if purpose is for-sale"
                    }
                },
                {
                    "name": "rentFrequency",
                    "type": "STRING",
                    "description": "One of the following : monthly|yearly|weekly|daily\nThe parameter only takes effect if purpose is for-rent",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "monthly",
                                "yearly",
                                "weekly",
                                "daily"
                            ]
                        },
                        "inter-dependency": "purpose, rentFrequency: rentFrequency only takes effect if purpose is for-rent"
                    }
                },
                {
                    "name": "purpose",
                    "type": "STRING",
                    "description": "One of the following : for-rent|for-sale",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "for-rent",
                                "for-sale"
                            ]
                        }
                    }
                },
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "hitsPerPage",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "25",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "One of the following : en|ar",
                    "default": "en",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "en",
                                "ar"
                            ]
                        }
                    }
                }
            ]
        },
