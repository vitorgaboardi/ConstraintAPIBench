                    "type": "NUMBER",
                    "description": "League id",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "event_type",
                    "type": "STRING",
                    "description": "Status: `prematch`, `live`  Please note that prematch and live events are different",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "prematch",
                                "live"
                            ]
                        }
                    }
                },
                {
                    "name": "event_ids",
                    "type": "NUMBER",
                    "description": "Event id",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "is_have_odds",
                    "type": "BOOLEAN",
                    "description": "`1` or `0`. You can only get matches for which there are already open odds, or matches that will be given odds in the future",
                    "default": true,
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                1,
                                0
                            ]
                        }
                    }
                },
                {
                    "name": "since",
                    "type": "NUMBER",
                    "description": "Since UTC time. Calls return changes since the provided `since` value.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                }
            ]
        },
        {
            "name": "List of leagues",
            "description": "Get a list of leagues",
            "url": "https://pinnacle-odds.p.rapidapi.com/kit/v1/leagues",
            "parameters": [
                {
                    "name": "sport_id",
                    "type": "NUMBER",
                    "description": "Sport id",
                    "default": 1,
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                }
            ]
        }
    ]
}