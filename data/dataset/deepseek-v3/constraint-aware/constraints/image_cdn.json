{
    "name": "Image CDN",
    "description": "Upload images & access them in your application.",
    "url": "https://rapidapi.com/thoenehannes/api/image-cdn/",
    "api_methods": [
        {
            "name": "Show History",
            "description": "Shows all images you uploaded in a pageable list",
            "url": "https://image-cdn.p.rapidapi.com/list",
            "parameters": [
                {
                    "name": "pageSize",
                    "type": "NUMBER",
                    "description": "The size of one page, maximum 100",
                    "default": "100",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 100
                        },
                        "technical": true
                    }
                },
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "The page you want to return, starting at 0",
                    "default": "0",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                }
            ]
        }
    ]
}