{
    "name": "Public Holiday",
    "description": "An API for Public Holidays & Bank Holidays",
    "url": "https://rapidapi.com/theapiguy/api/public-holiday/",
    "api_methods": [
        {
            "name": "Public Holidays",
            "description": "Returns the public holidays from a given year and country.",
            "url": "https://public-holiday.p.rapidapi.com/{year}/{countrycode}",
            "parameters": [
                {
                    "name": "CountryCode",
                    "type": "STRING",
                    "description": "CountryCode",
                    "default": "US",
                    "required": true
                },
                {
                    "name": "Year",
                    "type": "STRING",
                    "description": "Year",
                    "default": "2019",
                    "required": true
                }
            ]
        }
    ]
}