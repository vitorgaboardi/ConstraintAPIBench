{
    "name": "ADSBexchange.com",
    "description": "Live air traffic and aircraft positional data via ADS-B / MLAT / UAT\n\nNOTE: Please scroll down to view the Terms & Conditions for use and Legal Notice. ",
    "url": "https://rapidapi.com/adsbx/api/adsbexchange-com1/",
    "api_methods": [
        {
            "name": "Aircraft Last Position by Hex ID (icao)",
            "description": "Returns one aircraft last known location using the mode-s hex (icao), if aircraft has flown in the last 48 hours.",
            "url": "https://adsbexchange-com1.p.rapidapi.com/v2/hex/{icao}/",
            "parameters": [
                {
                    "name": "icao",
                    "type": "string",
                    "description": "",
                    "default": "a4b605",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ]
        },
        {
            "name": "Aircraft Live Position by Hex ID (icao)",
            "description": "Returns one aircraft using the mode-s hex (icao)",
            "url": "https://adsbexchange-com1.p.rapidapi.com/v2/icao/{icao}/",
            "parameters": [
                {
                    "name": "icao",
                    "type": "string",
                    "description": "",
                    "default": "a4b605",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ]
        },
        {
            "name": "Single Aircraft Position by Hex ID (icao)",
            "description": "Single aircraft Position by ICAO Mode S 6-digit transponder hex code (if currently tracked)",
            "url": "https://adsbexchange-com1.p.rapidapi.com/v2/icao/{icao}/",
            "parameters": [
                {
                    "name": "icao",
                    "type": "STRING",
                    "description": "ICAO code 000000-FFFFFF",
                    "default": "a4b065",
                    "required": true,
                    "constraints": {
                        "format": "ICAO Mode S 6-digit transponder hex code",
                        "values": {
                            "min": "000000",
                            "max": "FFFFFF"
                        },
                        "id": true
                    }
                }
            ]
        },
        {
            "name": "Single Aircraft Position by Registration",
            "description": "Single aircraft Position by Registration (if currently tracked)",
            "url": "https://adsbexchange-com1.p.rapidapi.com/v2/registration/{reg}/",
            "parameters": [
                {
                    "name": "reg",
                    "type": "string",
                    "description": "",
                    "default": "N8737L",
                    "required": true,
                    "constraints": {}
                }
            ]
        },
        {
