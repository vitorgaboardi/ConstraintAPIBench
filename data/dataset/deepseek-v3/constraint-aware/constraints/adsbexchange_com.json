            "description": "Show all Aircraft within 250 Nautical Mile radius of a point",
            "url": "https://adsbexchange-com1.p.rapidapi.com/v2/lat/{lat}/lon/{lon}/dist/250/",
            "parameters": [
                {
                    "name": "lon",
                    "type": "NUMBER",
                    "description": "Longitude (-180 to 180) Hint: US is negative longitude!",
                    "default": "-0.45536",
                    "required": true,
