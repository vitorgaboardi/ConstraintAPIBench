{
    "name": "ADSBx Flight Sim Traffic",
    "description": "ADSBexchange.com traffic feed for flight simulators",
    "url": "https://rapidapi.com/adsbx/api/adsbx-flight-sim-traffic/",
    "api_methods": [
        {
            "name": "LiveTraffic",
            "description": "Returns all aircraft within 25nm radius of specified Lat/Lon",
            "url": "https://adsbx-flight-sim-traffic.p.rapidapi.com/api/aircraft/json/lat/{lat}/lon/{lon}/dist/25/",
            "parameters": [
                {
                    "name": "lat",
                    "type": "NUMBER",
                    "description": "Latitude (-90 to 90)",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 90,
                            "min": -90
                        }
                    }
                },
                {
                    "name": "lon",
                    "type": "NUMBER",
                    "description": "Longitude (-180 to 180) Hint: US is negative longitude!",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "values": {
                            "max": 180,
                            "min": -180
                        }
                    }
                }
            ]
        }
    ]
}