{
    "name": "YouTube Search Results",
    "description": "Fetch the YouTube Search Results and the information for each item for a specific search term, without any limits!",
    "url": "https://rapidapi.com/marindelija/api/youtube-search-results/",
    "api_methods": [
        {
            "name": "YouTubeSearchResults",
            "description": "Get all the YouTube Search Results Items and their Information! No limits and no Google API Key needed!",
            "url": "https://youtube-search-results.p.rapidapi.com/youtube-search/",
            "parameters": [
                {
                    "name": "q",
                    "type": "STRING",
                    "description": "The URL Encoded search Query",
                    "default": "justin+bieber",
                    "required": true,
                    "constraints": {
                        "format": "URL Encoded"
                    }
                },
                {
                    "name": "next",
                    "type": "STRING",
                    "description": "Leave empty if you want the results from the first page.\nUse the value from [nextpageRef] from the previous request (for example first page) to get the next page (for example 2nd page)\nMUST BE URL ENCODED",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "format": "URL Encoded",
                        "technical": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me YouTube videos about cooking recipes.",
                    "parameters": {
                        "q": "cooking+recipes"
                    }
                },
                {
                    "utterance": "Find YouTube results for the latest tech gadgets.",
                    "parameters": {
                        "q": "latest+tech+gadgets"
                    }
                },
                {
                    "utterance": "Can you search YouTube for workout routines?",
                    "parameters": {
                        "q": "workout+routines"
                    }
                },
                {
                    "utterance": "Look up YouTube videos on home improvement tips.",
                    "parameters": {
                        "q": "home+improvement+tips"
                    }
                },
                {
                    "utterance": "Search YouTube for tutorials on playing the guitar.",
                    "parameters": {
                        "q": "tutorials+playing+the+guitar"
                    }
                },
                {
                    "utterance": "Find YouTube content about travel destinations in Europe.",
                    "parameters": {
                        "q": "travel+destinations+in+Europe"
                    }
                },
                {
                    "utterance": "Show me YouTube videos about healthy eating habits.",
                    "parameters": {
                        "q": "healthy+eating+habits"
                    }
                },
                {
                    "utterance": "Can you find YouTube results for DIY crafts?",
                    "parameters": {
                        "q": "DIY+crafts"
                    }
                },
                {
                    "utterance": "Search YouTube for videos on learning Spanish.",
                    "parameters": {
                        "q": "learning+Spanish"
                    }
                },
                {
                    "utterance": "Find YouTube videos about the best hiking trails.",
                    "parameters": {
                        "q": "best+hiking+trails"
                    }
                }
            ]
        }
    ]
}