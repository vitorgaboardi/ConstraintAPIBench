                    "constraints": {
                        "format": "jpg or png format"
                    }
                },
                {
                    "name": "Session",
                    "type": "STRING",
                    "description": "GUID string to represent the session. Will appear in billing for tracking purposes.",
                    "default": "string",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                }
            ],
            "utterances": [
