{
    "name": "Cars by API-Ninjas",
    "description": "Get detailed data on tens of thousands of vehicle models from dozens of automakers. See more info at https://api-ninjas.com/api/cars.",
    "url": "https://rapidapi.com/apininjas/api/cars-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/cars",
            "description": "API Ninjas Cars API endpoint.",
            "url": "https://cars-by-api-ninjas.p.rapidapi.com/v1/cars",
            "parameters": [
                {
                    "name": "model",
                    "type": "STRING",
                    "description": "Model of vehicle.",
                    "default": "corolla",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "max_city_mpg",
                    "type": "NUMBER",
                    "description": "Maximum city fuel efficiency in miles per gallon.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "min_comb_mpg",
                    "type": "NUMBER",
                    "description": "Minimum combination (city + highway) fuel efficiency in miles per gallon.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "max_hwy_mpg",
                    "type": "NUMBER",
                    "description": "Maximum highway fuel efficiency in miles per gallon.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "fuel_type",
                    "type": "STRING",
                    "description": "Type of fuel used. Possible values: **gas**, **diesel**, **electricity**",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "gas",
                                "diesel",
                                "electricity"
                            ]
                        }
                    }
                },
                {
                    "name": "limit",
                    "type": "STRING",
                    "description": "How many results to return. Must be between **1** and **30**. Default is **5**",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 30,
                            "min": 1
                        },
                        "technical": true
                    }
                },
                {
                    "name": "drive",
                    "type": "STRING",
                    "description": "Drive transmission. Possible values: **fwd** (front-wheel drive), **rwd** (rear-wheel drive), **awd** (all-wheel drive), **4wd** (four-wheel drive)",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "fwd",
                                "rwd",
                                "awd",
                                "4wd"
                            ]
                        }
                    }
                },
                {
                    "name": "max_comb_mpg",
                    "type": "NUMBER",
                    "description": "Maximum combination (city + highway) fuel efficiency in miles per gallon.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "make",
                    "type": "STRING",
                    "description": "Vehicle manufacturer.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "transmission",
                    "type": "STRING",
                    "description": "Type of transmission. Possible values: **manual**, **automatic**",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "manual",
                                "automatic"
                            ]
                        }
                    }
                },
                {
                    "name": "year",
                    "type": "STRING",
                    "description": "Vehicle model year.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "min_hwy_mpg",
                    "type": "NUMBER",
                    "description": "Minimum highway fuel efficiency in miles per gallon.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "min_city_mpg",
                    "type": "NUMBER",
                    "description": "Minimum City fuel efficiency in miles per gallon.",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "cylinders",
                    "type": "NUMBER",
                    "description": "Number of cylinders. Possible values: **2, 3 4, 5, 6, 8, 10, 12, 16**",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                2,
                                3,
                                4,
                                5,
                                6,
                                8,
                                10,
                                12,
                                16
                            ]
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Find me cars with a minimum combined fuel efficiency of 30 mpg.",
                    "parameters": {
                        "min_comb_mpg": 30
                    }
                },
                {
                    "utterance": "Show me electric cars with automatic transmission.",
                    "parameters": {
                        "fuel_type": "electricity",
                        "transmission": "automatic"
                    }
                },
                {
                    "utterance": "What are some Toyota models with front-wheel drive?",
                    "parameters": {
                        "make": "Toyota",
                        "drive": "fwd"
                    }
                },
                {
                    "utterance": "I'm looking for cars with at least 6 cylinders and a minimum highway fuel efficiency of 25 mpg.",
                    "parameters": {
                        "cylinders": 6,
                        "min_hwy_mpg": 25
                    }
                },
                {
                    "utterance": "Can you find diesel cars from 2020 with rear-wheel drive?",
                    "parameters": {
                        "fuel_type": "diesel",
                        "year": "2020",
                        "drive": "rwd"
                    }
                },
                {
                    "utterance": "Show me cars with a maximum city fuel efficiency of 40 mpg.",
                    "parameters": {
                        "max_city_mpg": 40
                    }
                },
                {
                    "utterance": "Find me manual transmission cars with all-wheel drive.",
                    "parameters": {
                        "transmission": "manual",
                        "drive": "awd"
                    }
                },
                {
                    "utterance": "What are some Honda models with a maximum combined fuel efficiency of 50 mpg?",
                    "parameters": {
                        "make": "Honda",
                        "max_comb_mpg": 50
                    }
                },
                {
                    "utterance": "I need cars with a minimum city fuel efficiency of 20 mpg and at least 4 cylinders.",
                    "parameters": {
                        "min_city_mpg": 20,
                        "cylinders": 4
                    }
                },
                {
                    "utterance": "Show me gas cars from 2018 with four-wheel drive.",
                    "parameters": {
                        "fuel_type": "gas",
                        "year": "2018",
                        "drive": "4wd"
                    }
                }
            ]
        }
    ]
}