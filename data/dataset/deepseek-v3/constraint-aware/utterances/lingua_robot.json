{
    "name": "Lingua Robot",
    "description": "English dictionary API providing an access to data of over 800 000 English lexical entries, such as words, phrasal verbs, multiword expressions etc. Use the API to get word definitions, usage examples, pronunciations, synonyms and antonyms or for text processing (lemmatization, morpheme segmentation, root word extraction, word inflections).",
    "url": "https://rapidapi.com/rokish/api/lingua-robot/",
    "api_methods": [
        {
            "name": "Entry",
            "description": "Retrieve data related to the specified entry, such as word or phrasal verb. The data includes meanings, pronunciations, lemmas, normalized lemmas etc.",
            "url": "https://lingua-robot.p.rapidapi.com/language/v1/entries/en/{entry}",
            "parameters": [
                {
                    "name": "entry",
                    "type": "STRING",
                    "description": "Entry (word, phrasal verb etc., case-sensitive)",
                    "default": "example",
                    "required": true
                }
            ],
            "utterances": [
                {
                    "utterance": "What does the word 'serendipity' mean?",
                    "parameters": {
                        "entry": "serendipity"
                    }
                },
                {
                    "utterance": "Can you give me the pronunciation of 'quintessential'?",
                    "parameters": {
                        "entry": "quintessential"
                    }
                },
                {
                    "utterance": "Tell me the synonyms for 'benevolent'.",
                    "parameters": {
                        "entry": "benevolent"
                    }
                },
                {
                    "utterance": "What are the antonyms of 'ephemeral'?",
                    "parameters": {
                        "entry": "ephemeral"
                    }
                },
                {
                    "utterance": "Provide examples of how to use 'ambivalent' in a sentence.",
                    "parameters": {
                        "entry": "ambivalent"
                    }
                },
                {
                    "utterance": "What is the definition of 'cogent'?",
                    "parameters": {
                        "entry": "cogent"
                    }
                },
                {
                    "utterance": "Can you lemmatize the word 'running'?",
                    "parameters": {
                        "entry": "running"
                    }
                },
                {
                    "utterance": "What is the root word of 'unbelievable'?",
                    "parameters": {
                        "entry": "unbelievable"
                    }
                },
                {
                    "utterance": "Give me the meanings of the phrasal verb 'look up'.",
                    "parameters": {
                        "entry": "look up"
                    }
                },
                {
                    "utterance": "What are the inflections of the word 'go'?",
                    "parameters": {
                        "entry": "go"
                    }
                }
            ]
        }
    ]
}