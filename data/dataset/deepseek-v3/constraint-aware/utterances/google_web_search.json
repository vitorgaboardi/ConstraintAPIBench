{
    "name": "Google Web Search",
    "description": "Google Web Search API. Search the world\u2019s information, including webpages, related keywords and more.",
    "url": "https://rapidapi.com/UnlimitedAPI/api/google-web-search1/",
    "api_methods": [
        {
            "name": "Search",
            "description": "Search",
            "url": "https://google-web-search1.p.rapidapi.com/",
            "parameters": [
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "Search query. You can use Google Search parameters.\n\ne.g. adele site:youtube.com\ne.g. harry potter filetype:pdf\ne.g. inurl:store\ne.g. ronaldo -cristiano",
                    "default": "World Cup",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "related_keywords",
                    "type": "STRING",
                    "description": "Shows related keywords. Default: false\n\nEntering true, increases API latency",
                    "default": "true",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "true",
                                "false"
                            ]
                        }
                    }
                },
                {
                    "name": "limit",
                    "type": "NUMBER",
                    "description": "Max results number. Max recommended value is 300.",
                    "default": "20",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 300
                        },
                        "technical": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Find information about the latest Mars rover mission.",
                    "parameters": {
                        "query": "latest Mars rover mission"
                    }
                },
                {
                    "utterance": "Search for vegan recipes using quinoa.",
                    "parameters": {
                        "query": "vegan recipes using quinoa"
                    }
                },
                {
                    "utterance": "Look up the history of the Eiffel Tower.",
                    "parameters": {
                        "query": "history of the Eiffel Tower"
                    }
                },
                {
                    "utterance": "Can you find PDFs about machine learning?",
                    "parameters": {
                        "query": "machine learning filetype:pdf"
                    }
                },
                {
                    "utterance": "Search for YouTube videos about Python programming.",
                    "parameters": {
                        "query": "Python programming site:youtube.com"
                    }
                },
                {
                    "utterance": "Find articles related to climate change and show related keywords.",
                    "parameters": {
                        "query": "climate change",
                        "related_keywords": "true"
                    }
                },
                {
                    "utterance": "Search for news about the latest iPhone release.",
                    "parameters": {
                        "query": "latest iPhone release"
                    }
                },
                {
                    "utterance": "Find information about the benefits of meditation.",
                    "parameters": {
                        "query": "benefits of meditation"
                    }
                },
                {
                    "utterance": "Can you search for pages with 'store' in the URL?",
                    "parameters": {
                        "query": "inurl:store"
                    }
                },
                {
                    "utterance": "Find information about the Renaissance period and include related keywords.",
                    "parameters": {
                        "query": "Renaissance period",
                        "related_keywords": "true"
                    }
                }
            ]
        }
    ]
}