{
    "name": "Weather by API-Ninjas",
    "description": "Get the latest weather data for any region in the world. See more info at https://api-ninjas.com/api/weather.",
    "url": "https://rapidapi.com/apininjas/api/weather-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/weather",
            "description": "API Ninjas Weather API endpoint.",
            "url": "https://weather-by-api-ninjas.p.rapidapi.com/v1/weather",
            "parameters": [
                {
                    "name": "country",
                    "type": "STRING",
                    "description": "Country name.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lon",
                    "type": "STRING",
                    "description": "Longitude of desired location. If used, **lat** parameter must also be supplied.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "lon, lat: if lon is provided, lat must also be supplied."
                    }
                },
                {
                    "name": "zip",
                    "type": "NUMBER",
                    "description": "5-digit Zip code (United States only).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "state",
                    "type": "STRING",
                    "description": "US state (United States only).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "city",
                    "type": "STRING",
                    "description": "City name.",
                    "default": "Seattle",
                    "required": false
                },
                {
                    "name": "lat",
                    "type": "NUMBER",
                    "description": "Latitude of desired location. If used, **lon** parameter must also be supplied.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "lon, lat: if lat is provided, lon must also be supplied."
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the weather like in New York City?",
                    "parameters": {
                        "city": "New York City"
                    }
                },
                {
                    "utterance": "Can you tell me the weather for Paris, France?",
                    "parameters": {
                        "city": "Paris",
                        "country": "France"
                    }
                },
                {
                    "utterance": "How's the weather in San Francisco, California?",
                    "parameters": {
                        "city": "San Francisco",
                        "state": "California"
                    }
                },
                {
                    "utterance": "What's the current weather at latitude 40.7128 and longitude -74.0060?",
                    "parameters": {
                        "lat": 40.7128,
                        "lon": -74.006
                    }
                },
                {
                    "utterance": "Can I get the weather for zip code 10001?",
                    "parameters": {
                        "zip": 10001
                    }
                },
                {
                    "utterance": "What's the weather like in Tokyo, Japan?",
                    "parameters": {
                        "city": "Tokyo",
                        "country": "Japan"
                    }
                },
                {
                    "utterance": "How's the weather in Chicago, Illinois?",
                    "parameters": {
                        "city": "Chicago",
                        "state": "Illinois"
                    }
                },
                {
                    "utterance": "What's the weather at latitude 34.0522 and longitude -118.2437?",
                    "parameters": {
                        "lat": 34.0522,
                        "lon": -118.2437
                    }
                },
                {
                    "utterance": "Can you tell me the weather for London, UK?",
                    "parameters": {
                        "city": "London",
                        "country": "UK"
                    }
                },
                {
                    "utterance": "What's the current weather in Miami, Florida?",
                    "parameters": {
                        "city": "Miami",
                        "state": "Florida"
                    }
                }
            ]
        }
    ]
}