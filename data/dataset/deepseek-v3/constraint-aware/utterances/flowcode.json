{
    "name": "Flowcode",
    "description": "Check out our new Developer Portal and docs: https://www.flowcode.com/developer-portal",
    "url": "https://rapidapi.com/flowcode/api/flowcode1/",
    "api_methods": [
        {
            "name": "Create QR Code",
            "description": "This API takes an endpoint and return and image for a QR Code in either svg, png, jpg, or pdf format",
            "url": "https://flowcode1.p.rapidapi.com/flowcode",
            "parameters": [
                {
                    "name": "url",
                    "type": "STRING",
                    "description": "This is the URL the QR code will scan to",
                    "default": "https://www.flowcode.com",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "imageType",
                    "type": "STRING",
                    "description": "Response image type:\npng, svg, pdf, or jpg",
                    "default": "svg",
                    "required": true,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "png",
                                "svg",
                                "pdf",
                                "jpg"
                            ]
                        }
                    }
                },
                {
                    "name": "bgColor",
                    "type": "STRING",
                    "description": "Hex background color for flowcode.\nFormat: #rrggbb or transparent for transparent background.",
                    "default": "#cbcbcb",
                    "required": false,
                    "constraints": {
                        "format": "Hex color code (#rrggbb) or 'transparent'"
                    }
                },
                {
                    "name": "fgColor",
                    "type": "STRING",
                    "description": "Hex foreground color for flowcode\nFormat: #rrggbb",
                    "default": "#ff0000",
                    "required": false,
                    "constraints": {
                        "format": "Hex color code (#rrggbb)"
                    }
                },
                {
                    "name": "rimTextTop",
                    "type": "STRING",
                    "description": "For style=rim, the call to action text to put at top of the rim",
                    "default": "Scan Me",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "style",
                    "type": "STRING",
                    "description": "Style of flowcode:\ndefault, inset, or rim",
                    "default": "default",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "default",
                                "inset",
                                "rim"
                            ]
                        }
                    }
                },
                {
                    "name": "moduleShape",
                    "type": "STRING",
                    "description": "Shape to use for code pattern:\ndefault, circle, square, or diamond",
                    "default": "default",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "default",
                                "circle",
                                "square",
                                "diamond"
                            ]
                        }
                    }
                },
                {
                    "name": "rimTextBottom",
                    "type": "STRING",
                    "description": "For style=rim, the call to action text to put at bottom of the rim",
                    "default": "Scan Me",
                    "required": false,
                    "constraints": {}
                }
            ],
            "utterances": [
                {
                    "utterance": "Generate a QR code for my website https://example.com in PNG format.",
                    "parameters": {
                        "url": "https://example.com",
                        "imageType": "png"
                    }
                },
                {
                    "utterance": "Create a QR code for https://myblog.com in SVG format with a transparent background.",
                    "parameters": {
                        "url": "https://myblog.com",
                        "imageType": "svg",
                        "bgColor": "transparent"
                    }
                },
                {
                    "utterance": "I need a QR code for https://store.com in JPG format with a blue background and red foreground.",
                    "parameters": {
                        "url": "https://store.com",
                        "imageType": "jpg",
                        "bgColor": "#0000ff",
                        "fgColor": "#ff0000"
                    }
                },
                {
                    "utterance": "Can you make a QR code for https://event.com in PDF format with a rim style and 'Scan Here' at the top?",
                    "parameters": {
                        "url": "https://event.com",
                        "imageType": "pdf",
                        "style": "rim",
                        "rimTextTop": "Scan Here"
                    }
                },
                {
                    "utterance": "Generate a QR code for https://portfolio.com in SVG format with a circle module shape.",
                    "parameters": {
                        "url": "https://portfolio.com",
                        "imageType": "svg",
                        "moduleShape": "circle"
                    }
                },
                {
                    "utterance": "Create a QR code for https://booking.com in PNG format with an inset style and a green background.",
                    "parameters": {
                        "url": "https://booking.com",
                        "imageType": "png",
                        "style": "inset",
                        "bgColor": "#00ff00"
                    }
                },
                {
                    "utterance": "I need a QR code for https://news.com in JPG format with a diamond module shape and 'Click Me' at the bottom.",
                    "parameters": {
                        "url": "https://news.com",
                        "imageType": "jpg",
                        "moduleShape": "diamond",
                        "rimTextBottom": "Click Me"
                    }
                },
                {
                    "utterance": "Generate a QR code for https://travel.com in PDF format with a rim style, 'Scan Now' at the top, and 'Visit Us' at the bottom.",
                    "parameters": {
                        "url": "https://travel.com",
                        "imageType": "pdf",
                        "style": "rim",
                        "rimTextTop": "Scan Now",
                        "rimTextBottom": "Visit Us"
                    }
                },
                {
                    "utterance": "Create a QR code for https://music.com in SVG format with a square module shape and a purple foreground.",
                    "parameters": {
                        "url": "https://music.com",
                        "imageType": "svg",
                        "moduleShape": "square",
                        "fgColor": "#800080"
                    }
                },
                {
                    "utterance": "Can you generate a QR code for https://fitness.com in PNG format with a default style and a yellow background?",
                    "parameters": {
                        "url": "https://fitness.com",
                        "imageType": "png",
                        "style": "default",
                        "bgColor": "#ffff00"
                    }
                }
            ]
        }
    ]
}