{
    "name": "Nutrition by API-Ninjas",
    "description": "Natural language API to extract nutrition data from any text. See more info at https://api-ninjas.com/api/nutrition.",
    "url": "https://rapidapi.com/apininjas/api/nutrition-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/nutrition",
            "description": "API Ninjas Nutrition API endpoint.",
            "url": "https://nutrition-by-api-ninjas.p.rapidapi.com/v1/nutrition",
            "parameters": [
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "Query text to extract nutrition information (e.g. **bacon and 3 eggs**).",
                    "default": "1lb brisket with fries",
                    "required": true
                }
            ],
            "utterances": [
                {
                    "utterance": "What are the nutritional facts for a chicken Caesar salad?",
                    "parameters": {
                        "query": "chicken Caesar salad"
                    }
                },
                {
                    "utterance": "Can you tell me the nutrition details of a bowl of oatmeal with blueberries?",
                    "parameters": {
                        "query": "bowl of oatmeal with blueberries"
                    }
                },
                {
                    "utterance": "I need the nutritional information for a grilled cheese sandwich.",
                    "parameters": {
                        "query": "grilled cheese sandwich"
                    }
                },
                {
                    "utterance": "What's the calorie count for a plate of spaghetti carbonara?",
                    "parameters": {
                        "query": "plate of spaghetti carbonara"
                    }
                },
                {
                    "utterance": "Could you provide the nutrition breakdown for a smoothie with banana and spinach?",
                    "parameters": {
                        "query": "smoothie with banana and spinach"
                    }
                },
                {
                    "utterance": "How many carbs are in a serving of quinoa salad?",
                    "parameters": {
                        "query": "serving of quinoa salad"
                    }
                },
                {
                    "utterance": "What\u2019s the protein content in a steak with mashed potatoes?",
                    "parameters": {
                        "query": "steak with mashed potatoes"
                    }
                },
                {
                    "utterance": "Can I get the nutritional values for a veggie burger with avocado?",
                    "parameters": {
                        "query": "veggie burger with avocado"
                    }
                },
                {
                    "utterance": "Tell me the fat content in a slice of pepperoni pizza.",
                    "parameters": {
                        "query": "slice of pepperoni pizza"
                    }
                },
                {
                    "utterance": "What are the nutritional details for a bowl of chicken noodle soup?",
                    "parameters": {
                        "query": "bowl of chicken noodle soup"
                    }
                }
            ]
        }
    ]
}