{
    "name": "Text-to-Speech",
    "description": "The Voice RSS Text-to-Speech (TTS) API allows conversion of textual content to speech easier than ever. Just connect to our Text-to-Speech (TTS) API with a few lines of code and get verbal representation of a textual content.",
    "url": "https://rapidapi.com/voicerss/api/text-to-speech-1/",
    "api_methods": [
        {
            "name": "Convert text to speech via HTTP POST",
            "description": "Converts textual content to audio content",
            "url": "https://voicerss-text-to-speech.p.rapidapi.com/",
            "parameters": [
                {
                    "name": "src",
                    "type": "STRING",
                    "description": "The textual content for converting to speech",
                    "default": "Hello, world!",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "hl",
                    "type": "STRING",
                    "description": "The textual content language",
                    "default": "en-us",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "r",
                    "type": "NUMBER",
                    "description": "The speech rate (speed). Allows values: from -10 (slowest speed) up to 10 (fastest speed). Default value: 0 (normal speed)",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 10,
                            "min": -10
                        }
                    }
                },
                {
                    "name": "c",
                    "type": "STRING",
                    "description": "The speech audio codec",
                    "default": "mp3",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "f",
                    "type": "STRING",
                    "description": "The speech audio formats",
                    "default": "8khz_8bit_mono",
                    "required": false,
                    "constraints": {}
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you convert 'The quick brown fox jumps over the lazy dog' to speech in English?",
                    "parameters": {
                        "src": "The quick brown fox jumps over the lazy dog",
                        "hl": "en-us"
                    }
                },
                {
                    "utterance": "Please turn 'Good morning, how are you?' into audio in Spanish.",
                    "parameters": {
                        "src": "Good morning, how are you?",
                        "hl": "es-es"
                    }
                },
                {
                    "utterance": "Convert 'Welcome to the conference' to speech in French with a slow speed.",
                    "parameters": {
                        "src": "Welcome to the conference",
                        "hl": "fr-fr",
                        "r": -5
                    }
                },
                {
                    "utterance": "Turn 'Happy birthday to you' into audio in German using the MP3 codec.",
                    "parameters": {
                        "src": "Happy birthday to you",
                        "hl": "de-de",
                        "c": "mp3"
                    }
                },
                {
                    "utterance": "Can you convert 'Thank you for your patience' to speech in Italian with a fast speed?",
                    "parameters": {
                        "src": "Thank you for your patience",
                        "hl": "it-it",
                        "r": 8
                    }
                },
                {
                    "utterance": "Please turn 'The meeting starts at 10 AM' into audio in Portuguese with a normal speed.",
                    "parameters": {
                        "src": "The meeting starts at 10 AM",
                        "hl": "pt-pt",
                        "r": 0
                    }
                },
                {
                    "utterance": "Convert 'Please stand by' to speech in Japanese using the WAV format.",
                    "parameters": {
                        "src": "Please stand by",
                        "hl": "ja-jp",
                        "f": "wav"
                    }
                },
                {
                    "utterance": "Turn 'We are experiencing technical difficulties' into audio in Russian with a slow speed and MP3 codec.",
                    "parameters": {
                        "src": "We are experiencing technical difficulties",
                        "hl": "ru-ru",
                        "r": -3,
                        "c": "mp3"
                    }
                },
                {
                    "utterance": "Can you convert 'Your call is important to us' to speech in Chinese with a fast speed and 16kHz format?",
                    "parameters": {
                        "src": "Your call is important to us",
                        "hl": "zh-cn",
                        "r": 7,
                        "f": "16khz_16bit_mono"
                    }
                },
                {
                    "utterance": "Please turn 'Please leave a message after the beep' into audio in Korean with a normal speed and MP3 codec.",
                    "parameters": {
                        "src": "Please leave a message after the beep",
                        "hl": "ko-kr",
                        "r": 0,
                        "c": "mp3"
                    }
                }
            ]
        }
    ]
}