{
    "name": "Domainr",
    "description": "Instant domain search.\r\n\r\nDocumentation available at https://domainr.com/docs/api",
    "url": "https://rapidapi.com/domainr/api/domainr/",
    "api_methods": [
        {
            "name": "/v2/register",
            "description": "Responds with an HTTP redirect to a supporting registrar.",
            "url": "https://domainr.p.rapidapi.com/v2/register",
            "parameters": [
                {
                    "name": "domain",
                    "type": "STRING",
                    "description": "Domain to register.",
                    "default": "example.com",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "registrar",
                    "type": "STRING",
                    "description": "The registrar's root domain.",
                    "default": "namecheap.com",
                    "required": false,
                    "constraints": {}
                }
            ],
            "utterances": [
                {
                    "utterance": "How can I register the domain mybusiness.com?",
                    "parameters": {
                        "domain": "mybusiness.com"
                    }
                },
                {
                    "utterance": "I want to register the domain techinnovations.org.",
                    "parameters": {
                        "domain": "techinnovations.org"
                    }
                },
                {
                    "utterance": "Can you help me register the domain greenearth.net?",
                    "parameters": {
                        "domain": "greenearth.net"
                    }
                },
                {
                    "utterance": "Register the domain creativestudio.io for me.",
                    "parameters": {
                        "domain": "creativestudio.io"
                    }
                },
                {
                    "utterance": "I need to register the domain healthyliving.co.",
                    "parameters": {
                        "domain": "healthyliving.co"
                    }
                },
                {
                    "utterance": "How do I register the domain traveladventures.com with Namecheap?",
                    "parameters": {
                        "domain": "traveladventures.com",
                        "registrar": "namecheap.com"
                    }
                },
                {
                    "utterance": "Please register the domain smarttech.ai using GoDaddy.",
                    "parameters": {
                        "domain": "smarttech.ai",
                        "registrar": "godaddy.com"
                    }
                },
                {
                    "utterance": "I\u2019d like to register the domain fashiontrends.biz with Google Domains.",
                    "parameters": {
                        "domain": "fashiontrends.biz",
                        "registrar": "domains.google"
                    }
                },
                {
                    "utterance": "Can you register the domain ecofriendly.earth for me?",
                    "parameters": {
                        "domain": "ecofriendly.earth"
                    }
                },
                {
                    "utterance": "Register the domain futuretech.xyz with Bluehost.",
                    "parameters": {
                        "domain": "futuretech.xyz",
                        "registrar": "bluehost.com"
                    }
                }
            ]
        }
    ]
}