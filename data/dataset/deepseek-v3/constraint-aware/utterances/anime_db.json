{
    "name": "Anime DB",
    "description": "Complete anime data. Ranking, synopsis, genre, search by title. Over 20k data updated everyday",
    "url": "https://rapidapi.com/brian.rofiq/api/anime-db/",
    "api_methods": [
        {
            "name": "Get All",
            "description": "Get List of anime. You can search, sort an specify genre",
            "url": "https://anime-db.p.rapidapi.com/anime",
            "parameters": [
                {
                    "name": "page",
                    "type": "STRING",
                    "description": "",
                    "default": "1",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "size",
                    "type": "STRING",
                    "description": "",
                    "default": "10",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "types",
                    "type": "STRING",
                    "description": "Anime type, separated by comma",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "genres",
                    "type": "STRING",
                    "description": "genres separated by comma",
                    "default": "Fantasy,Drama",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "sortBy",
                    "type": "STRING",
                    "description": "ranking or title",
                    "default": "ranking",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "ranking",
                                "title"
                            ]
                        }
                    }
                },
                {
                    "name": "sortOrder",
                    "type": "STRING",
                    "description": "asc or desc",
                    "default": "asc",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "asc",
                                "desc"
                            ]
                        }
                    }
                },
                {
                    "name": "search",
                    "type": "STRING",
                    "description": "Search by title or alternative titles. Search will ignore sort",
                    "default": "Fullmetal",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "search, sortBy, sortOrder: If search is provided, sortBy and sortOrder are ignored."
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me anime sorted by title in ascending order?",
                    "parameters": {
                        "sortBy": "title",
                        "sortOrder": "asc"
                    }
                },
                {
                    "utterance": "Find me anime with genres like Fantasy and Drama.",
                    "parameters": {
                        "genres": "Fantasy,Drama"
                    }
                },
                {
                    "utterance": "Search for anime titled 'Naruto'.",
                    "parameters": {
                        "search": "Naruto"
                    }
                },
                {
