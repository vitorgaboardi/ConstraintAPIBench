{
    "name": "Tasty",
    "description": "API to query data about recipe, plan, ingredients, etc... as on official site",
    "url": "https://rapidapi.com/apidojo/api/tasty/",
    "api_methods": [
        {
            "name": "recipes/list",
            "description": "List recipes by option filters or name",
            "url": "https://tasty.p.rapidapi.com/recipes/list",
            "parameters": [
                {
                    "name": "size",
                    "type": "NUMBER",
                    "description": "Number of items returned per response",
                    "default": "20",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "The offset of items to be ignored in response for paging",
                    "default": "0",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "Leave empty to sort by popular as default OR one of the following : approved_at:desc|approved_at:asc",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "approved_at:desc",
                                "approved_at:asc"
                            ]
                        }
                    }
                },
                {
                    "name": "q",
                    "type": "STRING",
                    "description": "Name of food or, ingredients to search by",
                    "default": "",
                    "required": false
                },
                {
                    "name": "tags",
                    "type": "STRING",
                    "description": "Get suitable values from /tags/list API",
                    "default": "under_30_minutes",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me some quick recipes that take less than 30 minutes to prepare?",
                    "parameters": {
                        "tags": "under_30_minutes"
                    }
                },
                {
                    "utterance": "What are the most recently approved recipes?",
                    "parameters": {
                        "sort": "approved_at:desc"
                    }
                },
                {
                    "utterance": "Find me recipes that include chicken and broccoli.",
                    "parameters": {
                        "q": "chicken broccoli"
                    }
                },
                {
                    "utterance": "Show me the oldest approved recipes.",
                    "parameters": {
                        "sort": "approved_at:asc"
                    }
                },
                {
                    "utterance": "I need some dessert recipes that are quick to make.",
                    "parameters": {
                        "q": "dessert",
                        "tags": "under_30_minutes"
                    }
                },
                {
                    "utterance": "What are some popular recipes for a vegetarian diet?",
                    "parameters": {
                        "q": "vegetarian"
                    }
                },
                {
                    "utterance": "Can you list recipes that are both quick and include pasta?",
                    "parameters": {
                        "q": "pasta",
                        "tags": "under_30_minutes"
                    }
                },
                {
                    "utterance": "Find me the latest approved recipes for breakfast.",
                    "parameters": {
                        "q": "breakfast",
                        "sort": "approved_at:desc"
                    }
                },
                {
                    "utterance": "Show me some easy-to-make recipes with beef.",
                    "parameters": {
                        "q": "beef",
                        "tags": "under_30_minutes"
                    }
                },
                {
                    "utterance": "What are the newest approved recipes that include seafood?",
                    "parameters": {
                        "q": "seafood",
                        "sort": "approved_at:asc"
                    }
                }
            ]
        },
