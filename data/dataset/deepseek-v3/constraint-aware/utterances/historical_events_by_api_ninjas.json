{
    "name": "Historical Events by API-Ninjas",
    "description": "Search through the most famous events in history. See more info at https://api-ninjas.com/api/historicalevents.",
    "url": "https://rapidapi.com/apininjas/api/historical-events-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/historicalevents",
            "description": "API Ninjas Historical Events API endpoint. Returns a list of up to 10 events that match the search parameters. At least one of the following parameters is required: text, year, month, day.",
            "url": "https://historical-events-by-api-ninjas.p.rapidapi.com/v1/historicalevents",
            "parameters": [
                {
                    "name": "text",
                    "type": "STRING",
                    "description": "query text to search events by. Use keywords or short phrases for best match results.",
                    "default": "roman empire",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "month",
                    "type": "NUMBER",
                    "description": "integer month (e.g. 3 for March).",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "min": 1,
                            "max": 12
                        }
                    }
                },
                {
                    "name": "day",
                    "type": "NUMBER",
                    "description": "calendar day of the month.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "min": 1,
                            "max": 31
                        }
                    }
                },
                {
                    "name": "year",
                    "type": "NUMBER",
                    "description": "4-digit year (e.g. 1776). For BC/BCE years, use a negative integer (e.g. -351 for 351 BC).",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "offset",
                    "type": "NUMBER",
                    "description": "number of results to offset (for pagination).",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "What historical events happened in July 1969?",
                    "parameters": {
                        "month": 7,
                        "year": 1969
                    }
                },
                {
                    "utterance": "Can you find events related to the American Revolution?",
                    "parameters": {
                        "text": "American Revolution"
                    }
                },
                {
                    "utterance": "Tell me about events that occurred on December 7th.",
                    "parameters": {
                        "month": 12,
                        "day": 7
                    }
                },
                {
                    "utterance": "What happened in the year 1066?",
                    "parameters": {
                        "year": 1066
                    }
                },
                {
                    "utterance": "Find events related to the fall of the Berlin Wall.",
                    "parameters": {
                        "text": "fall of the Berlin Wall"
                    }
                },
                {
                    "utterance": "What historical events took place on January 1st?",
                    "parameters": {
                        "month": 1,
                        "day": 1
                    }
                },
                {
                    "utterance": "Can you tell me about events in 1492?",
                    "parameters": {
                        "year": 1492
                    }
                },
                {
                    "utterance": "What happened in March 1917?",
                    "parameters": {
                        "month": 3,
                        "year": 1917
                    }
                },
                {
                    "utterance": "Find events related to the Renaissance period.",
                    "parameters": {
                        "text": "Renaissance period"
                    }
                },
                {
                    "utterance": "What events occurred on October 31st, 1517?",
                    "parameters": {
                        "month": 10,
                        "day": 31,
                        "year": 1517
                    }
                }
            ]
        }
    ]
}