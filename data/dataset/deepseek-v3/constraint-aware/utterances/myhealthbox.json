        {
            "name": "Updated Documents",
            "description": "Search documents that have been updated after a certain date or within a date interval",
            "url": "https://myhealthbox.p.rapidapi.com/search/updatedDocuments",
            "parameters": [
                {
                    "name": "sd",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "",
                    "default": "2020-06-01",
                    "required": true,
                    "constraints": {
                        "format": "DATE (YYYY-MM-DD)"
                    }
                },
                {
                    "name": "t",
                    "type": "ENUM",
                    "description": "Document type, one of:  \npil (Patient Information Leaflet), spc (Summary of Product Characteristics), par (Public Assessment Report), sds (Safety Data Sheet), man (User Manual), inf (Product Information), mmr (Additional Risk Minimisation Measures), hta (Health Technology Assessment Report)",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "pil",
                                "spc",
                                "par",
                                "sds",
                                "man",
                                "inf",
                                "mmr",
                                "hta"
                            ]
                        }
                    }
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "ed",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "limit",
                    "type": "NUMBER",
                    "description": "Default value: 32\nMax value: 100",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 100
                        },
                        "technical": true
                    }
                },
                {
                    "name": "c",
                    "type": "STRING",
                    "description": "See the Supported countries section in the Developers Manual",
                    "default": "us",
                    "required": false,
                    "constraints": {
                        "format": "Supported countries"
                    }
                },
                {
                    "name": "f",
                    "type": "ENUM",
                    "description": "Field optional parameter, if present it can only have 1 value. Perform date comparison on this attribute (date when record was updated in the myHealthbox database) instead of the default one (date when the actual document was updated).",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "date when record was updated in the myHealthbox database"
                            ]
                        }
                    }
                },
                {
                    "name": "l",
                    "type": "STRING",
                    "description": "See the Supported countries section in the Developers Manual.",
                    "default": "en",
                    "required": false,
                    "constraints": {
                        "format": "Supported languages"
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me documents updated since June 1, 2020?",
                    "parameters": {
                        "sd": "2020-06-01"
                    }
                },
                {
                    "utterance": "Find Patient Information Leaflets updated after June 1, 2020.",
                    "parameters": {
                        "sd": "2020-06-01",
                        "t": "pil"
                    }
                },
                {
                    "utterance": "Search for documents updated between June 1, 2020, and December 31, 2020.",
