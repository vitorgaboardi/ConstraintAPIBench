{
    "name": "MoviesDatabase",
    "description": "MoviesDatabase provides complete and updated data for over 9 million titles ( movies, series and episodes) and 11 million actors / crew and cast members",
    "url": "https://rapidapi.com/SAdrian/api/moviesdatabase/",
    "api_methods": [
        {
            "name": "/titles/x/upcoming",
            "description": " ",
            "url": "https://moviesdatabase.p.rapidapi.com/titles/x/upcoming",
            "parameters": [
                {
                    "name": "titleType",
                    "type": "STRING",
                    "description": "Filter by type of title",
                    "default": "",
                    "required": false
                },
                {
                    "name": "year",
                    "type": "NUMBER",
                    "description": "Year filter ex: ?year=2022",
                    "default": "",
                    "required": false
                },
                {
                    "name": "genre",
                    "type": "STRING",
                    "description": "Year filter ex: ?genre=Drama",
                    "default": "",
                    "required": false
                },
                {
                    "name": "info",
                    "type": "STRING",
                    "description": "Info type structure (default: mini-info) -> base_info / mini_info / image / ...",
                    "default": "",
                    "required": false
                },
                {
                    "name": "endYear",
                    "type": "NUMBER",
                    "description": "Year range filter -to- ex: ?endYear=2020",
                    "default": "",
                    "required": false
                },
                {
                    "name": "limit",
                    "type": "NUMBER",
                    "description": "Number of titles per page (default: 10) -> 10 max ",
                    "default": "",
                    "required": false
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "Add sorting (incr, decr) -> year.incr / year.decr",
                    "default": "",
                    "required": false
                },
                {
                    "name": "page",
                    "type": "STRING",
                    "description": "Page number",
                    "default": "",
                    "required": false
                },
                {
                    "name": "startYear",
                    "type": "NUMBER",
                    "description": "Year range filter -from- ex: ?startYear=2020",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me upcoming drama movies for 2023.",
                    "parameters": {
                        "titleType": "movie",
                        "year": 2023,
                        "genre": "Drama"
                    }
                },
                {
                    "utterance": "List the upcoming series in 2024 with mini-info.",
                    "parameters": {
                        "titleType": "series",
                        "year": 2024,
                        "info": "mini-info"
                    }
                },
                {
                    "utterance": "What are the upcoming action movies sorted by year in descending order?",
                    "parameters": {
                        "titleType": "movie",
                        "genre": "Action",
                        "sort": "year.decr"
                    }
                },
                {
                    "utterance": "Find upcoming titles from 2022 to 2025 with detailed base info.",
                    "parameters": {
                        "startYear": 2022,
                        "endYear": 2025,
                        "info": "base_info"
                    }
                },
                {
                    "utterance": "Give me a list of upcoming comedy series with images.",
                    "parameters": {
                        "titleType": "series",
                        "genre": "Comedy",
                        "info": "image"
                    }
                },
                {
                    "utterance": "Show the first 5 upcoming horror movies.",
                    "parameters": {
                        "titleType": "movie",
                        "genre": "Horror",
                        "limit": 5
                    }
                },
                {
                    "utterance": "What are the upcoming titles for 2023 on page 2?",
                    "parameters": {
                        "year": 2023,
                        "page": "2"
                    }
                },
                {
                    "utterance": "List upcoming documentaries sorted by year in ascending order.",
                    "parameters": {
                        "titleType": "documentary",
                        "sort": "year.incr"
                    }
                },
                {
                    "utterance": "Find upcoming animated movies for kids.",
                    "parameters": {
                        "titleType": "movie",
                        "genre": "Animation"
                    }
                },
                {
                    "utterance": "Show me upcoming titles from 2021 to 2023 with a limit of 10 per page.",
                    "parameters": {
                        "startYear": 2021,
                        "endYear": 2023,
                        "limit": 10
                    }
                }
            ]
        }
    ]
}