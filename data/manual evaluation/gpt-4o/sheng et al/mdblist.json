{
    "name": "MDBList",
    "description": "Get Movies and TV Shows data with ratings from multiple providers in JSON format",
    "url": "https://rapidapi.com/linaspurinis/api/mdblist/",
    "api_methods": [
        {
            "name": "Search by Title",
            "description": "Search for Movies or TV Shows by title, include option year or mediatype filter to narrow your results",
            "url": "https://mdblist.p.rapidapi.com/",
            "parameters": [
                {
                    "name": "s",
                    "type": "STRING",
                    "description": "Movie or TV Show Title to search",
                    "default": "jaws",
                    "required": true
                },
                {
                    "name": "l",
                    "type": "NUMBER",
                    "description": "Limit returned items.\nYou can use numbes from 1 to 100 (default is 50)",
                    "default": "",
                    "required": false
                },
                {
                    "name": "y",
                    "type": "NUMBER",
                    "description": "Year (format YYYY, for example 2020) to limit title search (would also include previous and next year movies)",
                    "default": "",
                    "required": false
                },
                {
                    "name": "m",
                    "type": "ENUM",
                    "description": "Search for only movie or tv show, leave empty for both",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Find information about the movie 'Inception'.",
                    "parameters": {
                        "s": "Inception"
                    }
                },
                {
                    "utterance": "Search for TV shows titled 'Friends' with a limit of 10 results.",
                    "parameters": {
                        "s": "Friends",
                        "l": 10
                    }
                },
                {
                    "utterance": "Get details on the movie 'Titanic' released around 1997.",
                    "parameters": {
                        "s": "Titanic",
                        "y": 1997
                    }
                },
                {
                    "utterance": "Look up the TV show 'Breaking Bad' with a maximum of 5 entries.",
                    "parameters": {
                        "s": "Breaking Bad",
                        "l": 5
                    }
                },
                {
                    "utterance": "Find movies titled 'The Matrix' from the year 1999.",
                    "parameters": {
                        "s": "The Matrix",
                        "y": 1999,
                        "m": "movie"
                    }
                },
                {
                    "utterance": "Search for the TV show 'The Office' with results limited to 20.",
                    "parameters": {
                        "s": "The Office",
                        "l": 20,
                        "m": "tv show"
                    }
                },
                {
                    "utterance": "Get information on 'Avatar' movies.",
                    "parameters": {
                        "s": "Avatar",
                        "m": "movie"
                    }
                },
                {
                    "utterance": "Find TV shows named 'Sherlock' from 2010.",
                    "parameters": {
                        "s": "Sherlock",
                        "y": 2010,
                        "m": "tv show"
                    }
                },
                {
                    "utterance": "Look up the movie 'Gladiator' with a limit of 15 results.",
                    "parameters": {
                        "s": "Gladiator",
                        "l": 15,
                        "m": "movie"
                    }
                },
                {
                    "utterance": "Search for any media titled 'Star Wars' with a focus on the year 1977.",
                    "parameters": {
                        "s": "Star Wars",
                        "y": 1977
                    }
                }
            ]
        }
    ]
}