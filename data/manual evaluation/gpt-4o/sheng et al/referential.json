{
    "name": "Referential",
    "description": "The fastest API to access countries, states, cities, continents, dial and zip codes in up to 20 languages. A collection of data APIs to support forms, signups, search and lookup. Our endpoints are optimized for speed and size. Our data is regularly maintained and comes from International Standardization bodies, the United Nations, government surveys and GIS datasets. We do not scrape WikiPedia etc",
    "url": "https://rapidapi.com/referential/api/referential/",
    "api_methods": [
        {
            "name": "Country by iso code",
            "description": "Get country by 2 letter iso code localized to a given language",
            "url": "https://referential.p.rapidapi.com/v1/country/{iso_code}",
            "parameters": [
                {
                    "name": "iso_code",
                    "type": "string",
                    "description": "2 letter iso country code. ",
                    "default": "US",
                    "required": true
                },
                {
                    "name": "fields",
                    "type": "STRING",
                    "description": "Comma deliminated list of fields to return in response",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "2 letter iso language code to localize country name",
                    "default": "en",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What is the country name for the ISO code 'FR' in French?",
                    "parameters": {
                        "iso_code": "FR",
                        "lang": "fr"
                    }
                },
                {
                    "utterance": "Can you provide details for the country with ISO code 'JP'?",
                    "parameters": {
                        "iso_code": "JP"
                    }
                },
                {
                    "utterance": "Get the country information for 'DE' in German.",
                    "parameters": {
                        "iso_code": "DE",
                        "lang": "de"
                    }
                },
                {
                    "utterance": "What is the localized name of the country with ISO code 'ES' in Spanish?",
                    "parameters": {
                        "iso_code": "ES",
                        "lang": "es"
                    }
                },
                {
                    "utterance": "Fetch the country data for 'IT' with fields 'name, population'.",
                    "parameters": {
                        "iso_code": "IT",
                        "fields": "name, population"
                    }
                },
                {
                    "utterance": "Retrieve the country name for ISO code 'CN' in Chinese.",
                    "parameters": {
                        "iso_code": "CN",
                        "lang": "zh"
                    }
                },
                {
                    "utterance": "What is the country name for 'BR' in Portuguese?",
                    "parameters": {
                        "iso_code": "BR",
                        "lang": "pt"
                    }
                },
                {
                    "utterance": "Get the country details for 'IN' with fields 'name, capital'.",
                    "parameters": {
                        "iso_code": "IN",
                        "fields": "name, capital"
                    }
                },
                {
                    "utterance": "Provide the country information for 'RU' in Russian.",
                    "parameters": {
                        "iso_code": "RU",
                        "lang": "ru"
                    }
                },
                {
                    "utterance": "What is the localized name of the country with ISO code 'KR' in Korean?",
                    "parameters": {
                        "iso_code": "KR",
                        "lang": "ko"
                    }
                }
            ]
        },
        {
            "name": "Languages",
            "description": "List the world's languages translated into 20 languages",
            "url": "https://referential.p.rapidapi.com/v1/lang",
            "parameters": [
                {
                    "name": "lang_3",
                    "type": "STRING",
                    "description": "Filter by 3 letter ISO language code",
                    "default": "",
                    "required": false
                },
                {
                    "name": "iso_a2",
                    "type": "STRING",
                    "description": "Filter on 2 letter country code",
                    "default": "",
                    "required": false
                },
                {
                    "name": "fields",
                    "type": "STRING",
                    "description": "Select fields to return in result [flag,iso_a2,lang_3,lang]",
                    "default": "iso_a2,lang_3,flag",
                    "required": false
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "Returns a language translated into lang language",
                    "default": "en",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you list all languages in French?",
                    "parameters": {
                        "lang": "fr"
                    }
                },
                {
                    "utterance": "Show me the languages spoken in Germany.",
                    "parameters": {
                        "iso_a2": "DE"
                    }
                },
                {
                    "utterance": "What are the languages available in Spanish?",
                    "parameters": {
                        "lang": "es"
                    }
                },
                {
                    "utterance": "List the languages with their 3-letter ISO codes.",
                    "parameters": {
                        "fields": "lang_3,lang"
                    }
                },
                {
                    "utterance": "Provide the list of languages in Japanese.",
                    "parameters": {
                        "lang": "ja"
                    }
                },
                {
                    "utterance": "Can you give me the languages spoken in Canada?",
                    "parameters": {
                        "iso_a2": "CA"
                    }
                },
                {
                    "utterance": "Show languages with their flags and 2-letter country codes.",
                    "parameters": {
                        "fields": "flag,iso_a2,lang"
                    }
                },
                {
                    "utterance": "List all languages in Italian.",
                    "parameters": {
                        "lang": "it"
                    }
                },
                {
                    "utterance": "What languages are spoken in Brazil?",
                    "parameters": {
                        "iso_a2": "BR"
                    }
                },
                {
                    "utterance": "Can you list the languages in Russian?",
                    "parameters": {
                        "lang": "ru"
                    }
                }
            ]
        }
    ]
}