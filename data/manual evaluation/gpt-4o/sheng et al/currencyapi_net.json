{
    "name": "CurrencyAPI.net",
    "description": "Real-time currency conversion on 152 currencies and cryptos",
    "url": "https://rapidapi.com/currencyapinet/api/currencyapi-net/",
    "api_methods": [
        {
            "name": "Timeframe",
            "description": "Display a currency conversion's historical rates within a given timeframe",
            "url": "https://currencyapi-net.p.rapidapi.com/timeframe",
            "parameters": [
                {
                    "name": "end_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-29",
                    "default": "",
                    "required": true
                },
                {
                    "name": "base",
                    "type": "STRING",
                    "description": "The currency code you want to get the historical data of. Eg, USD",
                    "default": "USD",
                    "required": true
                },
                {
                    "name": "start_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-25",
                    "default": "",
                    "required": true
                },
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me the historical exchange rates for EUR from January 1, 2023 to January 31, 2023.",
                    "parameters": {
                        "base": "EUR",
                        "start_date": "2023-01-01",
                        "end_date": "2023-01-31"
                    }
                },
                {
                    "utterance": "I need the historical currency data for GBP from March 1, 2022 to March 15, 2022.",
                    "parameters": {
                        "base": "GBP",
                        "start_date": "2022-03-01",
                        "end_date": "2022-03-15"
                    }
                },
                {
                    "utterance": "Can you provide the historical rates for JPY between February 1, 2021 and February 28, 2021?",
                    "parameters": {
                        "base": "JPY",
                        "start_date": "2021-02-01",
                        "end_date": "2021-02-28"
                    }
                },
                {
                    "utterance": "Fetch the historical conversion rates for AUD from April 10, 2020 to April 20, 2020.",
                    "parameters": {
                        "base": "AUD",
                        "start_date": "2020-04-10",
                        "end_date": "2020-04-20"
                    }
                },
                {
                    "utterance": "Get me the historical data for CAD from May 5, 2019 to May 25, 2019.",
                    "parameters": {
                        "base": "CAD",
                        "start_date": "2019-05-05",
                        "end_date": "2019-05-25"
                    }
                },
                {
                    "utterance": "I want to see the historical exchange rates for CHF from June 1, 2018 to June 30, 2018.",
                    "parameters": {
                        "base": "CHF",
                        "start_date": "2018-06-01",
