{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "url": "https://rapidapi.com/foreca-ltd-foreca-ltd-default/api/foreca-weather/",
    "api_methods": [
        {
            "name": "Hourly",
            "description": "Hourly forecast.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/hourly/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "history",
                    "type": "BOOLEAN",
                    "description": "Whether to include 24 hours of past data.",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tz",
                    "type": "STRING",
                    "description": "Time zone in response (IANA time zone database names)",
                    "default": "Europe/London",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the hourly weather forecast for New York City?",
                    "parameters": {
                        "location": "New York City"
                    }
                },
                {
                    "utterance": "Can you provide the hourly weather forecast for Los Angeles with wind speed in miles per hour?",
                    "parameters": {
                        "location": "Los Angeles",
                        "windunit": "MPH"
                    }
                },
                {
                    "utterance": "Give me the hourly weather forecast for Chicago with temperature in Fahrenheit.",
                    "parameters": {
                        "location": "Chicago",
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "What's the weather forecast for the next 12 hours in Miami?",
                    "parameters": {
                        "location": "Miami",
                        "periods": 12
                    }
                },
                {
                    "utterance": "Show me the hourly weather forecast for Denver including past 24 hours of data.",
                    "parameters": {
                        "location": "Denver",
                        "history": true
                    }
                },
                {
                    "utterance": "What's the hourly forecast for Seattle at an altitude of 100 meters?",
                    "parameters": {
                        "location": "Seattle",
                        "alt": 100
                    }
                },
                {
                    "utterance": "Can I get the hourly weather forecast for Boston in the Eastern time zone?",
                    "parameters": {
                        "location": "Boston",
                        "tz": "America/New_York"
                    }
                },
                {
                    "utterance": "Provide the hourly weather forecast for San Francisco using the basic dataset.",
                    "parameters": {
                        "location": "San Francisco",
                        "dataset": "basic"
                    }
                },
                {
                    "utterance": "What's the hourly weather forecast for Austin with wind speed in kilometers per hour?",
                    "parameters": {
                        "location": "Austin",
                        "windunit": "KMH"
                    }
                },
                {
                    "utterance": "Give me the hourly weather forecast for Las Vegas with temperature in Celsius.",
                    "parameters": {
                        "location": "Las Vegas",
                        "tempunit": "C"
                    }
                }
            ]
        }
    ]
}