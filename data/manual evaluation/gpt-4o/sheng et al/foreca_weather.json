{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "url": "https://rapidapi.com/foreca-ltd-foreca-ltd-default/api/foreca-weather/",
    "api_methods": [
        {
            "name": "Daily",
            "description": "Daily forecast.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/daily/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the daily weather forecast for New York City?",
                    "parameters": {
                        "location": "New York City"
                    }
                },
                {
                    "utterance": "Can you provide the weather forecast for Los Angeles with temperatures in Fahrenheit?",
                    "parameters": {
                        "location": "Los Angeles",
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "Give me the weather forecast for Denver at an altitude of 1600 meters.",
                    "parameters": {
                        "location": "Denver",
                        "alt": 1600
                    }
                },
                {
                    "utterance": "What's the weather like in Miami with wind speed in kilometers per hour?",
                    "parameters": {
                        "location": "Miami",
                        "windunit": "KMH"
                    }
                },
                {
                    "utterance": "I need the daily weather forecast for Chicago with a detailed dataset.",
                    "parameters": {
                        "location": "Chicago",
                        "dataset": "detailed"
                    }
                },
                {
                    "utterance": "Show me the weather forecast for Seattle for the next 10 periods.",
                    "parameters": {
                        "location": "Seattle",
                        "periods": 10
                    }
                },
                {
                    "utterance": "What's the weather forecast for San Francisco with temperatures in Celsius and wind speed in miles per hour?",
                    "parameters": {
                        "location": "San Francisco",
                        "tempunit": "C",
                        "windunit": "MPH"
                    }
                },
                {
                    "utterance": "Can I get the weather forecast for Boston with the full dataset?",
                    "parameters": {
                        "location": "Boston",
                        "dataset": "full"
                    }
                },
                {
                    "utterance": "Provide the weather forecast for Austin with wind speed in meters per second.",
                    "parameters": {
                        "location": "Austin",
                        "windunit": "MS"
                    }
                },
                {
                    "utterance": "What's the daily weather forecast for Las Vegas with a maximum of 12 periods?",
                    "parameters": {
                        "location": "Las Vegas",
                        "periods": 12
                    }
                }
            ]
        }
    ]
}