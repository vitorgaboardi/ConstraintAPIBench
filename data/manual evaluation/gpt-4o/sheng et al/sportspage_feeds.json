                {
                    "name": "league",
                    "type": "STRING",
                    "description": "A valid league code (i.e., NFL, NBA, MLB, NHL, NCAAF, or NCAAB)",
                    "default": "",
                    "required": true
                },
                {
                    "name": "conference",
                    "type": "STRING",
                    "description": "A conference within the specified league",
                    "default": "",
                    "required": false
                },
                {
                    "name": "division",
                    "type": "STRING",
                    "description": "A division within the specified conference",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you list all the teams in the NFL?",
                    "parameters": {
                        "league": "NFL"
                    }
                },
                {
                    "utterance": "Show me the teams in the NBA's Eastern Conference.",
                    "parameters": {
                        "league": "NBA",
                        "conference": "Eastern"
                    }
                },
                {
                    "utterance": "What are the teams in the MLB's American League West division?",
                    "parameters": {
                        "league": "MLB",
