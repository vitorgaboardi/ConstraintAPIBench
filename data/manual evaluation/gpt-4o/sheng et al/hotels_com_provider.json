            "description": "Get available rooms at the hotel",
            "url": "https://hotels-com-provider.p.rapidapi.com/v2/hotels/offers",
            "parameters": [
                {
                    "name": "adults_number",
                    "type": "NUMBER",
                    "description": "Number of adults",
                    "default": 1,
                    "required": true
                },
                {
                    "name": "checkout_date",
