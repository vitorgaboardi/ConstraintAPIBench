{
    "name": "Working days",
    "description": "The API returns the number of days,  hours, working days, working hours, wages, weekend days, and the list of public holidays of the requested date period and country. You can also add any number of working days or working hours to a given date. 50 countries and over 230 regional calendars are supported and we work hard to keep our database up to date by following government announcements regarding public holidays changes. All the calendars can be fully customized from our friendly working da...",
    "url": "https://rapidapi.com/joursouvres-api/api/working-days/",
    "api_methods": [
        {
            "name": "/1.3/add_working_days",
            "description": "Add (or remove) any number of working days to a date.",
            "url": "https://working-days.p.rapidapi.com/1.3/add_working_days",
            "parameters": [
                {
                    "name": "country_code",
                    "type": "STRING",
                    "description": "The ISO country code (2 letters).  See <a href=https://api.workingdays.org/api-countries >available countries & configurations</a>",
                    "default": "US",
                    "required": true,
                    "constraints": {
                        "format": "ISO 3166-1 alpha-2"
                    }
                },
                {
                    "name": "start_date",
                    "type": "STRING",
                    "description": "The start date (YYYY-MM-DD)",
                    "default": "2013-12-31",
                    "required": true,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                },
                {
                    "name": "increment",
                    "type": "NUMBER",
                    "description": "The number of working days you want to add to your start date (positive or negative integer but not zero)",
                    "default": 10,
                    "required": true,
                    "constraints": {
                        "values": {
                            "min": -2147483648,
                            "max": 2147483647
                        }
                    }
                },
                {
                    "name": "include_start",
                    "type": "BOOLEAN",
                    "description": "Should the count include the start_date? Default value is true. If you set include_start to false (\"false\" or \"0\"), the count will start at the next working day (or previous working day, if increment is negative)",
                    "default": true,
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "configuration",
                    "type": "STRING",
                    "description": "The name of the preset configuration to be used. See <a href=https://api.workingdays.org/api-countries.php>available countries & configurations</a>",
                    "default": "Federal holidays",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "profile_id",
                    "type": "STRING",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "id": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Add 5 working days to the start date of January 15, 2023, in Canada.",
                    "parameters": {
                        "country_code": "CA",
                        "start_date": "2023-01-15",
                        "increment": 5
                    }
                },
                {
                    "utterance": "I need to subtract 3 working days from March 10, 2023, in Germany, excluding the start date.",
                    "parameters": {
                        "country_code": "DE",
                        "start_date": "2023-03-10",
                        "increment": -3,
                        "include_start": false
                    }
                },
                {
                    "utterance": "Can you add 10 working days to the date April 1, 2023, in the UK using the 'Bank holidays' configuration?",
                    "parameters": {
                        "country_code": "GB",
                        "start_date": "2023-04-01",
                        "increment": 10,
                        "configuration": "Bank holidays"
                    }
                },
                {
                    "utterance": "Please add 7 working days to February 20, 2023, in Australia with my profile ID 'work_schedule_2023'.",
                    "parameters": {
                        "country_code": "AU",
                        "start_date": "2023-02-20",
                        "increment": 7,
                        "profile_id": "work_schedule_2023"
                    }
                },
                {
                    "utterance": "I want to add 15 working days to the start date of May 5, 2023, in Japan.",
                    "parameters": {
                        "country_code": "JP",
                        "start_date": "2023-05-05",
                        "increment": 15
                    }
                },
                {
                    "utterance": "Subtract 8 working days from June 12, 2023, in France, and include the start date in the count.",
                    "parameters": {
                        "country_code": "FR",
                        "start_date": "2023-06-12",
                        "increment": -8,
                        "include_start": true
                    }
                },
                {
                    "utterance": "Add 20 working days to July 1, 2023, in Italy using the 'Regional holidays' configuration.",
                    "parameters": {
                        "country_code": "IT",
                        "start_date": "2023-07-01",
                        "increment": 20,
                        "configuration": "Regional holidays"
                    }
                },
                {
                    "utterance": "I need to add 12 working days to August 15, 2023, in Brazil, excluding the start date.",
                    "parameters": {
                        "country_code": "BR",
                        "start_date": "2023-08-15",
                        "increment": 12,
                        "include_start": false
                    }
                },
                {
                    "utterance": "Please add 25 working days to September 10, 2023, in India with my profile ID 'holiday_schedule'.",
                    "parameters": {
                        "country_code": "IN",
                        "start_date": "2023-09-10",
                        "increment": 25,
                        "profile_id": "holiday_schedule"
                    }
                },
                {
                    "utterance": "Can you subtract 5 working days from October 20, 2023, in South Africa?",
                    "parameters": {
                        "country_code": "ZA",
                        "start_date": "2023-10-20",
                        "increment": -5
                    }
                }
            ]
        }
    ]
}