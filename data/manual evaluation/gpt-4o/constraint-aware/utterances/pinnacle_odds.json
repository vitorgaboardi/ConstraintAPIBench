{
    "name": "Pinnacle Odds",
    "description": "Pinnacle Sport API is a RESTful service for getting pre-match and live odds. Historical odds, score and results. Updates without delay. Sports: Soccer, tennis, basketball, hockey, american football, MMA, baseball. **Support**:  [tipsters@rapi.one](mailto:tipsters@rapi.one) / t.me/api_tipsters **Other sports api:** https://rapi.one",
    "url": "https://rapidapi.com/tipsters/api/pinnacle-odds/",
    "api_methods": [
        {
            "name": "List of special markets",
            "description": "Get a list of special markets. Always first issue a snapshot call and continue with the delta calls. Calls return changes since the provided `since` value. You must always use the since parameter, after the first call. Please note that prematch and live events are different",
            "url": "https://pinnacle-odds.p.rapidapi.com/kit/v1/special-markets",
            "parameters": [
                {
                    "name": "sport_id",
                    "type": "NUMBER",
                    "description": "Sport id",
                    "default": 1,
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "is_have_odds",
                    "type": "BOOLEAN",
                    "description": "`1` or `0`. You can only get matches for which there are already open odds, or matches that will be given odds in the future",
                    "default": true,
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                0,
                                1
                            ]
                        }
                    }
                },
                {
                    "name": "league_ids",
                    "type": "NUMBER",
                    "description": "League id",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "event_type",
                    "type": "STRING",
                    "description": "Status: `prematch`, `live`  Please note that prematch and live events are different",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "prematch",
                                "live"
                            ]
                        }
                    }
                },
                {
                    "name": "since",
                    "type": "NUMBER",
                    "description": "Since UTC time. Calls return changes since the provided `since` value.",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "event_ids",
                    "type": "NUMBER",
                    "description": "Event id",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "id": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me the special markets for soccer?",
                    "parameters": {
                        "sport_id": 1
                    }
                },
                {
                    "utterance": "I need the list of special markets for basketball with open odds.",
                    "parameters": {
                        "sport_id": 2,
                        "is_have_odds": 1
                    }
                },
                {
                    "utterance": "What are the special markets for live tennis events?",
                    "parameters": {
                        "sport_id": 3,
                        "event_type": "live"
                    }
                },
                {
                    "utterance": "Get me the special markets for the Premier League in soccer.",
                    "parameters": {
                        "sport_id": 1,
                        "league_ids": 100
                    }
                },
                {
                    "utterance": "Show me the special markets for upcoming hockey matches.",
                    "parameters": {
                        "sport_id": 4,
                        "event_type": "prematch"
                    }
                },
                {
                    "utterance": "I want to see the special markets for MMA events with odds available.",
                    "parameters": {
                        "sport_id": 5,
                        "is_have_odds": 1
                    }
                },
                {
                    "utterance": "List the special markets for the NBA in basketball.",
                    "parameters": {
                        "sport_id": 2,
                        "league_ids": 200
                    }
                },
                {
                    "utterance": "Can you provide the special markets for live baseball games?",
                    "parameters": {
                        "sport_id": 6,
                        "event_type": "live"
                    }
                },
                {
                    "utterance": "Find the special markets for the Super Bowl in American football.",
                    "parameters": {
                        "sport_id": 7,
                        "event_ids": 300
                    }
                },
                {
                    "utterance": "What are the special markets for live soccer matches with odds?",
                    "parameters": {
                        "sport_id": 1,
                        "event_type": "live",
                        "is_have_odds": 1
                    }
                }
            ]
        }
    ]
}