{
    "name": "CurrencyAPI.net",
    "description": "Real-time currency conversion on 152 currencies and cryptos",
    "url": "https://rapidapi.com/currencyapinet/api/currencyapi-net/",
    "api_methods": [
        {
            "name": "Timeframe",
            "description": "Display a currency conversion's historical rates within a given timeframe",
            "url": "https://currencyapi-net.p.rapidapi.com/timeframe",
            "parameters": [
                {
                    "name": "end_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-29",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                },
                {
                    "name": "base",
                    "type": "STRING",
                    "description": "The currency code you want to get the historical data of. Eg, USD",
                    "default": "USD",
                    "required": true,
                    "constraints": {
                        "format": "currency code"
                    }
                },
                {
                    "name": "start_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-25",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "YYYY-MM-DD"
                    }
                },
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "JSON",
                                "XML"
                            ]
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me the historical exchange rates for EUR from January 1, 2022, to January 31, 2022?",
                    "parameters": {
                        "start_date": "2022-01-01",
                        "end_date": "2022-01-31",
                        "base": "EUR"
                    }
                },
                {
                    "utterance": "I need the historical currency data for GBP between March 1, 2023, and March 15, 2023, in XML format.",
                    "parameters": {
                        "start_date": "2023-03-01",
                        "end_date": "2023-03-15",
                        "base": "GBP",
                        "output": "XML"
                    }
                },
                {
                    "utterance": "Get me the historical rates for JPY from February 10, 2021, to February 20, 2021.",
                    "parameters": {
                        "start_date": "2021-02-10",
                        "end_date": "2021-02-20",
                        "base": "JPY"
                    }
                },
                {
                    "utterance": "I'd like to see the historical conversion rates for AUD from April 5, 2020, to April 25, 2020, in JSON.",
                    "parameters": {
                        "start_date": "2020-04-05",
                        "end_date": "2020-04-25",
                        "base": "AUD",
                        "output": "JSON"
                    }
                },
                {
                    "utterance": "Please provide the historical exchange data for CAD from May 1, 2019, to May 10, 2019.",
                    "parameters": {
                        "start_date": "2019-05-01",
                        "end_date": "2019-05-10",
                        "base": "CAD"
                    }
                },
                {
                    "utterance": "Show me the historical currency rates for CHF from June 15, 2018, to June 30, 2018, in XML.",
                    "parameters": {
                        "start_date": "2018-06-15",
                        "end_date": "2018-06-30",
                        "base": "CHF",
                        "output": "XML"
                    }
                },
                {
                    "utterance": "Can you get the historical rates for INR from July 1, 2017, to July 15, 2017?",
                    "parameters": {
                        "start_date": "2017-07-01",
                        "end_date": "2017-07-15",
                        "base": "INR"
                    }
                },
                {
                    "utterance": "I want to see the historical exchange rates for CNY from August 10, 2016, to August 20, 2016, in JSON format.",
                    "parameters": {
                        "start_date": "2016-08-10",
                        "end_date": "2016-08-20",
                        "base": "CNY",
                        "output": "JSON"
                    }
                },
                {
                    "utterance": "Fetch the historical currency data for NZD from September 5, 2015, to September 15, 2015.",
                    "parameters": {
                        "start_date": "2015-09-05",
                        "end_date": "2015-09-15",
                        "base": "NZD"
                    }
                },
                {
                    "utterance": "I'd like the historical rates for SEK from October 1, 2014, to October 10, 2014, in XML.",
                    "parameters": {
                        "start_date": "2014-10-01",
                        "end_date": "2014-10-10",
                        "base": "SEK",
                        "output": "XML"
                    }
                }
            ]
        }
    ]
}