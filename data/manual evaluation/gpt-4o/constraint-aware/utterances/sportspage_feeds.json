{
    "name": "Sportspage Feeds",
    "description": "Real-time scores, schedules, and betting odds for US leagues.  Unbeatable value.  Documentation: https://sportspagefeeds.com/documentation",
    "url": "https://rapidapi.com/SportspageFeeds/api/sportspage-feeds/",
    "api_methods": [
        {
            "name": "Teams",
            "description": "Returns a list of teams within a specified league, conference, or division.",
            "url": "https://sportspage-feeds.p.rapidapi.com/teams",
            "parameters": [
                {
                    "name": "league",
                    "type": "STRING",
                    "description": "A valid league code (i.e., NFL, NBA, MLB, NHL, NCAAF, or NCAAB)",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "NFL",
                                "NBA",
                                "MLB",
                                "NHL",
                                "NCAAF",
                                "NCAAB"
                            ]
                        }
                    }
                },
                {
                    "name": "conference",
                    "type": "STRING",
                    "description": "A conference within the specified league",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "division",
                    "type": "STRING",
                    "description": "A division within the specified conference",
                    "default": "",
                    "required": false,
                    "constraints": {}
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you list all the teams in the NFL?",
                    "parameters": {
                        "league": "NFL"
                    }
                },
                {
                    "utterance": "Show me the teams in the NBA's Eastern Conference.",
                    "parameters": {
                        "league": "NBA",
                        "conference": "Eastern"
                    }
                },
                {
                    "utterance": "Which teams are in the MLB's American League West division?",
                    "parameters": {
                        "league": "MLB",
                        "conference": "American League",
                        "division": "West"
                    }
                },
                {
                    "utterance": "Give me a list of NHL teams.",
                    "parameters": {
                        "league": "NHL"
                    }
                },
                {
                    "utterance": "What teams are in the NCAAF's SEC conference?",
                    "parameters": {
                        "league": "NCAAF",
                        "conference": "SEC"
                    }
                },
                {
                    "utterance": "List the teams in the NCAAB's Big Ten conference.",
                    "parameters": {
                        "league": "NCAAB",
                        "conference": "Big Ten"
                    }
                },
                {
                    "utterance": "Can you provide the teams in the NFL's NFC East division?",
                    "parameters": {
                        "league": "NFL",
                        "conference": "NFC",
                        "division": "East"
                    }
                },
                {
                    "utterance": "Show me all the teams in the NBA.",
                    "parameters": {
                        "league": "NBA"
                    }
                },
                {
                    "utterance": "Which teams are part of the NHL's Western Conference?",
                    "parameters": {
                        "league": "NHL",
                        "conference": "Western"
                    }
                },
                {
                    "utterance": "Give me the list of teams in the MLB's National League Central division.",
                    "parameters": {
                        "league": "MLB",
                        "conference": "National League",
                        "division": "Central"
                    }
                }
            ]
        }
    ]
}