                    "constraints": {
                        "values": {
                            "enumerated": [
                                "unfurnished",
                                "furnished"
                            ]
                        }
                    }
                },
                {
                    "name": "bathsMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "One of the following : price-desc|price-asc|city-level-score|date-desc|verified-score",
                    "default": "city-level-score",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "price-desc",
                                "price-asc",
