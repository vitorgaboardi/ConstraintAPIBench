                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "purpose",
                    "type": "STRING",
                    "description": "One of the following : for-rent|for-sale",
                    "default": "for-rent",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "for-rent",
                                "for-sale"
                            ]
                        }
                    }
                },
                {
                    "name": "hasVideo",
                    "type": "BOOLEAN",
                    "description": "",
                    "default": "",
                    "required": false
                },
