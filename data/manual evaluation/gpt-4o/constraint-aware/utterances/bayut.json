                {
                    "name": "agencyExternalIDs",
                    "type": "STRING",
                    "description": "The externalID fields returned in .../agencies/list endpoint. Separated by comma for multiple agencies. Ex : 7737,5002",
                    "default": "",
                    "required": false
                },
                {
                    "name": "hitsPerPage",
                    "type": "NUMBER",
