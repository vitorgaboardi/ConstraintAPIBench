{
    "name": "Hotels com Provider",
    "description": "Search hotels, see prices, photos of the hotels, reviews.  **Site:** www.hotels.com **Support**:  [tipsters@rapi.one](mailto:tipsters@rapi.one) / t.me/api_tipsters **Other travel api:** https://rapi.one",
    "url": "https://rapidapi.com/tipsters/api/hotels-com-provider/",
    "api_methods": [
        {
            "name": "Reviews summary",
            "description": "Get hotel reviews summary",
            "url": "https://hotels-com-provider.p.rapidapi.com/v2/hotels/reviews/summary",
            "parameters": [
                {
                    "name": "locale",
                    "type": "STRING",
                    "description": "",
                    "default": "en_GB",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "domain",
                    "type": "STRING",
                    "description": "",
                    "default": "AE",
                    "required": true,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "hotel_id",
                    "type": "NUMBER",
                    "description": "Hotel ID",
                    "default": "1105156",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you give me a summary of reviews for the Hilton Garden Inn?",
                    "parameters": {
                        "hotel_id": "Hilton Garden Inn"
                    }
                },
                {
                    "utterance": "I'd like to see the review summary for the Marriott Downtown.",
                    "parameters": {
                        "hotel_id": "Marriott Downtown"
                    }
                },
                {
                    "utterance": "What do people say about the Ritz-Carlton in their reviews?",
                    "parameters": {
                        "hotel_id": "Ritz-Carlton"
                    }
                },
                {
                    "utterance": "Show me the reviews summary for the Four Seasons Hotel.",
                    "parameters": {
                        "hotel_id": "Four Seasons Hotel"
                    }
                },
                {
                    "utterance": "Could you provide a summary of the reviews for the Hyatt Regency?",
                    "parameters": {
                        "hotel_id": "Hyatt Regency"
                    }
                },
                {
                    "utterance": "I want to know the review summary for the InterContinental Hotel.",
                    "parameters": {
                        "hotel_id": "InterContinental Hotel"
                    }
                },
                {
                    "utterance": "Give me a summary of the reviews for the Sheraton Grand.",
                    "parameters": {
                        "hotel_id": "Sheraton Grand"
                    }
                },
                {
                    "utterance": "What's the review summary for the Westin Hotel?",
                    "parameters": {
                        "hotel_id": "Westin Hotel"
                    }
                },
                {
                    "utterance": "Can you show me the reviews summary for the Waldorf Astoria?",
                    "parameters": {
                        "hotel_id": "Waldorf Astoria"
                    }
                },
                {
                    "utterance": "I'd like to see what the reviews say about the St. Regis Hotel.",
                    "parameters": {
                        "hotel_id": "St. Regis Hotel"
                    }
                }
            ]
        }
    ]
}