{
    "name": "Flight Radar",
    "description": "The world's most popular flight tracker API which helps you to create Flight tracker application, such as : flightradar24.com",
    "url": "https://rapidapi.com/apidojo/api/flight-radar1/",
    "api_methods": [
        {
            "name": "flights/list-in-boundary",
            "description": "Helps to list flights, aircrafts in a GEO bounding box, and display them on a map",
            "url": "https://flight-radar1.p.rapidapi.com/flights/list-in-boundary",
            "parameters": [
                {
                    "name": "bl_lng",
                    "type": "NUMBER",
                    "description": "The bottom left longitude of the bounding box",
                    "default": "100.641975",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "tr_lat",
                    "type": "NUMBER",
                    "description": "The top right latitude of the bounding box",
                    "default": "13.771029",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "bl_lat",
                    "type": "NUMBER",
                    "description": "The bottom left latitude of the bounding box",
                    "default": "13.607884",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "tr_lng",
                    "type": "NUMBER",
                    "description": "The top right longitude of the bounding box",
                    "default": "100.861566",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "The value of 'Code' field returned in .../aircrafts/list endpoint",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "speed",
                    "type": "STRING",
                    "description": "Filter by speed. The format is min,max . Ex : 0,460",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "airline",
                    "type": "STRING",
                    "description": "The value of 'ICAO' field returned in .../airlines/list endpoint",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "altitude",
                    "type": "STRING",
                    "description": "Filter by altitude. The format is min,max . Ex : 0,48000",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "airport",
                    "type": "STRING",
                    "description": "The value of 'icao' field returned in .../airports/list endpoint",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "reg",
                    "type": "STRING",
                    "description": "Registration Ex : D-AIHV",
                    "default": "",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "limit",
                    "type": "NUMBER",
                    "description": "The number of flights per response (max 300)",
                    "default": "300",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 300
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me all flights in the area from 100.5 longitude and 13.6 latitude to 100.9 longitude and 13.8 latitude.",
                    "parameters": {
                        "bl_lng": 100.5,
                        "tr_lat": 13.8,
                        "bl_lat": 13.6,
                        "tr_lng": 100.9
                    }
                },
                {
                    "utterance": "List flights between coordinates 100.6, 13.7 and 100.8, 13.9 with a speed range of 200 to 500.",
                    "parameters": {
                        "bl_lng": 100.6,
                        "tr_lat": 13.9,
                        "bl_lat": 13.7,
                        "tr_lng": 100.8,
                        "speed": "200,500"
                    }
                },
                {
                    "utterance": "Can you find flights in the bounding box from 100.4 longitude, 13.5 latitude to 100.7 longitude, 13.7 latitude operated by airline ICAO123?",
                    "parameters": {
                        "bl_lng": 100.4,
                        "tr_lat": 13.7,
                        "bl_lat": 13.5,
                        "tr_lng": 100.7,
                        "airline": "ICAO123"
                    }
                },
                {
                    "utterance": "Retrieve flights in the area from 100.3 longitude, 13.4 latitude to 100.6 longitude, 13.6 latitude with altitude between 1000 and 20000.",
                    "parameters": {
                        "bl_lng": 100.3,
                        "tr_lat": 13.6,
                        "bl_lat": 13.4,
                        "tr_lng": 100.6,
                        "altitude": "1000,20000"
                    }
                },
                {
                    "utterance": "Get flights in the region from 100.2 longitude, 13.3 latitude to 100.5 longitude, 13.5 latitude for the airport ICAO456.",
                    "parameters": {
                        "bl_lng": 100.2,
                        "tr_lat": 13.5,
                        "bl_lat": 13.3,
                        "tr_lng": 100.5,
                        "airport": "ICAO456"
                    }
                },
                {
                    "utterance": "Find flights in the box from 100.1 longitude, 13.2 latitude to 100.4 longitude, 13.4 latitude with registration D-ABCD.",
                    "parameters": {
                        "bl_lng": 100.1,
