                    "parameters": {
                        "bl_lng": 100.0,
                        "tr_lat": 13.3,
                        "bl_lat": 13.1,
                        "tr_lng": 100.3,
                        "limit": 150
                    }
                },
                {
                    "utterance": "List all flights in the region from 99.9 longitude, 13.0 latitude to 100.2 longitude, 13.2 latitude for aircraft type A320.",
                    "parameters": {
                        "bl_lng": 99.9,
                        "tr_lat": 13.2,
                        "bl_lat": 13.0,
                        "tr_lng": 100.2,
                        "type": "A320"
                    }
                },
                {
                    "utterance": "Can you show flights in the area from 99.8 longitude, 12.9 latitude to 100.1 longitude, 13.1 latitude with speed between 100 and 400?",
                    "parameters": {
                        "bl_lng": 99.8,
                        "tr_lat": 13.1,
                        "bl_lat": 12.9,
                        "tr_lng": 100.1,
                        "speed": "100,400"
                    }
                },
                {
                    "utterance": "Retrieve flights in the bounding box from 99.7 longitude, 12.8 latitude to 100.0 longitude, 13.0 latitude with altitude range 5000 to 25000.",
                    "parameters": {
                        "bl_lng": 99.7,
                        "tr_lat": 13.0,
                        "bl_lat": 12.8,
                        "tr_lng": 100.0,
                        "altitude": "5000,25000"
                    }
                }
            ]
        }
    ]
}