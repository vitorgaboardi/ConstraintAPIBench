{
    "name": "Referential",
    "description": "The fastest API to access countries, states, cities, continents, dial and zip codes in up to 20 languages. A collection of data APIs to support forms, signups, search and lookup. Our endpoints are optimized for speed and size. Our data is regularly maintained and comes from International Standardization bodies, the United Nations, government surveys and GIS datasets. We do not scrape WikiPedia etc",
    "url": "https://rapidapi.com/referential/api/referential/",
    "api_methods": [
        {
            "name": "Country by iso code",
            "description": "Get country by 2 letter iso code localized to a given language",
            "url": "https://referential.p.rapidapi.com/v1/country/{iso_code}",
            "parameters": [
                {
                    "name": "iso_code",
                    "type": "string",
                    "description": "2 letter iso country code. ",
                    "default": "US",
                    "required": true,
                    "constraints": {
                        "format": "2 letter ISO country code"
                    }
                },
                {
                    "name": "fields",
                    "type": "STRING",
                    "description": "Comma deliminated list of fields to return in response",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "2 letter iso language code to localize country name",
                    "default": "en",
                    "required": false,
                    "constraints": {
                        "format": "2 letter ISO language code"
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you provide information about the country with the ISO code 'FR'?",
                    "parameters": {
                        "iso_code": "FR"
                    }
                },
                {
                    "utterance": "I'd like to know about the country with ISO code 'JP' in Japanese.",
                    "parameters": {
                        "iso_code": "JP",
                        "lang": "ja"
                    }
                },
                {
                    "utterance": "Give me details about the country with ISO code 'BR', including its population and area.",
                    "parameters": {
                        "iso_code": "BR",
                        "fields": "population,area"
                    }
                },
                {
                    "utterance": "What can you tell me about the country with ISO code 'DE' in German?",
                    "parameters": {
                        "iso_code": "DE",
                        "lang": "de"
                    }
                },
                {
                    "utterance": "Fetch the country information for ISO code 'IN' with fields for capital and currency.",
                    "parameters": {
                        "iso_code": "IN",
                        "fields": "capital,currency"
                    }
                },
                {
                    "utterance": "I need the country details for ISO code 'CN' in Chinese.",
                    "parameters": {
                        "iso_code": "CN",
                        "lang": "zh"
                    }
                },
                {
                    "utterance": "Could you get the country data for ISO code 'IT' with fields for language and timezone?",
                    "parameters": {
                        "iso_code": "IT",
                        "fields": "language,timezone"
                    }
                },
                {
                    "utterance": "Please provide the country information for ISO code 'AU' in English.",
                    "parameters": {
                        "iso_code": "AU",
                        "lang": "en"
                    }
                },
                {
                    "utterance": "What is the country with ISO code 'ES' and its main exports?",
                    "parameters": {
                        "iso_code": "ES",
                        "fields": "exports"
                    }
                },
                {
                    "utterance": "Can you show me the country details for ISO code 'RU' in Russian?",
                    "parameters": {
                        "iso_code": "RU",
                        "lang": "ru"
                    }
                }
            ]
        },
        {
            "name": "Languages",
            "description": "List the world's languages translated into 20 languages",
            "url": "https://referential.p.rapidapi.com/v1/lang",
            "parameters": [
                {
                    "name": "lang_3",
                    "type": "STRING",
                    "description": "Filter by 3 letter ISO language code",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "format": "3 letter ISO language code"
                    }
                },
                {
                    "name": "iso_a2",
                    "type": "STRING",
                    "description": "Filter on 2 letter country code",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "format": "2 letter country code"
                    }
                },
                {
                    "name": "fields",
                    "type": "STRING",
                    "description": "Select fields to return in result [flag,iso_a2,lang_3,lang]",
                    "default": "iso_a2,lang_3,flag",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "flag",
                                "iso_a2",
                                "lang_3",
                                "lang"
                            ]
                        }
                    }
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "Returns a language translated into lang language",
                    "default": "en",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me the list of languages in French?",
                    "parameters": {
                        "lang": "fr"
                    }
                },
                {
                    "utterance": "I need the languages translated into Spanish with their flags.",
                    "parameters": {
                        "lang": "es",
                        "fields": "flag,lang"
                    }
                },
                {
                    "utterance": "List the languages spoken in Germany.",
                    "parameters": {
                        "iso_a2": "DE"
                    }
                },
                {
                    "utterance": "What are the languages available in the United States?",
                    "parameters": {
                        "iso_a2": "US"
                    }
                },
                {
                    "utterance": "Could you provide the languages in Japanese with their ISO codes?",
                    "parameters": {
                        "lang": "ja",
                        "fields": "iso_a2,lang_3"
                    }
                },
                {
                    "utterance": "Show me the languages in Italy with their flags and ISO codes.",
                    "parameters": {
                        "iso_a2": "IT",
                        "fields": "flag,iso_a2,lang_3"
                    }
                },
                {
                    "utterance": "List the languages in Brazil translated into Portuguese.",
                    "parameters": {
                        "iso_a2": "BR",
                        "lang": "pt"
                    }
                },
                {
                    "utterance": "Can you give me the languages in China with their 3-letter codes?",
                    "parameters": {
                        "iso_a2": "CN",
                        "fields": "lang_3"
                    }
                },
                {
                    "utterance": "I want to see the languages in Russia with their flags.",
                    "parameters": {
                        "iso_a2": "RU",
                        "fields": "flag"
                    }
                },
                {
                    "utterance": "Provide the list of languages in India with their names in Hindi.",
                    "parameters": {
                        "iso_a2": "IN",
                        "lang": "hi"
                    }
                }
            ]
        }
    ]
}