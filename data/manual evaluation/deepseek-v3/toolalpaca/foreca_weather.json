{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "url": "https://rapidapi.com/foreca-ltd-foreca-ltd-default/api/foreca-weather/",
    "api_methods": [
        {
            "name": "Daily",
            "description": "Daily forecast.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/daily/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you tell me the weather forecast for Paris for the next 8 days?",
                    "parameters": {
                        "location": "Paris",
                        "periods": 8
                    }
                },
                {
                    "utterance": "I need the daily weather details for New York City with temperatures in Fahrenheit.",
                    "parameters": {
                        "location": "New York City",
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "What\u2019s the wind speed in Berlin for the next 12 days, measured in kilometers per hour?",
                    "parameters": {
                        "location": "Berlin",
                        "windunit": "KMH",
                        "periods": 12
                    }
                },
                {
                    "utterance": "Show me the weather forecast for Tokyo at an altitude of 500 meters.",
                    "parameters": {
                        "location": "Tokyo",
                        "alt": 500
                    }
                },
                {
                    "utterance": "Could you provide the full weather dataset for Sydney for the next 10 days?",
                    "parameters": {
                        "location": "Sydney",
                        "dataset": "full",
                        "periods": 10
                    }
                },
                {
                    "utterance": "I\u2019m planning a trip to London; can you give me the daily weather forecast with wind speeds in miles per hour?",
                    "parameters": {
                        "location": "London",
                        "windunit": "MPH"
                    }
                },
                {
                    "utterance": "What\u2019s the weather like in Dubai for the next week, with temperatures in Celsius?",
                    "parameters": {
                        "location": "Dubai",
                        "tempunit": "C",
                        "periods": 7
                    }
                },
                {
                    "utterance": "Get me the weather forecast for Rome at sea level for the next 5 days.",
                    "parameters": {
                        "location": "Rome",
                        "alt": 0,
                        "periods": 5
                    }
                },
                {
                    "utterance": "Can you check the weather in Mumbai for the next 6 days, including wind speed in meters per second?",
                    "parameters": {
                        "location": "Mumbai",
                        "windunit": "MS",
                        "periods": 6
                    }
                },
                {
                    "utterance": "I need the full weather details for Los Angeles for the next 9 days, with temperatures in Fahrenheit.",
                    "parameters": {
                        "location": "Los Angeles",
                        "tempunit": "F",
                        "dataset": "full",
                        "periods": 9
                    }
                }
            ]
        }
    ]
}