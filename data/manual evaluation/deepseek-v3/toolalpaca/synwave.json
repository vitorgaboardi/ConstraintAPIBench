{
    "name": "SynWave",
    "description": "A data storage management API offering the following features:\n\n",
    "url": "https://rapidapi.com/rpi4gx/api/synwave/",
    "api_methods": [
        {
            "name": "Upload a new file",
            "description": "Upload a new file.",
            "url": "https://synwave.p.rapidapi.com/v1/upload",
            "parameters": [
                {
                    "name": "file",
                    "type": "BINARY",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "auth_password",
                    "type": "STRING",
                    "description": "Password to protect this file. auth_username parameter is required if auth_password is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "expiration_seconds",
                    "type": "NUMBER",
                    "description": "Number of seconds before the file becomes unavailable.\n",
                    "default": 3600,
                    "required": false
                },
                {
                    "name": "auth_username",
                    "type": "STRING",
                    "description": "Username to protect this file. auth_password parameter is required if auth_username is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "download_limit",
                    "type": "NUMBER",
                    "description": "Number of downloads before the file becomes unavailable.\n",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you upload my vacation photos and set them to expire in 2 hours?",
                    "parameters": {
                        "file": "vacation photos",
                        "expiration_seconds": 7200
                    }
                },
                {
                    "utterance": "I need to store the quarterly financial report with a download limit of 5.",
                    "parameters": {
                        "file": "quarterly financial report",
                        "download_limit": 5
                    }
                },
                {
                    "utterance": "Upload the project proposal and protect it with the username 'admin' and password 'secure123'.",
                    "parameters": {
                        "file": "project proposal",
                        "auth_username": "admin",
                        "auth_password": "secure123"
                    }
                },
                {
                    "utterance": "Please save the wedding video and make it available for only 1 hour.",
                    "parameters": {
                        "file": "wedding video",
                        "expiration_seconds": 3600
                    }
                },
                {
                    "utterance": "Can you upload the training manual and restrict it to 3 downloads?",
                    "parameters": {
                        "file": "training manual",
                        "download_limit": 3
                    }
                },
                {
                    "utterance": "I want to store the confidential contract with the username 'manager' and password 'topsecret'.",
                    "parameters": {
                        "file": "confidential contract",
                        "auth_username": "manager",
                        "auth_password": "topsecret"
                    }
                },
                {
                    "utterance": "Upload the presentation slides and set them to expire in 30 minutes.",
                    "parameters": {
                        "file": "presentation slides",
                        "expiration_seconds": 1800
                    }
                },
                {
                    "utterance": "Save the research paper and limit it to 10 downloads.",
                    "parameters": {
                        "file": "research paper",
                        "download_limit": 10
                    }
                },
                {
                    "utterance": "Can you upload the product demo video and protect it with the username 'demo' and password 'viewonly'?",
                    "parameters": {
                        "file": "product demo video",
                        "auth_username": "demo",
                        "auth_password": "viewonly"
                    }
                },
                {
                    "utterance": "I need to store the event photos and make them available for only 3 hours.",
                    "parameters": {
                        "file": "event photos",
                        "expiration_seconds": 10800
                    }
                }
            ]
        }
    ]
}