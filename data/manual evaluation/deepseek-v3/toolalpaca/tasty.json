{
    "name": "Tasty",
    "description": "API to query data about recipe, plan, ingredients, etc... as on official site",
    "url": "https://rapidapi.com/apidojo/api/tasty/",
    "api_methods": [
        {
            "name": "tips/list",
            "description": "This endpoint is used to load tips (reviews)",
            "url": "https://tasty.p.rapidapi.com/tips/list",
            "parameters": [
                {
                    "name": "id",
                    "type": "NUMBER",
                    "description": "The value of recipe id returned in .../recipes/list .../feeds/list .../recipes/list-similarities endpoints",
                    "default": "3562",
                    "required": true
                },
                {
                    "name": "size",
                    "type": "NUMBER",
                    "description": "Number of items returned per response",
                    "default": "30",
                    "required": false
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "The offset of items to be ignored in response for paging",
                    "default": "0",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me the first 10 tips for the recipe with ID 1234?",
                    "parameters": {
                        "id": 1234,
                        "size": 10,
                        "from": 0
                    }
                },
                {
                    "utterance": "I need the next 15 tips for the lasagna recipe, starting from the 5th tip.",
                    "parameters": {
                        "id": 3562,
                        "size": 15,
                        "from": 5
                    }
                },
                {
                    "utterance": "Fetch the latest 20 reviews for the chocolate cake recipe with ID 5678.",
                    "parameters": {
                        "id": 5678,
                        "size": 20,
                        "from": 0
                    }
                },
                {
                    "utterance": "Could you get me tips 6 through 10 for the chicken curry recipe?",
                    "parameters": {
                        "id": 9101,
                        "size": 5,
                        "from": 5
                    }
                },
                {
                    "utterance": "Show me the first 25 tips for the recipe with ID 4321.",
                    "parameters": {
                        "id": 4321,
                        "size": 25,
                        "from": 0
                    }
                },
                {
                    "utterance": "I\u2019d like to see tips 11 to 20 for the beef stew recipe.",
                    "parameters": {
                        "id": 7531,
                        "size": 10,
                        "from": 10
                    }
                },
                {
                    "utterance": "Can you retrieve the first 30 tips for the recipe with ID 8642?",
                    "parameters": {
                        "id": 8642,
                        "size": 30,
                        "from": 0
                    }
                },
                {
                    "utterance": "Give me the next 10 tips for the vegetarian lasagna recipe, starting from tip 15.",
                    "parameters": {
                        "id": 2468,
                        "size": 10,
                        "from": 15
                    }
                },
                {
                    "utterance": "I need the first 5 tips for the recipe with ID 9753.",
                    "parameters": {
                        "id": 9753,
                        "size": 5,
                        "from": 0
                    }
                },
                {
                    "utterance": "Could you fetch tips 21 to 30 for the banana bread recipe?",
                    "parameters": {
                        "id": 1357,
                        "size": 10,
                        "from": 20
                    }
                }
            ]
        }
    ]
}