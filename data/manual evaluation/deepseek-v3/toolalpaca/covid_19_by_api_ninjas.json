{
    "name": "Covid-19 by API-Ninjas",
    "description": "Covid-19 case count and death data for every country in the world. See more info at https://api-ninjas.com/api/covid19.",
    "url": "https://rapidapi.com/apininjas/api/covid-19-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/covid19",
            "description": "API Ninjas Covid19 API endpoint. Either **date** or **country** must be set.",
            "url": "https://covid-19-by-api-ninjas.p.rapidapi.com/v1/covid19",
            "parameters": [
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "type of data to retrieve. Must be either cases or deaths. If not set, cases will be used by default.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "country",
                    "type": "STRING",
                    "description": "country name (case insensitive).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "date",
                    "type": "STRING",
                    "description": "date to retrieve single-day snapshot. Must be in the form of YYYY-MM-DD (e.g. 2022-01-01)",
                    "default": "2022-01-01",
                    "required": false
                },
                {
                    "name": "county",
                    "type": "STRING",
                    "description": "county name for US states (case insensitive). For United States data only. Must be used in conjunction with country (United States) and region (e.g. California).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "region",
                    "type": "STRING",
                    "description": "administrative region (also known as state or province in many countries) name (case insensitive). Must be used in conjunction with country. If not set, countries with data broken down by administrative regions will return separate data for each region.",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you check the number of COVID-19 cases in Japan for yesterday?",
                    "parameters": {
                        "country": "Japan",
                        "date": "2023-10-04",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "Find out how many COVID-19 deaths were reported in California last week.",
                    "parameters": {
                        "country": "United States",
                        "region": "California",
                        "date": "2023-09-27",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "I need the COVID-19 case count for Germany on October 1st, 2023.",
                    "parameters": {
                        "country": "Germany",
                        "date": "2023-10-01",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "What were the COVID-19 deaths in New York County on September 30th?",
                    "parameters": {
                        "country": "United States",
                        "region": "New York",
                        "county": "New York County",
                        "date": "2023-09-30",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "Show me the COVID-19 cases in Brazil for the last day of September.",
                    "parameters": {
                        "country": "Brazil",
                        "date": "2023-09-30",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "Get the number of COVID-19 deaths in Texas on October 2nd, 2023.",
                    "parameters": {
                        "country": "United States",
                        "region": "Texas",
                        "date": "2023-10-02",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "Can you provide the COVID-19 case data for France on September 28th?",
                    "parameters": {
                        "country": "France",
                        "date": "2023-09-28",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "Tell me the COVID-19 deaths in Los Angeles County on October 3rd.",
                    "parameters": {
                        "country": "United States",
                        "region": "California",
                        "county": "Los Angeles County",
                        "date": "2023-10-03",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "What was the COVID-19 case count in Italy on September 29th?",
                    "parameters": {
                        "country": "Italy",
                        "date": "2023-09-29",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "Retrieve the COVID-19 death numbers for Florida on October 1st, 2023.",
                    "parameters": {
                        "country": "United States",
                        "region": "Florida",
                        "date": "2023-10-01",
                        "type": "deaths"
                    }
                }
            ]
        }
    ]
}