                                4,
                                16,
                                3,
                                18,
                                21,
                                19,
                                14,
                                12,
                                17,
                                5,
                                6,
                                7,
                                9,
                                25,
                                20,
                                15,
                                13,
                                10,
                                8,
                                22,
                                23,
                                24,
                                11
                            ]
                        }
                    }
                },
                {
                    "name": "rentFrequency",
                    "type": "STRING",
                    "description": "One of the following : monthly|yearly|weekly|daily",
                    "default": "monthly",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "monthly",
                                "yearly",
                                "weekly",
                                "daily"
                            ]
                        }
                    }
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "One of the following : en|ar",
                    "default": "en",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "en",
                                "ar"
                            ]
                        }
                    }
                },
                {
                    "name": "priceMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "priceMin, priceMax: priceMin must be less than or equal to priceMax."
                    }
                },
                {
                    "name": "priceMax",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "priceMin, priceMax: priceMax must be greater than or equal to priceMin."
                    }
                },
                {
                    "name": "areaMax",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "areaMin, areaMax: areaMax must be greater than or equal to areaMin."
                    }
                }
            ],
            "utterances": [
