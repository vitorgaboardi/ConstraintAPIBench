{
    "name": "Bayut",
    "description": "This API helps to query real estate in UAE to create a real estate site/application",
    "url": "https://rapidapi.com/apidojo/api/bayut/",
    "api_methods": [
        {
            "name": "properties/list",
            "description": "List properties with options and filters",
            "url": "https://bayut.p.rapidapi.com/properties/list",
            "parameters": [
                {
                    "name": "locationExternalIDs",
                    "type": "STRING",
                    "description": "The location/externalID fields returned in .../auto-complete endpoint. Separated by comma for multiple locations. Ex : 5002,6020",
                    "default": "5002,6020",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "hasFloorPlan",
                    "type": "BOOLEAN",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "areaMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "areaMin, areaMax: areaMin must be less than or equal to areaMax."
                    }
                },
                {
                    "name": "purpose",
                    "type": "STRING",
                    "description": "One of the following : for-rent|for-sale",
                    "default": "for-rent",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "for-rent",
                                "for-sale"
                            ]
                        }
                    }
                },
                {
                    "name": "hasVideo",
                    "type": "BOOLEAN",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "hasPanorama",
                    "type": "BOOLEAN",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "furnishingStatus",
                    "type": "STRING",
                    "description": "One of the following : unfurnished|furnished",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "unfurnished",
                                "furnished"
                            ]
                        }
                    }
                },
                {
                    "name": "bathsMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "bathsMin, bathsMax: bathsMin must be less than or equal to bathsMax."
                    }
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "One of the following : price-desc|price-asc|city-level-score|date-desc|verified-score",
                    "default": "city-level-score",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "price-desc",
                                "price-asc",
                                "city-level-score",
                                "date-desc",
                                "verified-score"
                            ]
                        }
                    }
                },
                {
                    "name": "bathsMax",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "inter-dependency": "bathsMin, bathsMax: bathsMax must be greater than or equal to bathsMin."
                    }
                },
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "agencyExternalIDs",
                    "type": "STRING",
                    "description": "The externalID fields returned in .../agencies/list endpoint. Separated by comma for multiple agencies. Ex : 7737,5002",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "hitsPerPage",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "25",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "roomsMin",
