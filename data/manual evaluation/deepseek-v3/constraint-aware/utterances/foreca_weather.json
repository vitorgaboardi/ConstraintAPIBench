{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "url": "https://rapidapi.com/foreca-ltd-foreca-ltd-default/api/foreca-weather/",
    "api_methods": [
        {
            "name": "Hourly",
            "description": "Hourly forecast.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/hourly/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "history",
                    "type": "BOOLEAN",
                    "description": "Whether to include 24 hours of past data.",
                    "default": "0",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "tz",
                    "type": "STRING",
                    "description": "Time zone in response (IANA time zone database names)",
                    "default": "Europe/London",
                    "required": false,
                    "constraints": {
                        "format": "IANA time zone database names",
                        "technical": true
                    }
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 12,
                            "min": 1
                        },
                        "technical": true
                    }
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the hourly forecast for New York City?",
                    "parameters": {
                        "location": "New York City"
                    }
                },
                {
                    "utterance": "Can you give me the hourly weather updates for London?",
                    "parameters": {
                        "location": "London"
                    }
                },
                {
                    "utterance": "I need the hourly weather forecast for Paris.",
                    "parameters": {
                        "location": "Paris"
                    }
                },
                {
                    "utterance": "What will the weather be like in Tokyo for the next few hours?",
                    "parameters": {
                        "location": "Tokyo"
                    }
                },
                {
                    "utterance": "Show me the hourly forecast for Sydney.",
                    "parameters": {
                        "location": "Sydney"
                    }
                },
                {
                    "utterance": "Could you provide the hourly weather details for Berlin?",
                    "parameters": {
                        "location": "Berlin"
                    }
                },
                {
                    "utterance": "What's the hourly weather prediction for San Francisco?",
                    "parameters": {
                        "location": "San Francisco"
                    }
                },
                {
                    "utterance": "I'd like to see the hourly forecast for Dubai.",
                    "parameters": {
                        "location": "Dubai"
                    }
                },
                {
                    "utterance": "Can you tell me the hourly weather for Mumbai?",
                    "parameters": {
                        "location": "Mumbai"
                    }
                },
                {
                    "utterance": "What's the hourly weather outlook for Toronto?",
                    "parameters": {
                        "location": "Toronto"
                    }
                }
            ]
        }
    ]
}