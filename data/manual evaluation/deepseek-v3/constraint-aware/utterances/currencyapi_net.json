{
    "name": "CurrencyAPI.net",
    "description": "Real-time currency conversion on 152 currencies and cryptos",
    "url": "https://rapidapi.com/currencyapinet/api/currencyapi-net/",
    "api_methods": [
        {
            "name": "Timeframe",
            "description": "Display a currency conversion's historical rates within a given timeframe",
            "url": "https://currencyapi-net.p.rapidapi.com/timeframe",
            "parameters": [
                {
                    "name": "end_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-29",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "DATE (YYYY-MM-DD)"
                    }
                },
                {
                    "name": "base",
                    "type": "STRING",
                    "description": "The currency code you want to get the historical data of. Eg, USD",
                    "default": "USD",
                    "required": true,
                    "constraints": {
                        "format": "currency code"
                    }
                },
                {
                    "name": "start_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-25",
                    "default": "",
                    "required": true,
                    "constraints": {
                        "format": "DATE (YYYY-MM-DD)"
                    }
                },
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "JSON",
                                "XML"
                            ]
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "What were the historical exchange rates for USD between January 1, 2023, and January 31, 2023?",
                    "parameters": {
                        "start_date": "2023-01-01",
                        "end_date": "2023-01-31",
                        "base": "USD"
                    }
                },
                {
                    "utterance": "Can you show me the EUR conversion rates from March 15, 2022, to March 20, 2022, in XML format?",
                    "parameters": {
                        "start_date": "2022-03-15",
                        "end_date": "2022-03-20",
                        "base": "EUR",
                        "output": "XML"
                    }
                },
                {
                    "utterance": "I need the historical rates for GBP from December 1, 2021, to December 5, 2021.",
                    "parameters": {
                        "start_date": "2021-12-01",
                        "end_date": "2021-12-05",
                        "base": "GBP"
                    }
                },
                {
                    "utterance": "Show me the JPY exchange rates between April 10, 2020, and April 15, 2020, in JSON format.",
                    "parameters": {
                        "start_date": "2020-04-10",
                        "end_date": "2020-04-15",
                        "base": "JPY",
                        "output": "JSON"
                    }
                },
                {
                    "utterance": "What were the AUD conversion rates from July 1, 2019, to July 7, 2019?",
                    "parameters": {
                        "start_date": "2019-07-01",
                        "end_date": "2019-07-07",
                        "base": "AUD"
                    }
                },
                {
                    "utterance": "Can you provide the historical rates for CAD from September 1, 2022, to September 30, 2022, in XML?",
                    "parameters": {
                        "start_date": "2022-09-01",
                        "end_date": "2022-09-30",
                        "base": "CAD",
                        "output": "XML"
                    }
                },
                {
                    "utterance": "I\u2019d like to see the CHF exchange rates between May 1, 2021, and May 10, 2021.",
                    "parameters": {
                        "start_date": "2021-05-01",
                        "end_date": "2021-05-10",
                        "base": "CHF"
                    }
                },
                {
                    "utterance": "What were the historical rates for CNY from August 1, 2020, to August 15, 2020, in JSON format?",
                    "parameters": {
                        "start_date": "2020-08-01",
                        "end_date": "2020-08-15",
                        "base": "CNY",
                        "output": "JSON"
                    }
                },
                {
                    "utterance": "Show me the INR conversion rates between November 1, 2021, and November 5, 2021.",
                    "parameters": {
                        "start_date": "2021-11-01",
                        "end_date": "2021-11-05",
                        "base": "INR"
                    }
                },
                {
                    "utterance": "Can you display the historical rates for NZD from February 1, 2023, to February 28, 2023, in XML?",
                    "parameters": {
                        "start_date": "2023-02-01",
                        "end_date": "2023-02-28",
                        "base": "NZD",
                        "output": "XML"
                    }
                }
            ]
        }
    ]
}