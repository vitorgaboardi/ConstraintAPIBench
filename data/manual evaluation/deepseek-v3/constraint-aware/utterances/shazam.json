                {
                    "name": "key",
                    "type": "STRING",
                    "description": "The key field returned from .../songs/detect or .../search endpoint",
                    "default": "484129036",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "locale",
                    "type": "STRING",
                    "description": "The language code",
                    "default": "en-US",
                    "required": false,
                    "constraints": {
                        "technical": true
                    }
                }
            ],
            "utterances": [
