{
    "name": "MDBList",
    "description": "Get Movies and TV Shows data with ratings from multiple providers in JSON format",
    "url": "https://rapidapi.com/linaspurinis/api/mdblist/",
    "api_methods": [
        {
            "name": "Search by Title",
            "description": "Search for Movies or TV Shows by title, include option year or mediatype filter to narrow your results",
            "url": "https://mdblist.p.rapidapi.com/",
            "parameters": [
                {
                    "name": "s",
                    "type": "STRING",
                    "description": "Movie or TV Show Title to search",
                    "default": "jaws",
                    "required": true,
                    "constraints": {}
                },
                {
                    "name": "l",
                    "type": "NUMBER",
                    "description": "Limit returned items.\nYou can use numbes from 1 to 100 (default is 50)",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 100,
                            "min": 1
                        },
                        "technical": true
                    }
                },
                {
                    "name": "y",
                    "type": "NUMBER",
                    "description": "Year (format YYYY, for example 2020) to limit title search (would also include previous and next year movies)",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "format": "YYYY"
                    }
                },
                {
                    "name": "m",
                    "type": "ENUM",
                    "description": "Search for only movie or tv show, leave empty for both",
                    "default": "",
                    "required": false,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "movie",
                                "tv show"
                            ]
                        }
                    }
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you find information about the movie Inception?",
                    "parameters": {
                        "s": "Inception"
                    }
                },
                {
                    "utterance": "Search for the TV show Breaking Bad from 2008.",
                    "parameters": {
                        "s": "Breaking Bad",
                        "y": 2008
                    }
                },
                {
                    "utterance": "I'm looking for details on The Matrix. Can you help?",
                    "parameters": {
                        "s": "The Matrix"
                    }
                },
                {
                    "utterance": "Find me the movie Titanic released in 1997.",
                    "parameters": {
                        "s": "Titanic",
                        "y": 1997
                    }
                },
                {
                    "utterance": "Could you search for the TV show Friends?",
                    "parameters": {
                        "s": "Friends",
                        "m": "tv show"
                    }
                },
                {
                    "utterance": "I need information about the movie The Dark Knight from 2008.",
                    "parameters": {
                        "s": "The Dark Knight",
                        "y": 2008
                    }
                },
                {
                    "utterance": "Search for the TV show Stranger Things.",
                    "parameters": {
                        "s": "Stranger Things",
                        "m": "tv show"
                    }
                },
                {
                    "utterance": "Can you find details on the movie Avatar released in 2009?",
                    "parameters": {
                        "s": "Avatar",
                        "y": 2009
                    }
                },
                {
                    "utterance": "I'm looking for the TV show Game of Thrones.",
                    "parameters": {
                        "s": "Game of Thrones",
                        "m": "tv show"
                    }
                },
                {
                    "utterance": "Find me information about the movie Jurassic Park.",
                    "parameters": {
                        "s": "Jurassic Park"
                    }
                }
            ]
        }
    ]
}