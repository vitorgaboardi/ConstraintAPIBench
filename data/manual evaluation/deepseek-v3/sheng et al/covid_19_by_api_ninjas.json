{
    "name": "Covid-19 by API-Ninjas",
    "description": "Covid-19 case count and death data for every country in the world. See more info at https://api-ninjas.com/api/covid19.",
    "url": "https://rapidapi.com/apininjas/api/covid-19-by-api-ninjas/",
    "api_methods": [
        {
            "name": "/v1/covid19",
            "description": "API Ninjas Covid19 API endpoint. Either **date** or **country** must be set.",
            "url": "https://covid-19-by-api-ninjas.p.rapidapi.com/v1/covid19",
            "parameters": [
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "type of data to retrieve. Must be either cases or deaths. If not set, cases will be used by default.",
                    "default": "",
                    "required": false
                },
                {
                    "name": "country",
                    "type": "STRING",
                    "description": "country name (case insensitive).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "date",
                    "type": "STRING",
                    "description": "date to retrieve single-day snapshot. Must be in the form of YYYY-MM-DD (e.g. 2022-01-01)",
                    "default": "2022-01-01",
                    "required": false
                },
                {
                    "name": "county",
                    "type": "STRING",
                    "description": "county name for US states (case insensitive). For United States data only. Must be used in conjunction with country (United States) and region (e.g. California).",
                    "default": "",
                    "required": false
                },
                {
                    "name": "region",
                    "type": "STRING",
                    "description": "administrative region (also known as state or province in many countries) name (case insensitive). Must be used in conjunction with country. If not set, countries with data broken down by administrative regions will return separate data for each region.",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What were the COVID-19 cases in Canada on 2022-03-15?",
                    "parameters": {
                        "country": "Canada",
                        "date": "2022-03-15",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "Get the COVID-19 deaths in the United States for California on 2022-04-01.",
                    "parameters": {
                        "country": "United States",
                        "region": "California",
                        "date": "2022-04-01",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "Retrieve the COVID-19 cases in Germany on 2022-05-10.",
                    "parameters": {
                        "country": "Germany",
                        "date": "2022-05-10",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "What were the COVID-19 deaths in India on 2022-06-20?",
                    "parameters": {
                        "country": "India",
                        "date": "2022-06-20",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "Get the COVID-19 cases in the United States for Texas on 2022-07-04.",
                    "parameters": {
                        "country": "United States",
                        "region": "Texas",
                        "date": "2022-07-04",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "Retrieve the COVID-19 deaths in France on 2022-08-15.",
                    "parameters": {
                        "country": "France",
                        "date": "2022-08-15",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "What were the COVID-19 cases in the United States for Los Angeles County, California on 2022-09-01?",
                    "parameters": {
                        "country": "United States",
                        "region": "California",
                        "county": "Los Angeles County",
                        "date": "2022-09-01",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "Get the COVID-19 deaths in Brazil on 2022-10-10.",
                    "parameters": {
                        "country": "Brazil",
                        "date": "2022-10-10",
                        "type": "deaths"
                    }
                },
                {
                    "utterance": "Retrieve the COVID-19 cases in the United States for New York on 2022-11-11.",
                    "parameters": {
                        "country": "United States",
                        "region": "New York",
                        "date": "2022-11-11",
                        "type": "cases"
                    }
                },
                {
                    "utterance": "What were the COVID-19 deaths in Japan on 2022-12-25?",
                    "parameters": {
                        "country": "Japan",
                        "date": "2022-12-25",
                        "type": "deaths"
                    }
                }
            ]
        }
    ]
}