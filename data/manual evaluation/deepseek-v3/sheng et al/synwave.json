{
    "name": "SynWave",
    "description": "A data storage management API offering the following features:\n\n",
    "url": "https://rapidapi.com/rpi4gx/api/synwave/",
    "api_methods": [
        {
            "name": "Upload a new file",
            "description": "Upload a new file.",
            "url": "https://synwave.p.rapidapi.com/v1/upload",
            "parameters": [
                {
                    "name": "file",
                    "type": "BINARY",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "auth_password",
                    "type": "STRING",
                    "description": "Password to protect this file. auth_username parameter is required if auth_password is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "expiration_seconds",
                    "type": "NUMBER",
                    "description": "Number of seconds before the file becomes unavailable.\n",
                    "default": 3600,
                    "required": false
                },
                {
                    "name": "auth_username",
                    "type": "STRING",
                    "description": "Username to protect this file. auth_password parameter is required if auth_username is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "download_limit",
                    "type": "NUMBER",
                    "description": "Number of downloads before the file becomes unavailable.\n",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Upload my document to SynWave.",
                    "parameters": {
                        "file": "my document"
                    }
                },
                {
                    "utterance": "Upload the report file with a password 'secure123' and username 'admin'.",
                    "parameters": {
                        "file": "the report file",
                        "auth_password": "secure123",
                        "auth_username": "admin"
                    }
                },
                {
                    "utterance": "Upload the image file and set it to expire in 7200 seconds.",
                    "parameters": {
                        "file": "the image file",
                        "expiration_seconds": 7200
                    }
                },
                {
                    "utterance": "Upload the video file with a download limit of 5.",
                    "parameters": {
                        "file": "the video file",
                        "download_limit": 5
                    }
                },
                {
                    "utterance": "Upload the presentation file with a password 'pass123' and set it to expire in 1800 seconds.",
                    "parameters": {
                        "file": "the presentation file",
                        "auth_password": "pass123",
                        "expiration_seconds": 1800
                    }
                },
                {
                    "utterance": "Upload the spreadsheet file with a username 'user1' and a download limit of 10.",
                    "parameters": {
                        "file": "the spreadsheet file",
                        "auth_username": "user1",
                        "download_limit": 10
                    }
                },
                {
                    "utterance": "Upload the PDF file with a password 'secret' and username 'manager' and set it to expire in 3600 seconds.",
                    "parameters": {
                        "file": "the PDF file",
                        "auth_password": "secret",
                        "auth_username": "manager",
                        "expiration_seconds": 3600
                    }
                },
                {
                    "utterance": "Upload the text file with a download limit of 3 and set it to expire in 5400 seconds.",
                    "parameters": {
                        "file": "the text file",
                        "download_limit": 3,
                        "expiration_seconds": 5400
                    }
                },
                {
                    "utterance": "Upload the audio file with a password 'audio123' and a download limit of 7.",
                    "parameters": {
                        "file": "the audio file",
                        "auth_password": "audio123",
                        "download_limit": 7
                    }
                },
                {
                    "utterance": "Upload the archive file with a username 'archiver' and set it to expire in 900 seconds.",
                    "parameters": {
                        "file": "the archive file",
                        "auth_username": "archiver",
                        "expiration_seconds": 900
                    }
                }
            ]
        }
    ]
}