{
    "name": "MoviesDatabase",
    "description": "MoviesDatabase provides complete and updated data for over 9 million titles ( movies, series and episodes) and 11 million actors / crew and cast members",
    "url": "https://rapidapi.com/SAdrian/api/moviesdatabase/",
    "api_methods": [
        {
            "name": "/titles/x/upcoming",
            "description": " ",
            "url": "https://moviesdatabase.p.rapidapi.com/titles/x/upcoming",
            "parameters": [
                {
                    "name": "titleType",
                    "type": "STRING",
                    "description": "Filter by type of title",
                    "default": "",
                    "required": false
                },
                {
                    "name": "year",
                    "type": "NUMBER",
                    "description": "Year filter ex: ?year=2022",
                    "default": "",
                    "required": false
                },
                {
                    "name": "genre",
                    "type": "STRING",
                    "description": "Year filter ex: ?genre=Drama",
                    "default": "",
                    "required": false
                },
                {
                    "name": "info",
                    "type": "STRING",
                    "description": "Info type structure (default: mini-info) -> base_info / mini_info / image / ...",
                    "default": "",
                    "required": false
                },
                {
                    "name": "endYear",
                    "type": "NUMBER",
                    "description": "Year range filter -to- ex: ?endYear=2020",
                    "default": "",
                    "required": false
                },
                {
                    "name": "limit",
                    "type": "NUMBER",
                    "description": "Number of titles per page (default: 10) -> 10 max ",
                    "default": "",
                    "required": false
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "Add sorting (incr, decr) -> year.incr / year.decr",
                    "default": "",
                    "required": false
                },
                {
                    "name": "page",
                    "type": "STRING",
                    "description": "Page number",
                    "default": "",
                    "required": false
                },
                {
                    "name": "startYear",
                    "type": "NUMBER",
                    "description": "Year range filter -from- ex: ?startYear=2020",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What are the upcoming drama movies in 2023?",
                    "parameters": {
                        "titleType": "movie",
                        "year": 2023,
                        "genre": "Drama"
                    }
                },
                {
                    "utterance": "Show me the upcoming series from 2020 to 2022.",
                    "parameters": {
                        "titleType": "series",
                        "startYear": 2020,
                        "endYear": 2022
                    }
                },
                {
                    "utterance": "List the upcoming comedy movies in 2024 with base info.",
                    "parameters": {
                        "titleType": "movie",
                        "year": 2024,
                        "genre": "Comedy",
                        "info": "base_info"
                    }
                },
                {
                    "utterance": "What are the upcoming action movies sorted by year in descending order?",
                    "parameters": {
                        "titleType": "movie",
                        "genre": "Action",
                        "sort": "year.decr"
                    }
                },
                {
                    "utterance": "Show me the first 5 upcoming titles for 2023.",
                    "parameters": {
                        "year": 2023,
                        "limit": 5
                    }
                },
                {
                    "utterance": "What are the upcoming horror movies from 2021 to 2023?",
                    "parameters": {
                        "titleType": "movie",
                        "genre": "Horror",
                        "startYear": 2021,
                        "endYear": 2023
                    }
                },
                {
                    "utterance": "List the upcoming movies in 2022 with mini info.",
                    "parameters": {
                        "titleType": "movie",
                        "year": 2022,
                        "info": "mini_info"
                    }
                },
                {
                    "utterance": "Show me the upcoming titles on page 3 for 2023.",
                    "parameters": {
                        "year": 2023,
                        "page": "3"
                    }
                },
                {
                    "utterance": "What are the upcoming romance movies sorted by year in ascending order?",
                    "parameters": {
                        "titleType": "movie",
                        "genre": "Romance",
                        "sort": "year.incr"
                    }
                },
                {
                    "utterance": "List the upcoming sci-fi movies in 2025 with image info.",
                    "parameters": {
                        "titleType": "movie",
                        "year": 2025,
                        "genre": "Sci-Fi",
                        "info": "image"
                    }
                }
            ]
        }
    ]
}