{
    "name": "Postal Ninja",
    "description": "Official Postal Ninja tracking API for developers",
    "url": "https://rapidapi.com/PostalNinja/api/postal-ninja/",
    "api_methods": [
        {
            "name": "createTrack",
            "description": "Returns ID of newly created package (status=CREATED) or stored ID if this tracking number already exists (status=FOUND).",
            "url": "https://postal-ninja.p.rapidapi.com/v1/track",
            "parameters": [
                {
                    "name": "trackCode",
                    "type": "STRING",
                    "description": "Tracking number",
                    "default": "",
                    "required": true
                },
                {
                    "name": "ds",
                    "type": "STRING",
                    "description": "Carrier ID (optional)",
                    "default": "",
                    "required": false
                },
                {
                    "name": "destCountry",
                    "type": "STRING",
                    "description": "Optional destination country code (ISO 3166-1 alpha-2)",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Track the package with the tracking number 123456789.",
                    "parameters": {
                        "trackCode": "123456789"
                    }
                },
                {
                    "utterance": "Can you create a tracking entry for the package with the code ABC123XYZ?",
                    "parameters": {
                        "trackCode": "ABC123XYZ"
                    }
                },
                {
                    "utterance": "I need to track a package using the tracking number 987654321 and the carrier ID USPS.",
                    "parameters": {
                        "trackCode": "987654321",
                        "ds": "USPS"
                    }
                },
                {
                    "utterance": "Please track the package with the number XYZ987654 and the destination country code US.",
                    "parameters": {
                        "trackCode": "XYZ987654",
                        "destCountry": "US"
                    }
                },
                {
                    "utterance": "Create a tracking entry for the package with the tracking number 1122334455 and the carrier ID FedEx.",
                    "parameters": {
                        "trackCode": "1122334455",
                        "ds": "FedEx"
                    }
                },
                {
                    "utterance": "Track the package with the number 9988776655 and the destination country code CA.",
                    "parameters": {
                        "trackCode": "9988776655",
                        "destCountry": "CA"
                    }
                },
                {
                    "utterance": "I need to track a package with the tracking number 123ABC456 and the carrier ID DHL.",
                    "parameters": {
                        "trackCode": "123ABC456",
                        "ds": "DHL"
                    }
                },
                {
                    "utterance": "Please create a tracking entry for the package with the number 789XYZ123 and the destination country code GB.",
                    "parameters": {
                        "trackCode": "789XYZ123",
                        "destCountry": "GB"
                    }
                },
                {
                    "utterance": "Track the package with the tracking number 456DEF789 and the carrier ID UPS.",
                    "parameters": {
                        "trackCode": "456DEF789",
                        "ds": "UPS"
                    }
                },
                {
                    "utterance": "Can you track the package with the number 321CBA654 and the destination country code AU?",
                    "parameters": {
                        "trackCode": "321CBA654",
                        "destCountry": "AU"
                    }
                }
            ]
        }
    ]
}