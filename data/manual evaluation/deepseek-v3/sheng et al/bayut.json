{
    "name": "Bayut",
    "description": "This API helps to query real estate in UAE to create a real estate site/application",
    "url": "https://rapidapi.com/apidojo/api/bayut/",
    "api_methods": [
        {
            "name": "properties/list",
            "description": "List properties with options and filters",
            "url": "https://bayut.p.rapidapi.com/properties/list",
            "parameters": [
                {
                    "name": "locationExternalIDs",
                    "type": "STRING",
                    "description": "The location/externalID fields returned in .../auto-complete endpoint. Separated by comma for multiple locations. Ex : 5002,6020",
                    "default": "5002,6020",
                    "required": true
                },
                {
                    "name": "hasFloorPlan",
                    "type": "BOOLEAN",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "areaMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "purpose",
                    "type": "STRING",
                    "description": "One of the following : for-rent|for-sale",
                    "default": "for-rent",
                    "required": false
                },
                {
                    "name": "hasVideo",
                    "type": "BOOLEAN",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "hasPanorama",
                    "type": "BOOLEAN",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "furnishingStatus",
                    "type": "STRING",
                    "description": "One of the following : unfurnished|furnished",
                    "default": "",
                    "required": false
                },
                {
                    "name": "bathsMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "sort",
                    "type": "STRING",
                    "description": "One of the following : price-desc|price-asc|city-level-score|date-desc|verified-score",
                    "default": "city-level-score",
                    "required": false
                },
                {
                    "name": "bathsMax",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "agencyExternalIDs",
                    "type": "STRING",
                    "description": "The externalID fields returned in .../agencies/list endpoint. Separated by comma for multiple agencies. Ex : 7737,5002",
                    "default": "",
                    "required": false
                },
                {
                    "name": "hitsPerPage",
                    "type": "NUMBER",
                    "description": "For paging purpose",
                    "default": "25",
                    "required": false
                },
                {
                    "name": "roomsMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "roomsMax",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "categoryExternalID",
                    "type": "NUMBER",
                    "description": "Apartment -> 4|Townhouses -> 16|Villas -> 3|Penthouses -> 18|Hotel Apartments -> 21|Villa Compound -> 19|Residential Plot -> 14|Residential Floor -> 12|Residential Building -> 17|Office -> 5|Shop -> 6|Warehouse -> 7|Labour camp -> 9|Commercial Villa -> 25|Bulk Units -> 20|Commercial Plot -> 15|Commercial Floor -> 13|Commercial Building -> 10|Factory -> 8|Industrial Land -> 22|Mixed Use Land -> 23|Showroom -> 24|Other Commercial -> 11",
                    "default": "4",
                    "required": false
                },
                {
                    "name": "rentFrequency",
                    "type": "STRING",
                    "description": "One of the following : monthly|yearly|weekly|daily",
                    "default": "monthly",
                    "required": false
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "One of the following : en|ar",
                    "default": "en",
                    "required": false
                },
                {
                    "name": "priceMin",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "priceMax",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "areaMax",
                    "type": "NUMBER",
                    "description": "",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me properties for rent in Dubai with a minimum of 2 bedrooms.",
                    "parameters": {
                        "locationExternalIDs": "5002",
                        "purpose": "for-rent",
                        "roomsMin": 2
                    }
                },
                {
                    "utterance": "Find furnished apartments for sale in Abu Dhabi with a maximum price of 1,000,000 AED.",
                    "parameters": {
                        "locationExternalIDs": "6020",
