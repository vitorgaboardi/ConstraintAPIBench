{
    "name": "Tasty",
    "description": "API to query data about recipe, plan, ingredients, etc... as on official site",
    "url": "https://rapidapi.com/apidojo/api/tasty/",
    "api_methods": [
        {
            "name": "tips/list",
            "description": "This endpoint is used to load tips (reviews)",
            "url": "https://tasty.p.rapidapi.com/tips/list",
            "parameters": [
                {
                    "name": "id",
                    "type": "NUMBER",
                    "description": "The value of recipe id returned in .../recipes/list .../feeds/list .../recipes/list-similarities endpoints",
                    "default": "3562",
                    "required": true
                },
                {
                    "name": "size",
                    "type": "NUMBER",
                    "description": "Number of items returned per response",
                    "default": "30",
                    "required": false
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "The offset of items to be ignored in response for paging",
                    "default": "0",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you show me the tips for recipe id 1234?",
                    "parameters": {
                        "id": 1234
                    }
                },
                {
                    "utterance": "What are the reviews for recipe id 5678?",
                    "parameters": {
                        "id": 5678
                    }
                },
                {
                    "utterance": "Get the first 10 tips for recipe id 9101.",
                    "parameters": {
                        "id": 9101,
                        "size": 10
                    }
                },
                {
                    "utterance": "Show me the tips for recipe id 1121, starting from the 5th review.",
                    "parameters": {
                        "id": 1121,
                        "from": 5
                    }
                },
                {
                    "utterance": "What are the latest 20 tips for recipe id 3141?",
                    "parameters": {
                        "id": 3141,
                        "size": 20
                    }
                },
                {
                    "utterance": "Can you fetch the tips for recipe id 5161, skipping the first 15 reviews?",
                    "parameters": {
                        "id": 5161,
                        "from": 15
                    }
                },
                {
                    "utterance": "Get all the tips for recipe id 7181.",
                    "parameters": {
                        "id": 7181
                    }
                },
                {
                    "utterance": "Show me the first 5 tips for recipe id 9202.",
                    "parameters": {
                        "id": 9202,
                        "size": 5
                    }
                },
                {
                    "utterance": "What are the tips for recipe id 1222, starting from the 10th review?",
                    "parameters": {
                        "id": 1222,
                        "from": 10
                    }
                },
                {
                    "utterance": "Can you get the tips for recipe id 3242, with a limit of 15 reviews?",
                    "parameters": {
                        "id": 3242,
                        "size": 15
                    }
                }
            ]
        }
    ]
}