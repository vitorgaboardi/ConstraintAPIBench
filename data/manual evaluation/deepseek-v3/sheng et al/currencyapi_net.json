{
    "name": "CurrencyAPI.net",
    "description": "Real-time currency conversion on 152 currencies and cryptos",
    "url": "https://rapidapi.com/currencyapinet/api/currencyapi-net/",
    "api_methods": [
        {
            "name": "Timeframe",
            "description": "Display a currency conversion's historical rates within a given timeframe",
            "url": "https://currencyapi-net.p.rapidapi.com/timeframe",
            "parameters": [
                {
                    "name": "end_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-29",
                    "default": "",
                    "required": true
                },
                {
                    "name": "base",
                    "type": "STRING",
                    "description": "The currency code you want to get the historical data of. Eg, USD",
                    "default": "USD",
                    "required": true
                },
                {
                    "name": "start_date",
                    "type": "DATE (YYYY-MM-DD)",
                    "description": "Display the start date you wish to get the historical data from. Eg 2010-12-25",
                    "default": "",
                    "required": true
                },
                {
                    "name": "output",
                    "type": "STRING",
                    "description": "The output of the response. Either JSON or XML",
                    "default": "JSON",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What are the historical exchange rates for USD from 2023-01-01 to 2023-01-31?",
                    "parameters": {
                        "start_date": "2023-01-01",
                        "end_date": "2023-01-31",
                        "base": "USD"
                    }
                },
                {
                    "utterance": "Can you show me the historical rates for EUR between 2022-12-01 and 2022-12-15?",
                    "parameters": {
                        "start_date": "2022-12-01",
                        "end_date": "2022-12-15",
                        "base": "EUR"
                    }
                },
                {
                    "utterance": "Get the historical currency conversion data for GBP from 2021-05-10 to 2021-05-20.",
                    "parameters": {
                        "start_date": "2021-05-10",
                        "end_date": "2021-05-20",
                        "base": "GBP"
                    }
                },
                {
                    "utterance": "I need the historical exchange rates for JPY from 2020-03-01 to 2020-03-31.",
                    "parameters": {
                        "start_date": "2020-03-01",
                        "end_date": "2020-03-31",
                        "base": "JPY"
                    }
                },
                {
