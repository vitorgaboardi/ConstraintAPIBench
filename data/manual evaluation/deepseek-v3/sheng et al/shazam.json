{
    "name": "Shazam",
    "description": "Identify any song. Discover, artists, lyrics, videos & playlists to create a song detector site/application such as shazam.com",
    "url": "https://rapidapi.com/apidojo/api/shazam/",
    "api_methods": [
        {
            "name": "songs/list-recommendations",
            "description": "List related ones to a specific song.",
            "url": "https://shazam.p.rapidapi.com/songs/list-recommendations",
            "parameters": [
                {
                    "name": "key",
                    "type": "STRING",
                    "description": "The key field returned from .../songs/detect or .../search endpoint",
                    "default": "484129036",
                    "required": true
                },
                {
                    "name": "locale",
                    "type": "STRING",
                    "description": "The language code",
                    "default": "en-US",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you list songs related to the track with key 123456789?",
                    "parameters": {
                        "key": "123456789"
                    }
                },
                {
                    "utterance": "Show me recommendations for the song with key 987654321 in Spanish.",
                    "parameters": {
                        "key": "987654321",
                        "locale": "es-ES"
                    }
                },
                {
                    "utterance": "What are some similar songs to the track with key 555555555?",
                    "parameters": {
                        "key": "555555555"
                    }
                },
                {
                    "utterance": "List related songs for the track with key 111222333 in French.",
                    "parameters": {
                        "key": "111222333",
                        "locale": "fr-FR"
                    }
                },
                {
                    "utterance": "Can you recommend songs similar to the one with key 999888777?",
                    "parameters": {
                        "key": "999888777"
                    }
                },
                {
                    "utterance": "Find related songs for the track with key 444555666 in German.",
                    "parameters": {
                        "key": "444555666",
                        "locale": "de-DE"
                    }
                },
                {
                    "utterance": "What songs are related to the track with key 777888999?",
                    "parameters": {
                        "key": "777888999"
                    }
                },
                {
                    "utterance": "Show me similar songs to the track with key 222333444 in Italian.",
                    "parameters": {
                        "key": "222333444",
                        "locale": "it-IT"
                    }
                },
                {
                    "utterance": "List recommendations for the song with key 666777888 in Japanese.",
                    "parameters": {
                        "key": "666777888",
                        "locale": "ja-JP"
                    }
                },
                {
                    "utterance": "Can you find related songs for the track with key 333444555 in Portuguese?",
                    "parameters": {
                        "key": "333444555",
