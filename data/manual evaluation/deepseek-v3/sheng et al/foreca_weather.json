{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "url": "https://rapidapi.com/foreca-ltd-foreca-ltd-default/api/foreca-weather/",
    "api_methods": [
        {
            "name": "Hourly",
            "description": "Hourly forecast.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/hourly/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "history",
                    "type": "BOOLEAN",
                    "description": "Whether to include 24 hours of past data.",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tz",
                    "type": "STRING",
                    "description": "Time zone in response (IANA time zone database names)",
                    "default": "Europe/London",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What is the hourly weather forecast for New York?",
                    "parameters": {
                        "location": "New York"
                    }
                },
                {
                    "utterance": "Get the hourly weather forecast for London with temperature in Fahrenheit.",
                    "parameters": {
                        "location": "London",
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "What is the hourly weather forecast for Paris including past 24 hours of data?",
                    "parameters": {
                        "location": "Paris",
                        "history": true
                    }
                },
                {
                    "utterance": "Get the hourly weather forecast for Berlin with wind speed in knots.",
                    "parameters": {
                        "location": "Berlin",
                        "windunit": "KT"
                    }
                },
                {
                    "utterance": "What is the hourly weather forecast for Tokyo for the next 12 hours?",
                    "parameters": {
                        "location": "Tokyo",
                        "periods": 12
                    }
                },
                {
                    "utterance": "Get the hourly weather forecast for Sydney in the Australia/Sydney time zone.",
                    "parameters": {
                        "location": "Sydney",
                        "tz": "Australia/Sydney"
                    }
                },
                {
                    "utterance": "What is the hourly weather forecast for Los Angeles at an altitude of 100 meters?",
                    "parameters": {
                        "location": "Los Angeles",
                        "alt": 100
                    }
                },
                {
                    "utterance": "Get the hourly weather forecast for Moscow with the full dataset.",
                    "parameters": {
                        "location": "Moscow",
                        "dataset": "full"
                    }
                },
                {
                    "utterance": "What is the hourly weather forecast for Dubai for the next 8 hours?",
                    "parameters": {
                        "location": "Dubai",
                        "periods": 8
                    }
                },
                {
                    "utterance": "Get the hourly weather forecast for Toronto with temperature in Celsius.",
                    "parameters": {
                        "location": "Toronto",
                        "tempunit": "C"
                    }
                }
            ]
        }
    ]
}