{
    "name": "Referential",
    "description": "The fastest API to access countries, states, cities, continents, dial and zip codes in up to 20 languages. A collection of data APIs to support forms, signups, search and lookup. Our endpoints are optimized for speed and size. Our data is regularly maintained and comes from International Standardization bodies, the United Nations, government surveys and GIS datasets. We do not scrape WikiPedia etc",
    "url": "https://rapidapi.com/referential/api/referential/",
    "api_methods": [
        {
            "name": "Country by iso code",
            "description": "Get country by 2 letter iso code localized to a given language",
            "url": "https://referential.p.rapidapi.com/v1/country/{iso_code}",
            "parameters": [
                {
                    "name": "iso_code",
                    "type": "string",
                    "description": "2 letter iso country code. ",
                    "default": "US",
                    "required": true
                },
                {
                    "name": "fields",
                    "type": "STRING",
                    "description": "Comma deliminated list of fields to return in response",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "2 letter iso language code to localize country name",
                    "default": "en",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Get details for the country with the ISO code 'FR'.",
                    "parameters": {
                        "iso_code": "FR"
                    }
                },
                {
                    "utterance": "What is the localized name of the country with the ISO code 'DE' in German?",
                    "parameters": {
                        "iso_code": "DE",
                        "lang": "de"
                    }
                },
                {
                    "utterance": "Retrieve the capital and population for the country with the ISO code 'JP'.",
                    "parameters": {
                        "iso_code": "JP",
                        "fields": "capital,population"
                    }
                },
                {
                    "utterance": "Find the country name in Spanish for the ISO code 'ES'.",
                    "parameters": {
                        "iso_code": "ES",
                        "lang": "es"
                    }
                },
                {
                    "utterance": "Get the currency and timezone for the country with the ISO code 'IN'.",
                    "parameters": {
                        "iso_code": "IN",
                        "fields": "currency,timezone"
                    }
                },
                {
                    "utterance": "What is the country name in French for the ISO code 'CA'?",
                    "parameters": {
                        "iso_code": "CA",
                        "lang": "fr"
                    }
                },
                {
                    "utterance": "Retrieve the area and population for the country with the ISO code 'BR'.",
                    "parameters": {
                        "iso_code": "BR",
                        "fields": "area,population"
                    }
                },
                {
                    "utterance": "Get the country name in Italian for the ISO code 'IT'.",
                    "parameters": {
                        "iso_code": "IT",
                        "lang": "it"
                    }
                },
                {
                    "utterance": "Find the official language and flag for the country with the ISO code 'MX'.",
                    "parameters": {
                        "iso_code": "MX",
                        "fields": "official_language,flag"
                    }
                },
                {
                    "utterance": "What is the country name in Chinese for the ISO code 'CN'?",
                    "parameters": {
                        "iso_code": "CN",
                        "lang": "zh"
                    }
                }
            ]
        },
        {
            "name": "Languages",
            "description": "List the world's languages translated into 20 languages",
            "url": "https://referential.p.rapidapi.com/v1/lang",
            "parameters": [
                {
                    "name": "lang_3",
                    "type": "STRING",
                    "description": "Filter by 3 letter ISO language code",
                    "default": "",
                    "required": false
                },
                {
                    "name": "iso_a2",
                    "type": "STRING",
                    "description": "Filter on 2 letter country code",
                    "default": "",
                    "required": false
                },
                {
                    "name": "fields",
                    "type": "STRING",
                    "description": "Select fields to return in result [flag,iso_a2,lang_3,lang]",
                    "default": "iso_a2,lang_3,flag",
                    "required": false
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "Returns a language translated into lang language",
                    "default": "en",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "List all languages in French.",
                    "parameters": {
                        "lang": "fr"
                    }
                },
                {
                    "utterance": "Get the languages spoken in the United States.",
                    "parameters": {
                        "iso_a2": "US"
                    }
                },
                {
                    "utterance": "Show the languages with their flags and ISO codes in Spanish.",
                    "parameters": {
                        "fields": "flag,iso_a2,lang_3,lang",
                        "lang": "es"
                    }
                },
                {
                    "utterance": "Find the language with the ISO code 'fra'.",
                    "parameters": {
                        "lang_3": "fra"
                    }
                },
                {
                    "utterance": "List all languages in German, including their flags.",
                    "parameters": {
                        "fields": "flag,lang",
                        "lang": "de"
                    }
                },
                {
                    "utterance": "Get the languages spoken in Canada, translated into Japanese.",
                    "parameters": {
                        "iso_a2": "CA",
                        "lang": "ja"
                    }
                },
                {
                    "utterance": "Show the languages with their ISO codes in Italian.",
                    "parameters": {
                        "fields": "iso_a2,lang",
                        "lang": "it"
                    }
                },
                {
                    "utterance": "Find the language with the ISO code 'spa' and display its flag.",
                    "parameters": {
                        "lang_3": "spa",
                        "fields": "flag,lang_3"
                    }
                },
                {
                    "utterance": "List all languages in Portuguese, including their ISO codes and flags.",
                    "parameters": {
                        "fields": "iso_a2,lang_3,flag,lang",
                        "lang": "pt"
                    }
                },
                {
                    "utterance": "Get the languages spoken in Australia, translated into Chinese.",
                    "parameters": {
                        "iso_a2": "AU",
                        "lang": "zh"
                    }
                }
            ]
        }
    ]
}