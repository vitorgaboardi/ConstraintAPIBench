{
    "name": "Netflix_v2",
    "description": "Netflix data API provides details, stats and information of TV shows, movies, series, documentaries and more.",
    "api_methods": [
        {
            "name": "Search",
            "description": "Search",
            "parameters": [
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "Search Query",
                    "default": "stranger",
                    "required": true
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "",
                    "default": "en",
                    "required": false
                },
                {
                    "name": "limit_titles",
                    "type": "NUMBER",
                    "description": "Title Limit",
                    "default": "50",
                    "required": false
                },
                {
                    "name": "limit_suggestions",
                    "type": "NUMBER",
                    "description": "Suggestion Limit",
                    "default": "20",
                    "required": false
                },
                {
                    "name": "offset",
                    "type": "NUMBER",
                    "description": "Offset",
                    "default": "0",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you find me the top 10 titles related to 'Breaking Bad' in English?",
                    "parameters": {
                        "query": "Breaking Bad",
                        "lang": "en",
                        "limit_titles": 10
                    }
                },
                {
                    "utterance": "Look up documentaries about climate change and show me 5 suggestions.",
                    "parameters": {
                        "query": "climate change",
                        "limit_suggestions": 5
                    }
                },
                {
                    "utterance": "I want to see movies similar to 'Inception'; get me 15 titles starting from the 5th result.",
                    "parameters": {
                        "query": "Inception",
                        "limit_titles": 15,
                        "offset": 5
                    }
                },
                {
                    "utterance": "Search for Spanish language TV shows about cooking and list 8 of them.",
                    "parameters": {
                        "query": "cooking",
                        "lang": "es",
                        "limit_titles": 8
                    }
                },
                {
                    "utterance": "Find me 20 thriller series and also provide 10 related suggestions.",
                    "parameters": {
                        "query": "thriller",
                        "limit_titles": 20,
                        "limit_suggestions": 10
                    }
                },
                {
                    "utterance": "Could you get details on the latest 12 superhero movies available?",
                    "parameters": {
                        "query": "superhero",
                        "limit_titles": 12
                    }
                },
                {
                    "utterance": "Show me 7 comedy shows starting from the 3rd title in the list.",
                    "parameters": {
                        "query": "comedy",
                        "limit_titles": 7,
                        "offset": 2
                    }
                },
                {
                    "utterance": "I\u2019m interested in Japanese anime series; find 10 titles and 5 suggestions for me.",
                    "parameters": {
                        "query": "anime",
                        "lang": "ja",
                        "limit_titles": 10,
                        "limit_suggestions": 5
                    }
                },
                {
                    "utterance": "Get me a list of 6 romantic movies in French with some suggestions as well.",
                    "parameters": {
                        "query": "romantic",
                        "lang": "fr",
                        "limit_titles": 6,
                        "limit_suggestions": 4
                    }
                },
                {
                    "utterance": "Search for documentaries on space exploration and show me 10 titles starting from the 10th result.",
                    "parameters": {
                        "query": "space exploration",
                        "limit_titles": 10,
                        "offset": 9
                    }
                }
            ]
        }
    ]
}