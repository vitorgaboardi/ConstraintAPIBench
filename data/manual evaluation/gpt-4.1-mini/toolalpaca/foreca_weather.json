{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "api_methods": [
        {
            "name": "Daily",
            "description": "Daily forecast.",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you check the 5-day weather forecast for Tokyo and tell me the temperature in Fahrenheit?",
                    "parameters": {
                        "location": "Tokyo",
                        "periods": 5,
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "I need the daily weather report for Denver at 1600 meters altitude, including wind speed in kilometers per hour.",
                    "parameters": {
                        "location": "Denver",
                        "alt": 1600,
                        "windunit": "KMH"
                    }
                },
                {
                    "utterance": "Give me the full daily forecast for Paris for the next 7 days with temperatures in Celsius.",
                    "parameters": {
                        "location": "Paris",
                        "periods": 7,
                        "tempunit": "C",
                        "dataset": "full"
                    }
                },
                {
                    "utterance": "What\u2019s the weather outlook for New York City over the next 3 days? Please include wind speed in meters per second.",
                    "parameters": {
                        "location": "New York City",
                        "periods": 3,
                        "windunit": "MS"
                    }
                },
                {
                    "utterance": "Fetch the daily weather forecast for Sydney for 10 days, and show temperatures in Celsius and wind speed in miles per hour.",
                    "parameters": {
                        "location": "Sydney",
                        "periods": 10,
                        "tempunit": "C",
                        "windunit": "MPH"
                    }
                },
                {
                    "utterance": "I want to see the weather forecast for London for the next week, including altitude set at 35 meters.",
                    "parameters": {
                        "location": "London",
                        "periods": 7,
                        "alt": 35
                    }
                },
                {
                    "utterance": "Please provide the daily weather details for Rio de Janeiro with temperature in Fahrenheit and wind speed in kilometers per hour for 6 days.",
                    "parameters": {
                        "location": "Rio de Janeiro",
                        "periods": 6,
                        "tempunit": "F",
                        "windunit": "KMH"
                    }
                },
                {
                    "utterance": "Show me the full daily weather forecast for Cape Town for 8 days, using default units but with altitude set to 50 meters.",
                    "parameters": {
                        "location": "Cape Town",
                        "periods": 8,
                        "alt": 50,
                        "dataset": "full"
                    }
                },
                {
                    "utterance": "Could you get the weather forecast for Moscow for the next 4 days, with temperatures in Celsius and wind speed in meters per second?",
                    "parameters": {
                        "location": "Moscow",
                        "periods": 4,
                        "tempunit": "C",
                        "windunit": "MS"
                    }
                },
                {
                    "utterance": "Retrieve the daily weather forecast for Mumbai for 12 days, showing temperature in Celsius and wind speed in miles per hour.",
                    "parameters": {
                        "location": "Mumbai",
                        "periods": 12,
                        "tempunit": "C",
                        "windunit": "MPH"
                    }
                }
            ]
        }
    ]
}