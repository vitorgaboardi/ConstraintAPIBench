{
    "name": "Netflix_v2",
    "description": "Netflix data API provides details, stats and information of TV shows, movies, series, documentaries and more.",
    "api_methods": [
        {
            "name": "Search",
            "description": "Search",
            "parameters": [
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "Search Query",
                    "default": "stranger",
                    "required": true
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "",
                    "default": "en",
                    "required": false
                },
                {
                    "name": "limit_titles",
                    "type": "NUMBER",
                    "description": "Title Limit",
                    "default": "50",
                    "required": false
                },
                {
                    "name": "limit_suggestions",
                    "type": "NUMBER",
                    "description": "Suggestion Limit",
                    "default": "20",
                    "required": false
                },
                {
                    "name": "offset",
                    "type": "NUMBER",
                    "description": "Offset",
                    "default": "0",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Search for the TV show Stranger Things on Netflix.",
                    "parameters": {
                        "query": "Stranger Things"
                    }
                },
                {
                    "utterance": "Find movies related to The Crown with a limit of 10 titles.",
                    "parameters": {
                        "query": "The Crown",
                        "limit_titles": 10
                    }
                },
                {
                    "utterance": "Show me documentaries about climate change in Spanish.",
                    "parameters": {
                        "query": "climate change",
                        "lang": "es"
                    }
                },
                {
                    "utterance": "Search for comedy series with a suggestion limit of 5.",
                    "parameters": {
                        "query": "comedy",
                        "limit_suggestions": 5
                    }
                },
                {
                    "utterance": "Find thriller movies starting from the 10th result.",
                    "parameters": {
                        "query": "thriller",
                        "offset": 10
                    }
                },
                {
                    "utterance": "Look for romantic movies in French with a title limit of 15.",
                    "parameters": {
                        "query": "romantic",
                        "lang": "fr",
                        "limit_titles": 15
                    }
                },
                {
                    "utterance": "Search for sci-fi shows with 30 titles and 10 suggestions.",
                    "parameters": {
                        "query": "sci-fi",
                        "limit_titles": 30,
                        "limit_suggestions": 10
                    }
                },
                {
                    "utterance": "Find horror movies in German language.",
                    "parameters": {
                        "query": "horror",
                        "lang": "de"
                    }
                },
                {
                    "utterance": "Search for animated movies with an offset of 20.",
                    "parameters": {
                        "query": "animated",
                        "offset": 20
                    }
                },
                {
                    "utterance": "Look for crime documentaries with 25 titles and 15 suggestions in English.",
                    "parameters": {
                        "query": "crime documentaries",
                        "limit_titles": 25,
                        "limit_suggestions": 15,
                        "lang": "en"
                    }
                }
            ]
        }
    ]
}