{
    "name": "Tasty",
    "description": "API to query data about recipe, plan, ingredients, etc... as on official site",
    "api_methods": [
        {
            "name": "tips/list",
            "description": "This endpoint is used to load tips (reviews)",
            "parameters": [
                {
                    "name": "id",
                    "type": "NUMBER",
                    "description": "The value of recipe id returned in .../recipes/list .../feeds/list .../recipes/list-similarities endpoints",
                    "default": "3562",
                    "required": true
                },
                {
                    "name": "size",
                    "type": "NUMBER",
                    "description": "Number of items returned per response",
                    "default": "30",
                    "required": false
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "The offset of items to be ignored in response for paging",
                    "default": "0",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me tips for the recipe with id 3562.",
                    "parameters": {
                        "id": 3562
                    }
                },
                {
                    "utterance": "I want to see 10 tips for recipe id 1024.",
                    "parameters": {
                        "id": 1024,
                        "size": 10
                    }
                },
                {
                    "utterance": "Get tips starting from the 5th tip for recipe id 7890.",
                    "parameters": {
                        "id": 7890,
                        "from": 5
                    }
                },
                {
                    "utterance": "List 15 tips for recipe id 4500 starting from the 10th tip.",
                    "parameters": {
                        "id": 4500,
                        "size": 15,
                        "from": 10
                    }
                },
                {
                    "utterance": "Can you provide tips for recipe id 1234 with the default number of tips?",
                    "parameters": {
                        "id": 1234
                    }
                },
                {
                    "utterance": "I need 5 tips for recipe id 5678 starting from the beginning.",
                    "parameters": {
                        "id": 5678,
                        "size": 5,
                        "from": 0
                    }
                },
                {
                    "utterance": "Show me tips for recipe id 9999 starting from the 20th tip.",
                    "parameters": {
                        "id": 9999,
                        "from": 20
                    }
                },
                {
                    "utterance": "Get 25 tips for recipe id 2345.",
                    "parameters": {
                        "id": 2345,
                        "size": 25
                    }
                },
                {
                    "utterance": "I want to see tips for recipe id 6789 starting from the 3rd tip and show 7 tips.",
                    "parameters": {
                        "id": 6789,
                        "from": 3,
                        "size": 7
                    }
                },
                {
                    "utterance": "Please provide tips for recipe id 1111 with the default size and offset.",
                    "parameters": {
                        "id": 1111
                    }
                }
            ]
        }
    ]
}