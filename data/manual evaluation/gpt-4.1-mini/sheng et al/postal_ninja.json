{
    "name": "Postal Ninja",
    "description": "Official Postal Ninja tracking API for developers",
    "api_methods": [
        {
            "name": "createTrack",
            "description": "Returns ID of newly created package (status=CREATED) or stored ID if this tracking number already exists (status=FOUND).",
            "parameters": [
                {
                    "name": "trackCode",
                    "type": "STRING",
                    "description": "Tracking number",
                    "default": "",
                    "required": true
                },
                {
                    "name": "ds",
                    "type": "STRING",
                    "description": "Carrier ID (optional)",
                    "default": "",
                    "required": false
                },
                {
                    "name": "destCountry",
                    "type": "STRING",
                    "description": "Optional destination country code (ISO 3166-1 alpha-2)",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Can you create a tracking entry for the package with tracking number 1Z999AA10123456784?",
                    "parameters": {
                        "trackCode": "1Z999AA10123456784"
                    }
                },
                {
                    "utterance": "Please add tracking for the parcel with tracking code 9400110200881234567890 and specify the carrier as USPS.",
                    "parameters": {
                        "trackCode": "9400110200881234567890",
                        "ds": "USPS"
                    }
                },
                {
                    "utterance": "Create a new tracking record for the shipment with tracking number LX123456789CN destined for China.",
                    "parameters": {
                        "trackCode": "LX123456789CN",
                        "destCountry": "CN"
                    }
                },
                {
                    "utterance": "I want to track the package with tracking code 4202212345678901234567890123456789 from DHL.",
                    "parameters": {
                        "trackCode": "4202212345678901234567890123456789",
                        "ds": "DHL"
                    }
                },
                {
                    "utterance": "Add tracking for the parcel with tracking number RR123456789US going to the United States.",
                    "parameters": {
                        "trackCode": "RR123456789US",
                        "destCountry": "US"
                    }
                },
                {
                    "utterance": "Please create a tracking entry for the shipment with tracking code 1234567890.",
                    "parameters": {
                        "trackCode": "1234567890"
                    }
                },
                {
                    "utterance": "Track the package with tracking number 8Z12345E0305271688 and carrier FedEx.",
                    "parameters": {
                        "trackCode": "8Z12345E0305271688",
                        "ds": "FedEx"
                    }
                },
                {
                    "utterance": "Create tracking for the parcel with code CP123456789DE destined for Germany.",
                    "parameters": {
                        "trackCode": "CP123456789DE",
                        "destCountry": "DE"
                    }
                },
                {
                    "utterance": "Add a new tracking number 123456789ABC for the shipment.",
                    "parameters": {
                        "trackCode": "123456789ABC"
                    }
                },
                {
                    "utterance": "Please create tracking for the package with tracking code 1Z12345E1512345676 from UPS going to Canada.",
                    "parameters": {
                        "trackCode": "1Z12345E1512345676",
                        "ds": "UPS",
                        "destCountry": "CA"
                    }
                }
            ]
        }
    ]
}