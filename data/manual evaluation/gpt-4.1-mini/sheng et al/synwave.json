{
    "name": "SynWave",
    "description": "A data storage management API offering the following features:\n\n",
    "api_methods": [
        {
            "name": "Upload a new file",
            "description": "Upload a new file.",
            "parameters": [
                {
                    "name": "file",
                    "type": "BINARY",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "auth_password",
                    "type": "STRING",
                    "description": "Password to protect this file. auth_username parameter is required if auth_password is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "expiration_seconds",
                    "type": "NUMBER",
                    "description": "Number of seconds before the file becomes unavailable.\n",
                    "default": 3600,
                    "required": false
                },
                {
                    "name": "auth_username",
                    "type": "STRING",
                    "description": "Username to protect this file. auth_password parameter is required if auth_username is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "download_limit",
                    "type": "NUMBER",
                    "description": "Number of downloads before the file becomes unavailable.\n",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Please upload this document file for me.",
                    "parameters": {
                        "file": "document.pdf"
                    }
                },
                {
                    "utterance": "Upload the image file and set the expiration time to 7200 seconds.",
                    "parameters": {
                        "file": "photo.jpg",
                        "expiration_seconds": 7200
                    }
                },
                {
                    "utterance": "Can you upload this video file with a download limit of 5?",
                    "parameters": {
                        "file": "video.mp4",
                        "download_limit": 5
                    }
                },
                {
                    "utterance": "Upload the spreadsheet file and protect it with username 'user123' and password 'securePass'.",
                    "parameters": {
                        "file": "data.xlsx",
                        "auth_username": "user123",
                        "auth_password": "securePass"
                    }
                },
                {
                    "utterance": "Please upload the presentation file and set it to expire after 1800 seconds with a download limit of 10.",
                    "parameters": {
                        "file": "slides.pptx",
                        "expiration_seconds": 1800,
                        "download_limit": 10
                    }
                },
                {
                    "utterance": "Upload this audio file without any password protection but limit downloads to 3.",
                    "parameters": {
                        "file": "song.mp3",
                        "download_limit": 3
                    }
                },
                {
                    "utterance": "Upload the text file and protect it with username 'admin' and password 'admin123', and set expiration to 3600 seconds.",
                    "parameters": {
                        "file": "notes.txt",
                        "auth_username": "admin",
                        "auth_password": "admin123",
                        "expiration_seconds": 3600
                    }
                },
                {
                    "utterance": "Please upload this archive file with no expiration and no download limit.",
                    "parameters": {
                        "file": "backup.zip",
                        "expiration_seconds": 0
                    }
                },
                {
                    "utterance": "Upload the PDF file and set the download limit to 1 and expiration to 600 seconds.",
                    "parameters": {
                        "file": "ebook.pdf",
                        "download_limit": 1,
                        "expiration_seconds": 600
                    }
                },
                {
                    "utterance": "Upload this confidential file with username 'confuser' and password 'confpass', no download limit, and expiration of 86400 seconds.",
                    "parameters": {
                        "file": "confidential.docx",
                        "auth_username": "confuser",
                        "auth_password": "confpass",
                        "expiration_seconds": 86400
                    }
                }
            ]
        }
    ]
}