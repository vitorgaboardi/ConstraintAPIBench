{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "api_methods": [
        {
            "name": "Daily",
            "description": "Daily forecast.",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the daily weather forecast for New York City?",
                    "parameters": {
                        "location": "New York City"
                    }
                },
                {
                    "utterance": "Give me the daily forecast for Denver with altitude set to 1600 meters.",
                    "parameters": {
                        "location": "Denver",
                        "alt": 1600
                    }
                },
                {
                    "utterance": "Show the daily weather forecast for Paris with temperature in Fahrenheit.",
                    "parameters": {
                        "location": "Paris",
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "I want the daily forecast for Tokyo with wind speed in kilometers per hour.",
                    "parameters": {
                        "location": "Tokyo",
                        "windunit": "KMH"
                    }
                },
                {
                    "utterance": "Provide the daily weather forecast for London using the full dataset and 10 time periods.",
                    "parameters": {
                        "location": "London",
                        "dataset": "full",
                        "periods": 10
                    }
                },
                {
                    "utterance": "Get the daily forecast for Sydney with temperature in Celsius and wind speed in meters per second.",
                    "parameters": {
                        "location": "Sydney",
                        "tempunit": "C",
                        "windunit": "MS"
                    }
                },
                {
                    "utterance": "Show me the daily weather for Mexico City with altitude 2200 meters and 12 periods.",
                    "parameters": {
                        "location": "Mexico City",
                        "alt": 2200,
                        "periods": 12
                    }
                },
                {
                    "utterance": "What's the daily forecast for Cape Town with temperature in Celsius and wind speed in miles per hour?",
                    "parameters": {
                        "location": "Cape Town",
                        "tempunit": "C",
                        "windunit": "MPH"
                    }
                },
                {
                    "utterance": "Give me the daily weather forecast for Moscow with dataset set to basic and 8 periods.",
                    "parameters": {
                        "location": "Moscow",
                        "dataset": "basic",
                        "periods": 8
                    }
                },
                {
                    "utterance": "Provide the daily forecast for Rio de Janeiro with altitude 5 meters and temperature in Fahrenheit.",
                    "parameters": {
                        "location": "Rio de Janeiro",
                        "alt": 5,
                        "tempunit": "F"
                    }
                }
            ]
        }
    ]
}