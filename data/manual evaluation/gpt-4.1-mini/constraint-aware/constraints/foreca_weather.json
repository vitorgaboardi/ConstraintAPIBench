{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "api_methods": [
        {
            "name": "Daily",
            "description": "Daily forecast.",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true,
                    "constraints": {
                        "id": true
                    }
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false,
                    "constraints": {}
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false,
                    "constraints": {
                        "values": {
                            "max": 12
                        }
                    }
                }
            ]
        }
    ]
}