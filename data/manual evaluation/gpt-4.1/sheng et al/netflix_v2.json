{
    "name": "Netflix_v2",
    "description": "Netflix data API provides details, stats and information of TV shows, movies, series, documentaries and more.",
    "url": "https://rapidapi.com/Glavier/api/netflix54/",
    "api_methods": [
        {
            "name": "Search",
            "description": "Search",
            "url": "https://netflix54.p.rapidapi.com/search/",
            "parameters": [
                {
                    "name": "query",
                    "type": "STRING",
                    "description": "Search Query",
                    "default": "stranger",
                    "required": true
                },
                {
                    "name": "lang",
                    "type": "STRING",
                    "description": "",
                    "default": "en",
                    "required": false
                },
                {
                    "name": "limit_titles",
                    "type": "NUMBER",
                    "description": "Title Limit",
                    "default": "50",
                    "required": false
                },
                {
                    "name": "limit_suggestions",
                    "type": "NUMBER",
                    "description": "Suggestion Limit",
                    "default": "20",
                    "required": false
                },
                {
                    "name": "offset",
                    "type": "NUMBER",
                    "description": "Offset",
                    "default": "0",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Find information about the show Stranger Things.",
                    "parameters": {
                        "query": "Stranger Things"
                    }
                },
                {
                    "utterance": "Search for movies related to The Witcher in Spanish.",
                    "parameters": {
                        "query": "The Witcher",
                        "lang": "es"
                    }
                },
                {
                    "utterance": "Show me 5 titles for the query Money Heist.",
                    "parameters": {
                        "query": "Money Heist",
                        "limit_titles": 5
                    }
                },
                {
                    "utterance": "Give me 10 suggestions for the show Bridgerton.",
                    "parameters": {
                        "query": "Bridgerton",
                        "limit_suggestions": 10
                    }
                },
                {
                    "utterance": "Search for documentaries about nature and show results in French.",
                    "parameters": {
                        "query": "nature documentaries",
                        "lang": "fr"
                    }
                },
                {
                    "utterance": "Find the first 3 results for the movie Roma.",
                    "parameters": {
                        "query": "Roma",
                        "limit_titles": 3
                    }
                },
                {
                    "utterance": "Search for comedy specials and give me 15 suggestions.",
                    "parameters": {
                        "query": "comedy specials",
                        "limit_suggestions": 15
                    }
                },
                {
                    "utterance": "Look up Lupin and show me the results starting from the 5th entry.",
                    "parameters": {
                        "query": "Lupin",
                        "offset": 5
                    }
                },
                {
                    "utterance": "Find Korean dramas and show the first 8 titles.",
                    "parameters": {
                        "query": "Korean dramas",
                        "limit_titles": 8
                    }
                },
                {
                    "utterance": "Search for the movie Enola Holmes in German and show 2 suggestions.",
                    "parameters": {
                        "query": "Enola Holmes",
                        "lang": "de",
                        "limit_suggestions": 2
                    }
                }
            ]
        }
    ]
}