{
    "name": "Foreca Weather",
    "description": "Everything you need to build a weather app",
    "url": "https://rapidapi.com/foreca-ltd-foreca-ltd-default/api/foreca-weather/",
    "api_methods": [
        {
            "name": "Daily",
            "description": "Daily forecast.",
            "url": "https://foreca-weather.p.rapidapi.com/forecast/daily/{location}",
            "parameters": [
                {
                    "name": "location",
                    "type": "string",
                    "description": "",
                    "default": "102643743",
                    "required": true
                },
                {
                    "name": "alt",
                    "type": "NUMBER",
                    "description": "Altitude (meters)",
                    "default": "0",
                    "required": false
                },
                {
                    "name": "dataset",
                    "type": "STRING",
                    "description": "Variable set",
                    "default": "full",
                    "required": false
                },
                {
                    "name": "tempunit",
                    "type": "STRING",
                    "description": "Temperature unit in response",
                    "default": "C",
                    "required": false
                },
                {
                    "name": "windunit",
                    "type": "STRING",
                    "description": "Wind speed unit in response.",
                    "default": "MS",
                    "required": false
                },
                {
                    "name": "periods",
                    "type": "NUMBER",
                    "description": "Number of time steps (default 8, maximum 12)",
                    "default": "8",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "What's the daily weather forecast for New York City?",
                    "parameters": {
                        "location": "New York City"
                    }
                },
                {
                    "utterance": "Give me the 5-day weather outlook for Paris in Fahrenheit.",
                    "parameters": {
                        "location": "Paris",
                        "periods": 5,
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "Show me the daily forecast for Tokyo at an altitude of 500 meters.",
                    "parameters": {
                        "location": "Tokyo",
                        "alt": 500
                    }
                },
                {
                    "utterance": "What's the weather for the next 3 days in Cape Town using the basic dataset?",
                    "parameters": {
                        "location": "Cape Town",
                        "periods": 3,
                        "dataset": "basic"
                    }
                },
                {
                    "utterance": "Can I get the daily forecast for Sydney with wind speed in kilometers per hour?",
                    "parameters": {
                        "location": "Sydney",
                        "windunit": "KMH"
                    }
                },
                {
                    "utterance": "Give me the 7-day weather forecast for Berlin in Celsius.",
                    "parameters": {
                        "location": "Berlin",
                        "periods": 7,
                        "tempunit": "C"
                    }
                },
                {
                    "utterance": "What's the daily weather for Rio de Janeiro at sea level using the full dataset?",
                    "parameters": {
                        "location": "Rio de Janeiro",
                        "alt": 0,
                        "dataset": "full"
                    }
                },
                {
                    "utterance": "Show me the 10-day forecast for Toronto with wind speed in miles per hour.",
                    "parameters": {
                        "location": "Toronto",
                        "periods": 10,
                        "windunit": "MPH"
                    }
                },
                {
                    "utterance": "What's the daily forecast for Dubai at 300 meters altitude in Fahrenheit?",
                    "parameters": {
                        "location": "Dubai",
                        "alt": 300,
                        "tempunit": "F"
                    }
                },
                {
                    "utterance": "Give me the 12-day weather forecast for Mumbai using the full dataset and wind speed in knots.",
                    "parameters": {
                        "location": "Mumbai",
                        "periods": 12,
                        "dataset": "full",
                        "windunit": "KT"
                    }
                }
            ]
        }
    ]
}