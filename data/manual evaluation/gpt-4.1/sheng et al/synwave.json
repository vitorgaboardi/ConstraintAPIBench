{
    "name": "SynWave",
    "description": "A data storage management API offering the following features:\n\n",
    "url": "https://rapidapi.com/rpi4gx/api/synwave/",
    "api_methods": [
        {
            "name": "Upload a new file",
            "description": "Upload a new file.",
            "url": "https://synwave.p.rapidapi.com/v1/upload",
            "parameters": [
                {
                    "name": "file",
                    "type": "BINARY",
                    "description": "",
                    "default": "",
                    "required": true
                },
                {
                    "name": "auth_password",
                    "type": "STRING",
                    "description": "Password to protect this file. auth_username parameter is required if auth_password is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "expiration_seconds",
                    "type": "NUMBER",
                    "description": "Number of seconds before the file becomes unavailable.\n",
                    "default": 3600,
                    "required": false
                },
                {
                    "name": "auth_username",
                    "type": "STRING",
                    "description": "Username to protect this file. auth_password parameter is required if auth_username is set.\n",
                    "default": "",
                    "required": false
                },
                {
                    "name": "download_limit",
                    "type": "NUMBER",
                    "description": "Number of downloads before the file becomes unavailable.\n",
                    "default": "",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Upload this PDF file.",
                    "parameters": {
                        "file": "PDF file"
                    }
                },
                {
                    "utterance": "Please upload the image and set it to expire in 600 seconds.",
                    "parameters": {
                        "file": "image",
                        "expiration_seconds": 600
                    }
                },
                {
                    "utterance": "Upload the document and protect it with username 'alice' and password 'secure123'.",
                    "parameters": {
                        "file": "document",
                        "auth_username": "alice",
                        "auth_password": "secure123"
                    }
                },
                {
                    "utterance": "Can you upload this video file and allow it to be downloaded only 3 times?",
                    "parameters": {
                        "file": "video file",
                        "download_limit": 3
                    }
                },
                {
                    "utterance": "Upload this spreadsheet, set the username to 'bob' and password to 'letmein', and make it expire in 2 hours.",
                    "parameters": {
                        "file": "spreadsheet",
                        "auth_username": "bob",
                        "auth_password": "letmein",
                        "expiration_seconds": 7200
                    }
                },
                {
                    "utterance": "Upload this audio file with a download limit of 1 and expiration of 30 minutes.",
                    "parameters": {
                        "file": "audio file",
                        "download_limit": 1,
                        "expiration_seconds": 1800
                    }
                },
                {
                    "utterance": "Please upload the zip archive and protect it with password 'archivepass'. Username should be 'carol'.",
                    "parameters": {
                        "file": "zip archive",
                        "auth_username": "carol",
                        "auth_password": "archivepass"
                    }
                },
                {
                    "utterance": "Upload this presentation file and set it to expire after 24 hours.",
                    "parameters": {
                        "file": "presentation file",
                        "expiration_seconds": 86400
                    }
                },
                {
                    "utterance": "Upload the CSV file, allow only 5 downloads, and set username to 'dave' with password 'csvpass'.",
                    "parameters": {
                        "file": "CSV file",
                        "download_limit": 5,
                        "auth_username": "dave",
                        "auth_password": "csvpass"
                    }
                },
                {
                    "utterance": "Upload this text file and make it available for 10 downloads or 1 hour, whichever comes first.",
                    "parameters": {
                        "file": "text file",
                        "download_limit": 10,
                        "expiration_seconds": 3600
                    }
                }
            ]
        }
    ]
}