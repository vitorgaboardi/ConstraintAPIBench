{
    "name": "Tasty",
    "description": "API to query data about recipe, plan, ingredients, etc... as on official site",
    "url": "https://rapidapi.com/apidojo/api/tasty/",
    "api_methods": [
        {
            "name": "tips/list",
            "description": "This endpoint is used to load tips (reviews)",
            "url": "https://tasty.p.rapidapi.com/tips/list",
            "parameters": [
                {
                    "name": "id",
                    "type": "NUMBER",
                    "description": "The value of recipe id returned in .../recipes/list .../feeds/list .../recipes/list-similarities endpoints",
                    "default": "3562",
                    "required": true
                },
                {
                    "name": "size",
                    "type": "NUMBER",
                    "description": "Number of items returned per response",
                    "default": "30",
                    "required": false
                },
                {
                    "name": "from",
                    "type": "NUMBER",
                    "description": "The offset of items to be ignored in response for paging",
                    "default": "0",
                    "required": false
                }
            ],
            "utterances": [
                {
                    "utterance": "Show me the tips for recipe 3562.",
                    "parameters": {
                        "id": 3562
                    }
                },
                {
                    "utterance": "What are the reviews for recipe 1024?",
                    "parameters": {
                        "id": 1024
                    }
                },
                {
                    "utterance": "Give me 10 tips for recipe 5000.",
                    "parameters": {
                        "id": 5000,
                        "size": 10
                    }
                },
                {
                    "utterance": "List the first 5 reviews for recipe 789.",
                    "parameters": {
                        "id": 789,
                        "size": 5
                    }
                },
                {
                    "utterance": "Show tips for recipe 2500, starting from the 11th tip.",
                    "parameters": {
                        "id": 2500,
                        "from": 10
                    }
                },
                {
                    "utterance": "Can I see 20 tips for recipe 3562, skipping the first 5?",
                    "parameters": {
                        "id": 3562,
                        "size": 20,
                        "from": 5
                    }
                },
                {
                    "utterance": "Display all available tips for recipe 4001.",
                    "parameters": {
                        "id": 4001
                    }
                },
                {
                    "utterance": "What are the latest 15 reviews for recipe 1234?",
                    "parameters": {
                        "id": 1234,
                        "size": 15
                    }
                },
                {
                    "utterance": "Show me tips for recipe 5678, starting from the 21st tip and show 10 tips.",
                    "parameters": {
                        "id": 5678,
                        "from": 20,
                        "size": 10
                    }
                },
                {
                    "utterance": "Get the first tip for recipe 8888.",
                    "parameters": {
                        "id": 8888,
                        "size": 1
                    }
                }
            ]
        }
    ]
}