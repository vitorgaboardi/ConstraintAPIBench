{
    "name": "CAISO",
    "description": "API for California Independent System Operator data provided from caiso.com. It includes data on energy emissions, demand, supply, and prices. Updated daily.",
    "url": "https://rapidapi.com/buildingviz-buildingviz-default/api/caiso/",
    "api_methods": [
        {
            "name": "caiso",
            "description": "Gets standard CAISO data",
            "url": "https://caiso.p.rapidapi.com/caiso",
            "parameters": [
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "Type needs to be one of \\\\\\\\\\\\\\\"co2\\\\\\\\\\\\\\\", \\\\\\\\\\\\\\\"fuelsource\\\\\\\\\\\\\\\", \\\\\\\\\\\\\\\"netdemand\\\\\\\\\\\\\\\", or \\\\\\\\\\\\\\\"all\\\\\\\\\\\\\\\". The \\\\\\\\\\\\\\\"all\\\\\\\\\\\\\\\" value will combine the results for the three other tables into a single CSV file with all the columns you requested.",
                    "default": "co2",
                    "required": true,
                    "constraints": {
                        "values": {
                            "enumerated": [
                                "co2",
                                "fuelsource",
                                "netdemand",
                                "all"
                            ]
                        }
                    }
                },
                {
                    "name": "enddate",
                    "type": "STRING",
                    "description": "",
                    "default": "20220901",
                    "required": false,
                    "constraints": {
                        "format": "^\\d{4}\\d{2}\\d{2}$"
                    }
                },
                {
                    "name": "interval",
                    "type": "STRING",
                    "description": "",
                    "default": "30",
                    "required": false
                },
                {
                    "name": "startdate",
                    "type": "STRING",
                    "description": "",
                    "default": "20220101",
                    "required": false,
                    "constraints": {
                        "format": "^\\d{4}\\d{2}\\d{2}$"
                    }                }
            ]
        },
        {
            "name": "caisonode",
            "description": "Returns nodes where prices are tracked by CAISO",
            "url": "https://caiso.p.rapidapi.com/caisonode",
            "parameters": [
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "",
                    "default": "state",
                    "required": true
                },
                {
                    "name": "long",
                    "type": "NUMBER",
                    "description": "",
                    "default": "-114.1056",
                    "required": false
                },
                {
                    "name": "radius",
                    "type": "NUMBER",
                    "description": "",
                    "default": "100",
                    "required": false
                },
                {
                    "name": "region",
                    "type": "STRING",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "state",
                    "type": "STRING",
                    "description": "",
                    "default": "wa",
                    "required": false
                },
                {
                    "name": "name",
                    "type": "STRING",
                    "description": "",
                    "default": "",
                    "required": false
                },
                {
                    "name": "lat",
                    "type": "NUMBER",
                    "description": "",
                    "default": "34.6106",
                    "required": false
                }
            ]
        },
        {
            "name": "caisoregion",
            "description": "Gets Regional Authorities where node prices are tracked by CAISO",
            "url": "https://caiso.p.rapidapi.com/caisoregion",
            "parameters": []
        },
        {
            "name": "caisoprice",
            "description": "Fetches hourly CAISO price data",
            "url": "https://caiso.p.rapidapi.com/caisoprice",
            "parameters": [
                {
                    "name": "type",
                    "type": "STRING",
                    "description": "",
                    "default": "date",
                    "required": true
                },
                {
                    "name": "page",
                    "type": "NUMBER",
                    "description": "",
                    "default": "12",
                    "required": false
                },
                {
                    "name": "date",
                    "type": "STRING",
                    "description": "",
                    "default": "20220601",
                    "required": false
                },
                {
                    "name": "format",
                    "type": "STRING",
                    "description": "",
                    "default": "jsonpage",
                    "required": false
                },
                {
                    "name": "node",
                    "type": "STRING",
                    "description": "",
                    "default": "",
                    "required": false
                }
            ]
        }
    ]
}